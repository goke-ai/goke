    "name": "Jobu",
    "commonName": "Jobu",
    "title": "Ìwé Jobu",
    "order": 18,
    "numberOfChapters": 42,
    "sha256": "280cccb42ea4fc425914658127e6f2d3a50b65b86cfbb87a2bea57218af3b847",
    "firstChapterApiLink": "/api/yor_ycb/JOB/1.json",
