    "name": "Orin Solomoni",
    "commonName": "Orin Solomoni",
    "title": "Ìwé Orin Solomoni",
    "order": 22,
    "numberOfChapters": 8,
    "sha256": "b681d3863dccaf57133fc817b549c8699681fd51f2c1857319c8671f3ccb7e55",
