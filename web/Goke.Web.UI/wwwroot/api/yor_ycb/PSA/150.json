  "chapter": {
    "number": 150,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ẹ fi ìyìn fún Olúwa.",
            "poem": 1
          },
          {
            "lineBreak": true
          },
          {
            "text": "Ẹ fi ìyìn fún Ọlọ́run ní ibi mímọ́ rẹ̀,",
            "poem": 1
          },
          {
            "text": "ẹ yìn ín nínú agbára ọ̀run rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ẹ yìn ín fún iṣẹ́ agbára rẹ̀.",
            "poem": 1
          },
          {
            "text": "Ẹ yìn ín fún títóbi rẹ̀ tí ó tayọ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ẹ fi ohùn ìpè yìn ín.",
            "poem": 1
          },
          {
            "text": "Ẹ fi ohun èlò orin yìn ín.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ẹ fi ohun èlò orin àti ijó yìn ín",
            "poem": 1
          },
          {
            "text": "fi ohun èlò orin olókùn àti ìpè yìn ín,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ẹ yìn ín pẹ̀lú aro olóhùn òkè,",
            "poem": 1
          },
          {
            "text": "ẹ yìn ín lára aro olóhùn gooro.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Jẹ́ kí gbogbo ohun tí ó ní ẹ̀mí yin Olúwa.",
            "poem": 1
          },
          {
            "lineBreak": true
          },
          {
            "text": "Ẹ fi ìyìn fún Olúwa.",
            "poem": 1
          }
        ]
      }
    ],
