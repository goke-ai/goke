  "chapter": {
    "number": 123,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Orin fún ìgòkè."
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ìwọ ní mo gbé ojú mí sókè sí,",
            "poem": 1
          },
          {
            "text": "ìwọ tí ń gbé inú ọ̀run.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Kíyèsi, bí ojú àwọn",
            "poem": 1
          },
          {
            "text": "ìránṣẹ́kùnrin ti máa ń wo ọwọ́ àwọn baba wọn,",
            "poem": 2
          },
          {
            "text": "àti bí ojú ìránṣẹ́bìnrin ti máa ń wo ọwọ́ ìyá rẹ̀",
            "poem": 2
          },
          {
            "text": "bẹ́ẹ̀ ni ojú wa ń wo Olúwa Ọlọ́run wa,",
            "poem": 1
          },
          {
            "text": "títí yóò fi ṣàánú fún wa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Olúwa, ṣàánú fún wa, ṣàánú fún wa;",
            "poem": 1
          },
          {
            "text": "nítorí tí a kún fún ẹ̀gàn púpọ̀púpọ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ọkàn wa kún púpọ̀",
            "poem": 1
          },
          {
            "text": "fún ẹ̀gàn àwọn onírera,",
            "poem": 2
          },
          {
            "text": "àti fún ẹ̀gàn àwọn agbéraga.",
            "poem": 2
          }
        ]
      }
    ],
