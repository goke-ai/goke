  "chapter": {
    "number": 78,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Maskili ti Asafu."
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ẹ̀yin ènìyàn mi, ẹ gbọ́ ẹ̀kọ́ mi;",
            "poem": 1
          },
          {
            "text": "tẹ́tí rẹ sí ọ̀rọ̀ ẹnu mi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Èmi ó la ẹnu mi ní òwe,",
            "poem": 1
          },
          {
            "text": "èmi o sọ ohun ìkọ̀kọ̀, ohun ti ọjọ́ pípẹ́;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "ohun tí a ti gbọ́ tí a sì ti mọ̀,",
            "poem": 1
          },
          {
            "text": "ohun tí àwọn baba wa ti sọ fún wa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Àwa kì yóò pa wọ́n mọ́",
            "poem": 1
          },
          {
            "text": "kúrò lọ́dọ̀ àwọn ọmọ,",
            "poem": 2
          },
          {
            "text": "ní fífi ìyìn Olúwa, àti ipa rẹ̀",
            "poem": 1
          },
          {
            "text": "àti iṣẹ́ ìyanu tó ti ṣe hàn",
            "poem": 2
          },
          {
            "text": "fún ìran tí ń bọ̀.",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ó gbé ẹ̀rí kalẹ̀ ní Jakọbu",
            "poem": 1
          },
          {
            "text": "o sì fìdí àṣẹ múlẹ̀ ní Israẹli,",
            "poem": 2
          },
          {
            "text": "èyí tí ó pàṣẹ fún àwọn baba ńlá wa",
            "poem": 1
          },
          {
            "text": "láti kọ́ àwọn ọmọ wọn,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "nítorí náà, àwọn ìran tí ń bọ̀ yóò mọ̀ wọ́n",
            "poem": 1
          },
          {
            "text": "bẹ́ẹ̀ ni àwọn ọmọ tí a kò tí ì bí",
            "poem": 2
          },
          {
            "text": "tí yóò dìde tí wọn yóò sọ fún àwọn ọmọ wọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Nígbà náà ni wọ́n ò fi ìgbẹ́kẹ̀lé wọn sínú Ọlọ́run",
            "poem": 1
          },
          {
            "text": "wọn kò sì ní gbàgbé iṣẹ́ Ọlọ́run",
            "poem": 2
          },
          {
            "text": "ṣùgbọ́n wọn ó pa àṣẹ rẹ̀ mọ́.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Kí wọn kí ó má ṣe dàbí àwọn baba ńlá wọn,",
            "poem": 1
          },
          {
            "text": "ìran alágídí àti ọlọ́tẹ̀,",
            "poem": 2
          },
          {
            "text": "ìran tí ọkàn wọn kò ṣọ òtítọ́ si olóore,",
            "poem": 2
          },
          {
            "text": "àti ẹ̀mí ẹni tí kò bá Ọlọ́run dúró ṣinṣin.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Àwọn ọkùnrin Efraimu, tí ó há mọ́ ọ tí wọ́n mú ọrun,",
            "poem": 1
          },
          {
            "text": "wọ́n yípadà ní ọjọ́ ogun",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Wọn kò pa májẹ̀mú Ọlọ́run mọ́",
            "poem": 1
          },
          {
            "text": "wọn sì kọ̀ láti máa gbé nínú òfin rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Wọ́n gbàgbé ohun tí ó ti ṣe,",
            "poem": 1
          },
          {
            "text": "àwọn ìyanu tí ó ti fihàn wọ́n.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ó ṣe ìyanu níwájú àwọn baba wọn ní ilẹ̀ Ejibiti, ní agbègbè Ṣoani.",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Ó pín òkun ní yà, ó sì mú wọn kọjá",
            "poem": 1
          },
          {
            "text": "Ó mù kí omi náà dúró bi odi gíga.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Ní ọ̀sán, ó fi ìkùùkuu àwọsánmọ̀ ṣe amọ̀nà wọn",
            "poem": 1
          },
          {
            "text": "àti ní gbogbo òru pẹ̀lú ìmọ́lẹ̀ iná.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Ó sán àpáta ní aginjù",
            "poem": 1
          },
          {
            "text": "ó sì fún wọn ní omi mímu lọ́pọ̀lọ́pọ̀",
            "poem": 2
          },
          {
            "text": "bí ẹni pé láti inú ibú wá.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Ó mú ìṣàn omi jáde láti inú àpáta",
            "poem": 1
          },
          {
            "text": "omi ṣíṣàn sílẹ̀ gẹ́gẹ́ bí odò.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Wọ́n sì tún tẹ̀síwájú láti dẹ́ṣẹ̀ sí i",
            "poem": 1
          },
          {
            "text": "ní ìṣọ̀tẹ̀ sí Ọ̀gá-ògo ní aginjù.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Wọ́n mọ̀ ọ́n mọ̀ dán Ọlọ́run wò",
            "poem": 1
          },
          {
            "text": "nípa bíbéèrè fún oúnjẹ tí wọn bẹ̀bẹ̀ fún.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Wọ́n sọ̀rọ̀-òdì sí Ọlọ́run, wí pé",
            "poem": 1
          },
          {
            "text": "“Ọlọ́run ha lè tẹ́ tábìlì ní aginjù?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Nígbà tí ó lu àpáta, omi tú jáde,",
            "poem": 1
          },
          {
            "text": "odò sì sàn lọ́pọ̀lọ́pọ̀",
            "poem": 2
          },
          {
            "text": "ṣùgbọ́n òun ha lè fún wa lóúnjẹ",
            "poem": 1
          },
          {
            "text": "ó ha le pèsè ẹran fún àwọn ènìyàn rẹ̀?”",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Nígbà tí Olúwa gbọ́ wọn, ó bínú gidigidi;",
            "poem": 1
          },
          {
            "text": "iná rẹ̀ sọ̀kalẹ̀ sí Jakọbu,",
            "poem": 1
          },
          {
            "text": "ìbínú rẹ̀ sì rú sí Israẹli,",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "nítorí wọn kò gba Ọlọ́run gbọ́,",
            "poem": 1
          },
          {
            "text": "wọn kò sì gbẹ́kẹ̀lé ìgbàlà rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 23,
        "content": [
          {
            "text": "Síbẹ̀ ó pàṣẹ fún ìràwọ̀ ojú ọ̀run,",
            "poem": 1
          },
          {
            "text": "ó sì ṣí ìlẹ̀kùn ọ̀run sílẹ̀;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 24,
        "content": [
          {
            "text": "Ó rọ manna fún àwọn ènìyàn láti jẹ,",
            "poem": 1
          },
          {
            "text": "ó fún wọn ní ọkà láti ọ̀run.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 25,
        "content": [
          {
            "text": "Àwọn ènìyàn jẹ oúnjẹ àwọn angẹli;",
            "poem": 1
          },
          {
            "text": "Ó fún wọn ní oúnjẹ ní àjẹyó,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 26,
        "content": [
          {
            "text": "Ó mú afẹ́fẹ́ ìlà-oòrùn láti ọ̀run wá",
            "poem": 1
          },
          {
            "text": "ó mú afẹ́fẹ́ gúúsù wá nípa agbára rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 27,
        "content": [
          {
            "text": "Ó rọ òjò ẹran sílẹ̀ sórí wọn bí erùpẹ̀ ilẹ̀,",
            "poem": 1
          },
          {
            "text": "àti ẹyẹ abìyẹ́ gẹ́gẹ́ bí iyanrìn etí òkun",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 28,
        "content": [
          {
            "text": "Ó jẹ́ kí wọn jáde ní ibùdó wọn,",
            "poem": 1
          },
          {
            "text": "yíká àgọ́ wọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 29,
        "content": [
          {
            "text": "Wọ́n jẹ, wọ́n sì yó jọjọ",
            "poem": 1
          },
          {
            "text": "nítorí ó ti fún wọn ní ohun tí wọn fìtara bẹ̀bẹ̀ fún.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 30,
        "content": [
          {
            "text": "Ṣùgbọ́n wọn kò kúrò nínú oúnjẹ tí wọn fìtara bẹ̀bẹ̀ fún,",
            "poem": 1
          },
          {
            "text": "nígbà tí oúnjẹ wọn sì wà ní ẹnu wọn,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 31,
        "content": [
          {
            "text": "ìbínú Ọlọ́run dìde sí wọn",
            "poem": 1
          },
          {
            "text": "ó pa àwọn tí ó jùlọ nínú wọn,",
            "poem": 2
          },
          {
            "text": "ó sì lu àwọn ọ̀dọ́mọkùnrin Israẹli bolẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 32,
        "content": [
          {
            "text": "Nínú gbogbo ìwọ̀nyí, wọ́n ń dẹ́ṣẹ̀ síwájú;",
            "poem": 1
          },
          {
            "text": "nínú gbogbo iṣẹ́ ìyanu rẹ̀, wọ́n kò gbàgbọ́.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 33,
        "content": [
          {
            "text": "O fi òpin sí ayé wọn nínú asán",
            "poem": 1
          },
          {
            "text": "àti ọdún wọn nínú ìpayà.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 34,
        "content": [
          {
            "text": "Nígbàkígbà tí Ọlọ́run bá pa wọ́n,",
            "poem": 1
          },
          {
            "text": "wọn yóò wá a kiri;",
            "poem": 2
          },
          {
            "text": "wọn yóò fi ìtara yípadà sí i.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 35,
        "content": [
          {
            "text": "Wọ́n rántí pé Ọlọ́run ní àpáta wọn;",
            "poem": 1
          },
          {
            "text": "wí pé Ọlọ́run Ọ̀gá-ògo jùlọ ni olùràpadà àpáta wọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 36,
        "content": [
          {
            "text": "Ṣùgbọ́n nígbà náà ni wọ́n yóò pọ́n ọn pẹ̀lú ẹnu wọ́n",
            "poem": 1
          },
          {
            "text": "wọ́n fi ahọ́n wọ́n purọ́ fún un;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 37,
        "content": [
          {
            "text": "ọkàn wọn kò sòtítọ́ sí i,",
            "poem": 1
          },
          {
            "text": "wọn kò jẹ́ olódodo sí májẹ̀mú rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 38,
        "content": [
          {
            "text": "Síbẹ̀ ó ṣàánú;",
            "poem": 1
          },
          {
            "text": "ó dárí àìṣedéédéé wọn jì",
            "poem": 2
          },
          {
            "text": "òun kò sì pa wọ́n run",
            "poem": 1
          },
          {
            "text": "nígbà púpọ̀ ló ń yí ìbínú rẹ̀ padà",
            "poem": 2
          },
          {
            "text": "kò sì ru ìbínú rẹ̀ sókè",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 39,
        "content": [
          {
            "text": "Ó rántí pé ẹlẹ́ran ara ni wọ́n,",
            "poem": 1
          },
          {
            "text": "afẹ́fẹ́ tó ń kọjá tí kò lè padà.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 40,
        "content": [
          {
            "text": "Nígbà gbogbo ní wọn ń ṣọ̀tẹ̀ sí i ní aginjù",
            "poem": 1
          },
          {
            "text": "wọn mú un bínú nínú ilẹ̀ tí ó di ahoro!",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 41,
        "content": [
          {
            "text": "Síwájú àti síwájú wọ́n dán Ọlọ́run wò;",
            "poem": 1
          },
          {
            "text": "wọ́n mú ẹni mímọ́ Israẹli bínú.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 42,
        "content": [
          {
            "text": "Wọ́n kò rántí agbára rẹ̀:",
            "poem": 1
          },
          {
            "text": "ní ọjọ́ tí ó rà wọ́n padà lọ́wọ́ àwọn aninilára,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 43,
        "content": [
          {
            "text": "ní ọjọ́ tí ó fi iṣẹ́ ìyanu rẹ̀ hàn ní Ejibiti,",
            "poem": 1
          },
          {
            "text": "àti iṣẹ́ àmì rẹ̀ ni ẹkùn Ṣoani",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 44,
        "content": [
          {
            "text": "Ó sọ omi wọn di ẹ̀jẹ̀;",
            "poem": 1
          },
          {
            "text": "wọn kò lè mu láti odò wọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 45,
        "content": [
          {
            "text": "Ó rán ọ̀wọ́ eṣinṣin láti pa wọ́n run,",
            "poem": 1
          },
          {
            "text": "àti ọ̀pọ̀lọ́ tí ó bá wọn jẹun.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 46,
        "content": [
          {
            "text": "Ó fi ọkà wọn fún láńtata,",
            "poem": 1
          },
          {
            "text": "àwọn ìre oko wọn fún eṣú.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 47,
        "content": [
          {
            "text": "Ó fi yìnyín ba àjàrà wọn jẹ́",
            "poem": 1
          },
          {
            "text": "ó bá èso sikamore wọn jẹ́.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 48,
        "content": [
          {
            "text": "Ó fi ohun ọ̀sìn wọn fún yìnyín,",
            "poem": 1
          },
          {
            "text": "agbo ẹran wọn fún mọ̀nàmọ́ná.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 49,
        "content": [
          {
            "text": "Ó mú kíkorò ìbínú rẹ̀ wá sí wọn lára,",
            "poem": 1
          },
          {
            "text": "ìrunú àti ìkáàánú, àti ìpọ́njú,",
            "poem": 2
          },
          {
            "text": "nípa rírán angẹli apanirun sí wọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 50,
        "content": [
          {
            "text": "Ó pèsè ipa fún ìbínú rẹ̀,",
            "poem": 1
          },
          {
            "text": "òun kò gbà wọ́n lọ́wọ́ ikú,",
            "poem": 2
          },
          {
            "text": "ṣùgbọ́n ó fi ẹ̀mí wọ́n fún àjàkálẹ̀-ààrùn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 51,
        "content": [
          {
            "text": "Ó kọlu àwọn àkọ́bí ọmọ ọkùnrin Ejibiti,",
            "poem": 1
          },
          {
            "text": "olórí agbára wọn nínú àgọ́ Hamu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 52,
        "content": [
          {
            "text": "Ṣùgbọ́n ó mú àwọn ènìyàn rẹ̀ jáde bí ọ̀wọ́ ẹran;",
            "poem": 1
          },
          {
            "text": "ó ṣọ́ wọn gẹ́gẹ́ bí àgùntàn nínú aginjù.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 53,
        "content": [
          {
            "text": "Ó dáàbò bò wọ́n dáradára, nítorí náà ni ẹ̀rù kò ṣe bà wọ́n",
            "poem": 1
          },
          {
            "text": "ṣùgbọ́n òkun padé mọ́ àwọn ọ̀tá wọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 54,
        "content": [
          {
            "text": "Bákan náà ni ó mú wọn wá sí etí ilẹ̀ mímọ́ rẹ̀",
            "poem": 1
          },
          {
            "text": "òkè tí ọwọ́ ọ̀tún rẹ̀ ti gbà",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 55,
        "content": [
          {
            "text": "Ó lé kèfèrí jáde níwájú wọn",
            "poem": 1
          },
          {
            "text": "ó sì fi títa okùn pín ilẹ̀ náà fún wọn gẹ́gẹ́ bí ilẹ̀ ìní;",
            "poem": 2
          },
          {
            "text": "ó mú àwọn ẹ̀yà Israẹli jókòó ní ilẹ̀ wọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 56,
        "content": [
          {
            "text": "Ṣùgbọ́n wọn dán Ọlọ́run wò",
            "poem": 1
          },
          {
            "text": "wọn sì ṣọ̀tẹ̀ sí Ọ̀gá-ògo;",
            "poem": 2
          },
          {
            "text": "wọn kò pa àṣẹ rẹ̀ mọ́.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 57,
        "content": [
          {
            "text": "Ṣùgbọ́n wọ́n yípadà,",
            "poem": 1
          },
          {
            "text": "wọn jẹ́ aláìṣòdodo gẹ́gẹ́ bí àwọn baba wọn",
            "poem": 2
          },
          {
            "text": "wọ́n sì pẹ̀yìndà sí apákan bí ọrun ẹ̀tàn.",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 58,
        "content": [
          {
            "text": "Wọ́n bí i nínú pẹ̀lú ibi gíga wọn;",
            "poem": 1
          },
          {
            "text": "wọn rú owú rẹ̀ sókè nípa òrìṣà wọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 59,
        "content": [
          {
            "text": "Nígbà tí Ọlọ́run gbọ́ wọn,",
            "poem": 1
          },
          {
            "text": "inú bí i gidigidi;",
            "poem": 2
          },
          {
            "text": "ó kọ Israẹli pátápátá.",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 60,
        "content": [
          {
            "text": "Ó kọ àgọ́ Ṣilo sílẹ̀,",
            "poem": 1
          },
          {
            "text": "àgọ́ tí ó ti gbé kalẹ̀ láàrín àwọn ènìyàn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 61,
        "content": [
          {
            "text": "Ó rán àpótí ẹ̀rí, agbára rẹ̀ lọ sí ìgbèkùn,",
            "poem": 1
          },
          {
            "text": "dídán ògo rẹ̀ lọ sí ọwọ́ àwọn ọ̀tá.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 62,
        "content": [
          {
            "text": "Ó fi àwọn ènìyàn rẹ̀ lé idà lọ́wọ́,",
            "poem": 1
          },
          {
            "text": "ó sì bínú sí àwọn ohun ìní rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 63,
        "content": [
          {
            "text": "Iná run àwọn ọ̀dọ́mọkùnrin wọn,",
            "poem": 1
          },
          {
            "text": "àwọn ọmọbìnrin wọn kò sì ní orin ìgbéyàwó:",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 64,
        "content": [
          {
            "text": "àfi àlùfáà wọn fún idà,",
            "poem": 1
          },
          {
            "text": "àwọn opó wọn kò sì le è sọkún.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 65,
        "content": [
          {
            "text": "Olúwa sì jí gẹ́gẹ́ bí ẹni ojú oorun,",
            "poem": 1
          },
          {
            "text": "gẹ́gẹ́ bí ọkùnrin ti i jí kúrò nínú ìraníyè ọtí.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 66,
        "content": [
          {
            "text": "Ó kọlu àwọn ọ̀tá rẹ̀ padà;",
            "poem": 1
          },
          {
            "text": "ó fi wọn sínú ìtìjú ayérayé.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 67,
        "content": [
          {
            "text": "Nígbà náà ni ó kọ́ àgọ́ Josẹfu,",
            "poem": 1
          },
          {
            "text": "kò sì yan ẹ̀yà Efraimu,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 68,
        "content": [
          {
            "text": "ṣùgbọ́n ó yan ẹ̀yà Juda,",
            "poem": 1
          },
          {
            "text": "òkè Sioni, èyí tí ó fẹ́ràn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 69,
        "content": [
          {
            "text": "Ó kọ́ ibi mímọ́ rẹ̀, ibi gíga,",
            "poem": 1
          },
          {
            "text": "gẹ́gẹ́ bí ayé tí ó fi ìdí rẹ̀ múlẹ̀ títí láé.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 70,
        "content": [
          {
            "text": "Ó yan Dafidi ìránṣẹ́ rẹ̀",
            "poem": 1
          },
          {
            "text": "ó mú láti inú àwọn agbo ẹran;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 71,
        "content": [
          {
            "text": "Ó mú un jáde nínú ìtọ́jú àgùntàn",
            "poem": 1
          },
          {
            "text": "láti jẹ́ olùṣọ́-àgùntàn àwọn ènìyàn rẹ̀ Jakọbu",
            "poem": 2
          },
          {
            "text": "àti Israẹli ogún un rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 72,
        "content": [
          {
            "text": "Dafidi sì ṣọ́ wọn pẹ̀lú òtítọ́ inú ọkàn;",
            "poem": 1
          },
          {
            "text": "pẹ̀lú ọwọ́ òye ni ó fi darí wọn.",
            "poem": 2
          }
        ]
      }
