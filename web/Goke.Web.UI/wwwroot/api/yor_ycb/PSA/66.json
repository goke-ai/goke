  "chapter": {
    "number": 66,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Fún adarí orin. Orin. Saamu."
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ẹ hó ìhó ayọ̀ sí Ọlọ́run, ẹ̀yin ilẹ̀ gbogbo!",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ẹ kọrin ọlá orúkọ rẹ̀;",
            "poem": 2
          },
          {
            "text": "ẹ kọrin ìyìnsí i.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ẹ wí fún Ọlọ́run pé, “Ìwọ ti ní ẹ̀rù tó nínú iṣẹ́ rẹ!",
            "poem": 1
          },
          {
            "text": "Nípa ọ̀pọ̀ agbára rẹ",
            "poem": 2
          },
          {
            "text": "ni àwọn ọ̀tá rẹ yóò fi sìn ọ́.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Gbogbo ayé ń wólẹ̀ fún ọ;",
            "poem": 1
          },
          {
            "text": "wọn ń kọrin ìyìn sí ọ,",
            "poem": 2
          },
          {
            "text": "wọ́n ń kọrin ìyìn sí orúkọ rẹ.” Sela.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ẹ wá wo ohun tí Ọlọ́run ṣe,",
            "poem": 1
          },
          {
            "text": "iṣẹ́ rẹ̀ ti ní ẹ̀rù tó sí àwọn ọmọ ènìyàn!",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ó yí òkun padà sí ilẹ̀ gbígbẹ,",
            "poem": 1
          },
          {
            "text": "wọ́n fi ẹsẹ̀ rin inú omi kọjá,",
            "poem": 2
          },
          {
            "text": "níbẹ̀ ni àwa yọ̀ nínú rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ó ń jẹ ọba títí láé nípa agbára rẹ̀,",
            "poem": 1
          },
          {
            "text": "ojú rẹ̀ ń wo orílẹ̀-èdè",
            "poem": 2
          },
          {
            "text": "kí àwọn ọlọ̀tẹ̀ má ṣe gbé ara wọn ga. Sela.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ẹ yin Ọlọ́run wa, ẹ̀yin ènìyàn,",
            "poem": 1
          },
          {
            "text": "jẹ́ kí a mú ni gbọ́ ohùn ìyìn rẹ̀;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Ó ti dá ààbò bo ẹ̀mí wa,",
            "poem": 1
          },
          {
            "text": "kò sì jẹ́ kí ẹsẹ̀ wa kí ó yẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Nítorí ìwọ, Ọlọ́run, dán wa wò;",
            "poem": 1
          },
          {
            "text": "ìwọ dán wa bí a tí ń dán fàdákà wò.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ìwọ mú wa wá sínú ẹ̀wọ̀n",
            "poem": 1
          },
          {
            "text": "o sì di ẹrù lé ẹ̀yìn wa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ìwọ mú àwọn ènìyàn gún wá ní orí",
            "poem": 1
          },
          {
            "text": "àwa la iná àti omi kọjá",
            "poem": 2
          },
          {
            "text": "ṣùgbọ́n ìwọ mú wa dé ibi ọ̀pọ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Èmi ó wá sí tẹmpili rẹ pẹ̀lú ọrẹ ẹbọ sísun,",
            "poem": 1
          },
          {
            "text": "kí n sì mú ẹ̀jẹ́ mi ṣẹ sí ọ,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "ẹ̀jẹ́ tí ètè mi jẹ́, tí ẹnu mi sì sọ",
            "poem": 1
          },
          {
            "text": "nígbà tí mo wà nínú ìṣòro.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Èmi ó rú ẹbọ sísun ọlọ́ràá sí ọ,",
            "poem": 1
          },
          {
            "text": "àti ẹbọ ọ̀rá àgbò;",
            "poem": 2
          },
          {
            "text": "èmi ó rú akọ màlúù àti ewúrẹ́. Sela.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Ẹ wá gbọ́ gbogbo ẹ̀yin tí ó bẹ̀rù Ọlọ́run;",
            "poem": 1
          },
          {
            "text": "ẹ jẹ́ kí n sọ ohun tí ó ṣe fún mi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Mo fi ẹnu mi kígbe sókè sí i,",
            "poem": 1
          },
          {
            "text": "ìyìn rẹ̀ wà ní ẹnu mi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Bí èmi bá gba ẹ̀ṣẹ̀ ní àyà mi,",
            "poem": 1
          },
          {
            "text": "Olúwa kì yóò gbọ́ ohùn mi;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "ṣùgbọ́n Ọlọ́run gbọ́ nítòótọ́",
            "poem": 1
          },
          {
            "text": "ó ti gbọ́ ohùn mi nínú àdúrà.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Ìyìn ni fún Ọlọ́run",
            "poem": 1
          },
          {
            "text": "ẹni tí kò kọ àdúrà mi",
            "poem": 2
          },
          {
            "text": "tàbí mú ìfẹ́ rẹ̀ kúrò lọ́dọ̀ mi!",
            "poem": 2
          }
        ]
      }
    ],
