    "commonName": "Saamu",
    "title": "Ìwé Saamu",
    "order": 19,
    "numberOfChapters": 150,
    "sha256": "0ad2683a279b6e0fa232310ea04ed3216c9da675f26a00ab4a8af4bfc4c3b24d",
    "firstChapterApiLink": "/api/yor_ycb/PSA/1.json",
