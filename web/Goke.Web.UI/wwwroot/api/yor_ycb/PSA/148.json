  "chapter": {
    "number": 148,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ẹ fi ìyìn fún Olúwa.",
            "poem": 1
          },
          {
            "lineBreak": true
          },
          {
            "text": "Ẹ fi ìyìn fún Olúwa láti ọ̀run wá,",
            "poem": 1
          },
          {
            "text": "ẹ fi ìyìn fún un níbi gíga.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ẹ fi ìyìn fún un,",
            "poem": 1
          },
          {
            "text": "gbogbo ẹ̀yin angẹli rẹ̀,",
            "poem": 2
          },
          {
            "text": "ẹ fi ìyìn fún un,",
            "poem": 1
          },
          {
            "text": "gbogbo ẹ̀yin ọmọ-ogun rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ẹ fi ìyìn fún un,",
            "poem": 1
          },
          {
            "text": "oòrùn àti òṣùpá.",
            "poem": 2
          },
          {
            "text": "Ẹ fi ìyìn fún un,",
            "poem": 1
          },
          {
            "text": "gbogbo ẹ̀yin ìràwọ̀ ìmọ́lẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ẹ fi ìyìn fún un,",
            "poem": 1
          },
          {
            "text": "ẹ̀yin ọ̀run àti àwọn ọ̀run gíga",
            "poem": 2
          },
          {
            "text": "àti ẹ̀yin omi tí ń bẹ ní òkè ọ̀run.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ẹ jẹ́ kí wọn kí ó yin orúkọ Olúwa",
            "poem": 1
          },
          {
            "text": "nítorí ó pàṣẹ a sì dá wọn",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ó fi ìdí wọn múlẹ̀ láé àti láéláé",
            "poem": 1
          },
          {
            "text": "ó sì ti ṣe ìlànà kan tí kì yóò já.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ẹ yin Olúwa láti ilẹ̀ ayé wá",
            "poem": 1
          },
          {
            "text": "ẹ̀yin ẹ̀dá inú òkun títóbi",
            "poem": 2
          },
          {
            "text": "àti ẹ̀yin ibú òkun,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "mọ̀nàmọ́ná àti òjò yìnyín",
            "poem": 1
          },
          {
            "text": "ìdí omi àti àwọn ìkùùkuu,",
            "poem": 2
          },
          {
            "text": "ìjì líle tí ń mú ọ̀rọ̀ rẹ̀ ṣẹ,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "òkè ńlá àti gbogbo òkè kéékèèké,",
            "poem": 1
          },
          {
            "text": "igi eléso àti gbogbo igi kedari,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "àwọn ẹranko ńlá àti ẹran ọ̀sìn",
            "poem": 1
          },
          {
            "text": "gbogbo ohun afàyàfà àti àwọn ẹyẹ abìyẹ́,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "àwọn ọba ayé àti ènìyàn ayé gbogbo",
            "poem": 1
          },
          {
            "text": "àwọn ọmọ-aládé àti gbogbo àwọn onídàájọ́ ayé,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "ọ̀dọ́mọkùnrin àti ọ̀dọ́mọbìnrin",
            "poem": 1
          },
          {
            "text": "àwọn arúgbó àti àwọn ọmọdé.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Ẹ jẹ́ kí wọn kí ó yin orúkọ Olúwa",
            "poem": 1
          },
          {
            "text": "nítorí orúkọ rẹ̀ nìkan ni ó ní ọlá",
            "poem": 2
          },
          {
            "text": "ògo rẹ̀ kọjá ayé àti ọ̀run",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Ó sì gbé ìwo kan sókè fún àwọn ènìyàn rẹ̀,",
            "poem": 1
          },
          {
            "text": "ìyìn fún gbogbo ènìyàn mímọ́ rẹ̀ àní fún àwọn ọmọ Israẹli,",
            "poem": 2
          },
          {
            "text": "àwọn ènìyàn tí ó súnmọ́ ọ̀dọ̀ rẹ̀.",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Ẹ fi ìyìn fún Olúwa.",
            "poem": 1
          }
        ]
      }
    ],
