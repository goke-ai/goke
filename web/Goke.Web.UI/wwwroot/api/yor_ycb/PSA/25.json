  "chapter": {
    "number": 25,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Ti Dafidi."
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Olúwa,",
            "poem": 1
          },
          {
            "text": "ìwọ ni mo gbé ọkàn mi sókè sí.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ọlọ́run, èmi gbẹ́kẹ̀lé ọ;",
            "poem": 1
          },
          {
            "text": "má ṣe jẹ́ kí ojú ó tì mí",
            "poem": 2
          },
          {
            "text": "má ṣe jẹ́ kí àwọn ọ̀tá mi ó yọ̀ mí.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ẹni tí ó dúró tì ọ́",
            "poem": 1
          },
          {
            "text": "ojú kì yóò tì í,",
            "poem": 2
          },
          {
            "text": "àwọn tí ń ṣẹ̀ láìnídìí",
            "poem": 1
          },
          {
            "text": "ni kí ojú kí ó tì.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Fi ọ̀nà rẹ hàn mí, Olúwa,",
            "poem": 1
          },
          {
            "text": "kọ mi ní ipa tìrẹ;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "ṣe amọ̀nà mi nínú òtítọ́ ọ̀ rẹ, kí o sì kọ́ mi,",
            "poem": 1
          },
          {
            "text": "nítorí ìwọ ni Ọlọ́run ìgbàlà mi;",
            "poem": 2
          },
          {
            "text": "ìwọ ni mo dúró tì ní gbogbo ọjọ́.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Rántí, Olúwa àánú àti ìfẹ́ ẹ̀ rẹ̀ ńlá,",
            "poem": 1
          },
          {
            "text": "torí pé wọ́n ti wà ní ìgbà àtijọ́.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Má ṣe rántí àwọn ẹ̀ṣẹ̀ ìgbà èwe mi",
            "poem": 1
          },
          {
            "text": "tàbí ìrékọjá mi;",
            "poem": 2
          },
          {
            "text": "gẹ́gẹ́ bí i ìfẹ́ ẹ rẹ̀ rántí mi",
            "poem": 1
          },
          {
            "text": "nítorí tí ìwọ dára, Ìwọ! Olúwa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Rere àti ẹni ìdúró ṣinṣin ní Olúwa:",
            "poem": 1
          },
          {
            "text": "nítorí náà, ó kọ́ àwọn ẹlẹ́ṣẹ̀ ni ọ̀nà náà.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Ó ṣe amọ̀nà àwọn onírẹ̀lẹ̀ ni ohun tí ó dára,",
            "poem": 1
          },
          {
            "text": "ó sì kọ́ àwọn onírẹ̀lẹ̀ ní ọ̀nà rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Gbogbo ipa ọ̀nà Olúwa ni ìfẹ́ àti òdodo tí ó dúró ṣinṣin,",
            "poem": 1
          },
          {
            "text": "fún àwọn tí ó pa májẹ̀mú àti ẹ̀rí rẹ̀ mọ́.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Nítorí orúkọ rẹ, Olúwa,",
            "poem": 1
          },
          {
            "text": "dárí ẹ̀bi ẹ̀ṣẹ̀ mi jì mí, nítorí tí ó tóbi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Àwọn wo ni ó bẹ̀rù Olúwa?",
            "poem": 1
          },
          {
            "text": "Yóò kọ́ wọn ní ọ̀nà èyí tí wọn yóò yàn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Wọn yóò lo ọjọ́ wọn nínú àlàáfíà,",
            "poem": 1
          },
          {
            "text": "àwọn ọmọ wọn yóò sì gba ilẹ̀ náà.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Olúwa fi ọkàn tán, àwọn tí o bẹ̀rù u rẹ̀;",
            "poem": 1
          },
          {
            "text": "ó sọ májẹ̀mú rẹ̀ di mí mọ̀ fún wọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Ojú mi gbé sókè sọ́dọ̀ Olúwa,",
            "poem": 1
          },
          {
            "text": "nítorí pé yóò yọ ẹsẹ̀ mi kúrò nínú àwọ̀n náà.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Yípadà sí mi, kí o sì ṣe oore fún mi;",
            "poem": 1
          },
          {
            "text": "nítorí pé mo nìkan wà, mo sì di olùpọ́njú.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Tì mí lẹ́yìn kúrò nínú ìpọ́njú àyà mi;",
            "poem": 1
          },
          {
            "text": "kí o sì fà mí yọ kúrò nínú ìpọ́njú mi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Kíyèsi ìjìyà àti wàhálà mi,",
            "poem": 1
          },
          {
            "text": "kí o sì dárí gbogbo ẹ̀ṣẹ̀ mi jì.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Kíyèsi ọ̀pọ̀lọpọ̀ àwọn ọ̀tá mi,",
            "poem": 1
          },
          {
            "text": "tí wọn kórìíra mi pẹ̀lú ìwà ìkà wọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Pa ọkàn mi mọ́, kí o sì gbà mí sílẹ̀;",
            "poem": 1
          },
          {
            "text": "má ṣe jẹ́ kí a fi mí sínú ìtìjú,",
            "poem": 2
          },
          {
            "text": "nítorí pé ìwọ ni ibi ìsádi mi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Jẹ́ kí òtítọ́ inú àti ìdúró ṣinṣin kí ó pa mí mọ́;",
            "poem": 1
          },
          {
            "text": "nítorí pé mo dúró tì ọ́.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "Ra Israẹli padà, Ìwọ Ọlọ́run,",
            "poem": 1
          },
          {
            "text": "nínú gbogbo ìṣòro rẹ̀!",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": []
  },
