  "chapter": {
    "number": 93,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Olúwa ń jẹ ọba, ọláńlá ni ó wọ̀ ní aṣọ;",
            "poem": 1
          },
          {
            "text": "ọláńlá ni Olúwa wọ̀ ní aṣọ",
            "poem": 2
          },
          {
            "text": "àti ìhámọ́ra rẹ̀ pẹ̀lú agbára.",
            "poem": 2
          },
          {
            "text": "Ó fi ìdí ayé múlẹ̀;",
            "poem": 1
          },
          {
            "text": "kò sì le è yí.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ìjọba rẹ̀ wà láti ọjọ́ pípẹ́;",
            "poem": 1
          },
          {
            "text": "ìwọ wà títí ayérayé.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "A ti gbé òkun sókè, Olúwa,",
            "poem": 1
          },
          {
            "text": "òkun ti gbé ohùn wọn sókè;",
            "poem": 2
          },
          {
            "text": "òkun ti gbé rírú omi wọn sókè.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ó ni ògo ju àrá omi ńlá lọ,",
            "poem": 1
          },
          {
            "text": "ó ni ògo ju òkun rírú lọ",
            "poem": 2
          },
          {
            "text": "Olúwa ga ní ògo.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ẹ̀rí rẹ̀ dúró ṣinṣin;",
            "poem": 1
          },
          {
            "text": "ìwà mímọ́ ni ó fi ṣe ilé rẹ̀ lọ́ṣọ̀ọ́",
            "poem": 2
          },
          {
            "text": "fún ọjọ́ àìlópin, Olúwa.",
            "poem": 2
          }
        ]
      }
    ],
