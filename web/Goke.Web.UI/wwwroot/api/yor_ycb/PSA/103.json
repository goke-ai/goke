  "chapter": {
    "number": 103,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Ti Dafidi."
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Yin Olúwa, ìwọ ọkàn mi; àti gbogbo ohun tí ó wà nínú mi, yín orúkọ rẹ̀ mímọ́.",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Yin Olúwa, ìwọ ọkàn mi, kí o má ṣe gbàgbé gbogbo oore rẹ̀,",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "ẹni tí ó dárí gbogbo ẹ̀ṣẹ̀ rẹ̀ jì ọ́",
            "poem": 1
          },
          {
            "text": "tí ó sì wo gbogbo ààrùn rẹ̀ sàn,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "ẹni tí ó ra ẹ̀mí rẹ padà kúrò nínú kòtò ikú",
            "poem": 1
          },
          {
            "text": "ẹni tí ó fi ìṣeun ìfẹ́ àti ìyọ́nú dé ọ ní adé,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "ẹni tí ó fi ohun dídára tẹ́ ọ lọ́rùn",
            "poem": 1
          },
          {
            "text": "kí ìgbà èwe rẹ̀ lè di ọ̀tún bí ti ẹyẹ idì.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Olúwa ń ṣe òdodo àti ìdájọ́ fún",
            "poem": 1
          },
          {
            "text": "gbogbo àwọn tí a ni lára.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ó fi ọ̀nà rẹ̀ hàn fún Mose, iṣẹ́ rẹ̀ fun àwọn ọmọ Israẹli;",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Olúwa ni aláàánú àti olóore,",
            "poem": 1
          },
          {
            "text": "ó lọ́ra láti bínú, ó sì pọ̀ ní ìfẹ́.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Òun kì í bá ni wí ní ìgbà gbogbo",
            "poem": 1
          },
          {
            "text": "bẹ́ẹ̀ ni kì í pa ìbínú rẹ mọ́ láéláé,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Òun kì í ṣe sí wa gẹ́gẹ́ bí ẹ̀ṣẹ̀ wa",
            "poem": 1
          },
          {
            "text": "bẹ́ẹ̀ ni kì í san án fún wa gẹ́gẹ́",
            "poem": 2
          },
          {
            "text": "bí àìṣedéédéé wa.",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Nítorí bí ọ̀run ṣe ga sílẹ̀,",
            "poem": 1
          },
          {
            "text": "bẹ́ẹ̀ ni ìfẹ́ rẹ̀ tóbi sí àwọn tí ó bẹ̀rù rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Bí ìlà-oòrùn ti jìnnà sí ìwọ̀-oòrùn",
            "poem": 1
          },
          {
            "text": "bẹ́ẹ̀ ni ó ṣe mú ìrékọjá wá jìnnà sí wa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Bí baba ti ń ṣe ìyọ́nú sí àwọn ọmọ rẹ̀,",
            "poem": 1
          },
          {
            "text": "bẹ́ẹ̀ ni Olúwa ń ṣe ìyọ́nú sí àwọn tí ó bẹ̀rù rẹ̀,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "nítorí tí ó mọ dídá wa,",
            "poem": 1
          },
          {
            "text": "ó rántí pé erùpẹ̀ ni wá.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Bí ó ṣe ti ènìyàn ni, ọjọ́ rẹ̀ dàbí koríko,",
            "poem": 1
          },
          {
            "text": "ó gbilẹ̀ bí ìtànná ewéko igbó,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "afẹ́fẹ́ fẹ́ kọjá lọ lórí rẹ̀,",
            "poem": 1
          },
          {
            "text": "kò sì rántí ibùjókòó rẹ̀ mọ́.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Ṣùgbọ́n láti ayérayé ni ìfẹ́",
            "poem": 1
          },
          {
            "text": "Olúwa ti wà pẹ̀lú àwọn tí ó bẹ̀rù rẹ̀,",
            "poem": 2
          },
          {
            "text": "àti òdodo rẹ̀ wà láti ọmọdọ́mọ,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "sí àwọn tí ó pa májẹ̀mú rẹ̀ mọ́",
            "poem": 1
          },
          {
            "text": "àti àwọn tí ó rántí òfin rẹ̀ láti ṣe wọ́n.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Olúwa ti pèsè ìtẹ́ rẹ̀ nínú ọ̀run,",
            "poem": 1
          },
          {
            "text": "ìjọba rẹ̀ ní ó sì borí ohun gbogbo.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Yin Olúwa, ẹ̀yin angẹli rẹ̀,",
            "poem": 1
          },
          {
            "text": "tí ó ní ipá,",
            "poem": 2
          },
          {
            "text": "tí ó pa òfin ọ̀rọ̀ rẹ̀ mọ́, tì ó sì ń se ìfẹ́ rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Yin Olúwa, ẹ̀yin ogun ọ̀run rẹ̀ gbogbo,",
            "poem": 1
          },
          {
            "text": "ẹ̀yin ìránṣẹ́ rẹ̀ tí ń ṣe ìfẹ́ rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "Yin Olúwa, gbogbo iṣẹ́ rẹ̀ ní",
            "poem": 1
          },
          {
            "text": "ibi gbogbo ìjọba rẹ̀.",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Yin Olúwa, ìwọ ọkàn mi.",
            "poem": 1
          }
        ]
      }
    ],
