  "chapter": {
    "number": 134,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Orin ìgòkè."
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ẹ kíyèsi i, ẹ fi ìbùkún fún Olúwa,",
            "poem": 1
          },
          {
            "text": "gbogbo ẹ̀yin ìránṣẹ́ Olúwa,",
            "poem": 2
          },
          {
            "text": "tí ó dúró ní ilé Olúwa ní òru.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ẹ gbé ọwọ́ yín sókè sí ibi mímọ́,",
            "poem": 1
          },
          {
            "text": "kí ẹ sì fi ìbùkún fún Olúwa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Olúwa tí ó dá ọ̀run òun ayé,",
            "poem": 1
          },
          {
            "text": "kí ó bùsi i fún ọ láti Sioni wá.",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": []
  },
