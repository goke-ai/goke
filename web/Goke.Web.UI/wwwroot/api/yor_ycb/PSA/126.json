  "chapter": {
    "number": 126,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Orin fún ìgòkè."
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Nígbà tí Olúwa mú ìkólọ Sioni padà,",
            "poem": 1
          },
          {
            "text": "àwa dàbí ẹni tí ó ń lá àlá.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Nígbà náà ni ẹnu wa kún fún ẹ̀rín,",
            "poem": 1
          },
          {
            "text": "àti ahọ́n wa kọ orin;",
            "poem": 2
          },
          {
            "text": "nígbà náà ni wọ́n wí nínú àwọn kèfèrí pé,",
            "poem": 1
          },
          {
            "text": "Olúwa ṣe ohun ńlá fún wọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Olúwa ṣe ohun ńlá fún wa;",
            "poem": 1
          },
          {
            "text": "nítorí náà àwa ń yọ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Olúwa mú ìkólọ wa padà,",
            "poem": 1
          },
          {
            "text": "bí ìṣàn omi ní gúúsù.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Àwọn tí ń fi omijé fún irúgbìn",
            "poem": 1
          },
          {
            "text": "yóò fi ayọ̀ ka.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ẹni tí ń fi ẹkún rìn lọ,",
            "poem": 1
          },
          {
            "text": "tí ó sì gbé irúgbìn lọ́wọ́,",
            "poem": 2
          },
          {
            "text": "lóòtítọ́, yóò fi ayọ̀ padà wá,",
            "poem": 1
          },
          {
            "text": "yóò sì ru ìtí rẹ̀.",
            "poem": 2
          }
        ]
      }
    ],
