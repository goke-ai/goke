  "chapter": {
    "number": 100,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Saamu. Fún ọpẹ́."
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ẹ hó ìhó ayọ̀ sí Olúwa, gbogbo ayé.",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ẹ fi ayọ̀ sin Olúwa,",
            "poem": 1
          },
          {
            "text": "ẹ wá síwájú rẹ̀ pẹ̀lú orin dídùn",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Olúwa Ọlọ́run ni ó dá wa,",
            "poem": 1
          },
          {
            "text": "kí ẹ̀yin kí ó mọ̀ pé",
            "poem": 2
          },
          {
            "text": "tirẹ̀ ni àwa, àwa ní ènìyàn rẹ̀",
            "poem": 1
          },
          {
            "text": "àti àgùntàn pápá rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ẹ lọ sí ẹnu-ọ̀nà rẹ̀ pẹ̀lú ọpẹ́",
            "poem": 1
          },
          {
            "text": "àti sí àgbàlá rẹ̀ pẹ̀lú ìyìn;",
            "poem": 2
          },
          {
            "text": "ẹ fi ọpẹ́ àti ìyìn fún orúkọ rẹ̀.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Nítorí tí Olúwa pọ̀ ní oore",
            "poem": 1
          },
          {
            "text": "ìfẹ́ rẹ̀ sì wà títí láé;",
            "poem": 2
          },
          {
            "text": "àti òtítọ́ rẹ̀ láti ìran de ìran.",
            "poem": 2
          }
        ]
      }
    ],
