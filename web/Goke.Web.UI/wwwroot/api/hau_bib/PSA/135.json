  "chapter": {
    "number": 135,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Yabi Ubangiji.",
            "poem": 1
          },
          {
            "noteId": 1207
          },
          {
            "lineBreak": true
          },
          {
            "text": "Yabi sunan Ubangiji;",
            "poem": 1
          },
          {
            "text": "yabe shi, ku bayin Ubangiji,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "ku waɗanda kuke hidima a gidan Ubangiji,",
            "poem": 1
          },
          {
            "text": "cikin filayen gidan Allahnmu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Yabi Ubangiji, gama Ubangiji nagari ne;",
            "poem": 1
          },
          {
            "text": "rera yabo ga sunansa, gama wannan yana da kyau.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Gama Ubangiji ya zaɓi Yaƙub ya zama nasa,",
            "poem": 1
          },
          {
            "text": "Isra’ila ya zama mallakarsa mai daraja.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Na san cewa Ubangiji yana da girma,",
            "poem": 1
          },
          {
            "text": "cewa shugabanmu ya fi dukan alloli girma.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ubangiji yana yin abin da ya ga dama,",
            "poem": 1
          },
          {
            "text": "a sammai da kuma a duniya,",
            "poem": 2
          },
          {
            "text": "cikin tekuna da kuma cikin dukan zurfafansu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Yakan sa gizagizai su taso daga iyakokin duniya;",
            "poem": 1
          },
          {
            "text": "yakan aika da walƙiya tare da ruwan sama",
            "poem": 2
          },
          {
            "text": "ya fito da iska daga ɗakunan ajiyarsa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ya kashe ’ya’yan fari na Masar,",
            "poem": 1
          },
          {
            "text": "’ya’yan fari na mutane da na dabbobi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Ya aiko da alamu da abubuwan banmamaki a tsirkiyarku, ya Masar,",
            "poem": 1
          },
          {
            "text": "a kan Fir’auna da kuma dukan bayinsa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Ya bugi al’ummai masu yawa",
            "poem": 1
          },
          {
            "text": "ya kuma karkashe manyan sarakuna,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Sihon sarkin Amoriyawa,",
            "poem": 1
          },
          {
            "text": "Og sarkin Bashan",
            "poem": 2
          },
          {
            "text": "da kuma dukan sarakunan Kan’ana,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "ya kuma ba da ƙasarsa kamar abin gādo,",
            "poem": 1
          },
          {
            "text": "abin gādo ga mutanensa Isra’ila.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Sunanka, ya Ubangiji, dawwammame ne har abada,",
            "poem": 1
          },
          {
            "text": "sanin da aka yi maka, ya Ubangiji, yana nan cikin dukan zamanai.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Gama Ubangiji zai nuna cewa mutanensa ba su da laifi",
            "poem": 1
          },
          {
            "text": "ya kuma ji tausayin bayinsa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Gumakan al’ummai azurfa ne da zinariya,",
            "poem": 1
          },
          {
            "text": "da hannuwan mutane suka yi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Suna da bakuna, amma ba sa magana,",
            "poem": 1
          },
          {
            "text": "idanu, amma ba sa gani;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "suna da kunnuwa, amma ba sa ji,",
            "poem": 1
          },
          {
            "text": "ba kuwa numfashi a bakunansu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Waɗanda suka yi su za su zama kamar su,",
            "poem": 1
          },
          {
            "text": "haka kuma zai zama da dukan waɗanda suke dogara gare su.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Ya gidan Isra’ila, yabi Ubangiji;",
            "poem": 1
          },
          {
            "text": "Ya gidan Haruna, yabi Ubangiji;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Ya gidan Lawi, yabi Ubangiji;",
            "poem": 1
          },
          {
            "text": "ku da kuke tsoronsa, yabi Ubangiji.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Yabo ya tabbata ga Ubangiji daga Sihiyona,",
            "poem": 1
          },
          {
            "text": "gare shi wanda yake zama a Urushalima.",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Yabi Ubangiji.",
            "poem": 1
          }
        ]
      }
    ],
