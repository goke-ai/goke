  "chapter": {
    "number": 90,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Addu’ar Musa Mutumin Allah."
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ubangiji kai ne mazauninmu",
            "poem": 1
          },
          {
            "text": "cikin dukan zamanai.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Kafin a haifi duwatsu",
            "poem": 1
          },
          {
            "text": "ko a fid da ƙasa da duniya,",
            "poem": 2
          },
          {
            "text": "daga madawwami zuwa madawwami kai Allah ne.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ka komar da mutane zuwa ƙura,",
            "poem": 1
          },
          {
            "text": "kana cewa, “Ku koma ƙura, ya ku ’ya’yan mutane.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Gama shekaru dubu a gabanka",
            "poem": 1
          },
          {
            "text": "kamar kwana ɗaya ne da ya wuce,",
            "poem": 2
          },
          {
            "text": "ko sa’a guda na dare.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ka share mutane cikin barcin mutuwa;",
            "poem": 1
          },
          {
            "text": "suna kama da sabuwa ciyawar safiya,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "ko da yake da safe takan yi sabuwar huda",
            "poem": 1
          },
          {
            "text": "da yamma sai ta bushe ta kuma yanƙwane.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "An cinye mu ta wurin fushinka",
            "poem": 1
          },
          {
            "text": "mun kuma razana ta wurin hasalarka.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ka ajiye laifinmu a gabanka,",
            "poem": 1
          },
          {
            "text": "asirin zunubanmu a hasken kasancewarka.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Dukan kwanakinmu sun wuce ƙarƙashin fushinka;",
            "poem": 1
          },
          {
            "text": "mun gama shekarunmu da nishi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Tsawon kwanakinmu shekaru saba’in ne,",
            "poem": 1
          },
          {
            "text": "ko tamanin, in muna da ƙarfi;",
            "poem": 2
          },
          {
            "text": "duk da haka tsawonsu wahaloli ne kawai da ɓacin rai,",
            "poem": 1
          },
          {
            "text": "gama da sauri suke wucewa, ta mu kuma ta ƙare.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Wa ya san ƙarfin fushinka?",
            "poem": 1
          },
          {
            "text": "Gama hasalarka tana da girma kamar tsoron da ya dace da kai.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ka koya mana yawan kwanakinmu daidai,",
            "poem": 1
          },
          {
            "text": "don mu sami zuciyar hikima.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Ka ji mu, ya Ubangiji! Har yaushe zai ci gaba?",
            "poem": 1
          },
          {
            "text": "Ka ji tausayin bayinka.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Ka ƙosar da mu da safe da ƙaunarka marar ƙarewa,",
            "poem": 1
          },
          {
            "text": "don mu rera don yabo, mu kuma yi murna dukan kwanakinmu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Faranta mana zuciya kamar yawan kwanakin da muka sha azaba,",
            "poem": 1
          },
          {
            "text": "kamar yawan shekarun da muka ga wahala.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Bari a nuna ayyukanka ga bayinka,",
            "poem": 1
          },
          {
            "text": "darajarka ga ’ya’yansu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Bari alherin shugabanmu Allah yă kasance a kanmu;",
            "poem": 1
          },
          {
            "text": "ka albarkaci mana aikin hannuwanmu,",
            "poem": 2
          },
          {
            "text": "I, ka albarkaci aikin hannuwanmu.",
            "poem": 2
          }
        ]
      }
    ],
