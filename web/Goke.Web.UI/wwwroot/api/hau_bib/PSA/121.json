  "chapter": {
    "number": 121,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Waƙar haurawa."
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Na tā da idanuna zuwa wajen tuddai,",
            "poem": 1
          },
          {
            "text": "ta ina ne taimakona zai zo?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Taimakona zai zo daga wurin Ubangiji,",
            "poem": 1
          },
          {
            "text": "wanda ya kafa sama da ƙasa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ba zai bari ƙafarka tă yi santsi ba,",
            "poem": 1
          },
          {
            "text": "shi da yake tsaronka ba zai yi gyangyaɗi ba;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "tabbatacce, shi da yake tsaron Isra’ila",
            "poem": 1
          },
          {
            "text": "ba ya gyangyaɗi ko barci.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ubangiji yana tsaronka,",
            "poem": 1
          },
          {
            "text": "Ubangiji ne inuwa a hannun damanka;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "rana ba za tă buge ka cikin yini ba",
            "poem": 1
          },
          {
            "text": "balle wata da dare.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ubangiji zai kiyaye ka daga dukan masifa,",
            "poem": 1
          },
          {
            "text": "zai tsare ranka;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ubangiji zai kiyaye shigarka da fitarka",
            "poem": 1
          },
          {
            "text": "yanzu da har abada kuma.",
            "poem": 2
          }
        ]
      }
    ],
