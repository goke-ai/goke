  "chapter": {
    "number": 133,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Waƙar haurawa. Ta Dawuda."
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Abu mai kyau ne, mai daɗi kuma",
            "poem": 1
          },
          {
            "text": "sa’ad da ’yan’uwa suna zaman lafiya!",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Yana kamar mai mai darajar da aka zuba a kai,",
            "poem": 1
          },
          {
            "text": "yana gangarawa a gemu,",
            "poem": 2
          },
          {
            "text": "yana gangarawa a gemun Haruna,",
            "poem": 1
          },
          {
            "text": "har zuwa wuyan rigunansa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Yana kamar raɓar Hermon",
            "poem": 1
          },
          {
            "text": "da take zubowa a kan Dutsen Sihiyona.",
            "poem": 2
          },
          {
            "text": "Gama a can Ubangiji ya tabbatar da albarkarsa,",
            "poem": 1
          },
          {
            "text": "har ma rai na har abada.",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": []
