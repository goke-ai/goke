  "chapter": {
    "number": 110,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Ta Dawuda. Zabura ce."
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ubangiji ya ce wa Ubangijina,",
            "poem": 1
          },
          {
            "text": "“Zauna a hannun damana,",
            "poem": 2
          },
          {
            "text": "sai na sa abokan gābanka",
            "poem": 1
          },
          {
            "text": "su zama matashin sawunka.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ubangiji zai fadada ikon sandan mulkinka daga Sihiyona",
            "poem": 1
          },
          {
            "text": "za ka yi mulki a tsakiyar abokan gābanka.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Rundunarka za su so yin yaƙi",
            "poem": 1
          },
          {
            "text": "a ranarka ta yaƙi.",
            "poem": 2
          },
          {
            "text": "Saye cikin ɗaukaka mai tsarki,",
            "poem": 1
          },
          {
            "text": "daga cikin ciki na safiya",
            "poem": 2
          },
          {
            "text": "za ka karɓi raɓar ƙuruciyarka.",
            "poem": 2
          },
          {
            "noteId": 1195
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ubangiji ya rantse",
            "poem": 1
          },
          {
            "text": "ba zai kuma canja zuciyarsa ba,",
            "poem": 2
          },
          {
            "text": "“Kai firist ne har abada,",
            "poem": 1
          },
          {
            "text": "bisa ga tsarin Melkizedek.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ubangiji yana a hannun damanka;",
            "poem": 1
          },
          {
            "text": "zai murƙushe sarakuna a ranar fushinsa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Zai hukunta al’ummai, yana tarawa gawawwaki",
            "poem": 1
          },
          {
            "text": "yana kuma murƙushe masu mulkin dukan duniya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Zai sha daga rafin da yake kusa da hanya;",
            "poem": 1
          },
          {
            "text": "saboda haka zai ɗaga kansa sama.",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": [
      {
        "noteId": 1195,
        "caller": "+",
        "text": "Ko kuwa / samarinka za su zo gare ka kamar raba",
        "reference": {
          "chapter": 110,
          "verse": 3
        }
      }
    ]
