  "chapter": {
    "number": 136,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ku yi godiya ga Ubangiji, gama nagari ne shi.",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ku yi godiya ga Allahn alloli.",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ku yi godiya ga Ubangijin iyayengiji.",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Gare shi wanda shi kaɗai yake aikata abubuwan banmamaki,",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Wanda ta wurin ganewarsa ya yi sammai,",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Wanda ya shimfiɗa duniya a kan ruwaye,",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Wanda ya yi manyan haskoki,",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Rana don tă yi mulkin yini,",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Wata da taurari kuma don su yi mulkin dare;",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Gare shi wanda ya kashe ’ya’yan fari Masar",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ya kuma fitar da Isra’ila daga cikinsu",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Da hannu mai ƙarfi da hannu mai iko;",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Gare shi wanda ya raba Jan Teku biyu",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Ya kawo Isra’ila ta tsakiyarsa,",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Amma ya share Fir’auna da mayaƙansa cikin Jan Teku;",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Gare shi wanda ya bi da mutanensa cikin hamada,",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Wanda ya buge manyan sarakuna,",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Ya karkashe manyan sarakuna,",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Sihon sarkin Amoriyawa",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Da kuma Og sarkin Bashan,",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Ya kuma ba da ƙasarsu gādo,",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "Gādo ga bawansa Isra’ila;",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 23,
        "content": [
          {
            "text": "Shi wanda ya tuna da mu cikin ƙasƙancinmu",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 24,
        "content": [
          {
            "text": "Ya kuma ’yantar da mu daga abokan gābanmu,",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "verse",
        "number": 25,
        "content": [
          {
            "text": "Wanda kuma ke ba da abinci ga kowace halitta.",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 26,
        "content": [
          {
            "text": "Ku yi godiya ga Allah na sama.",
            "poem": 1
          },
          "Ƙaunarsa madawwamiya ce."
        ]
      }
    ],
