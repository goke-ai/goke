  "chapter": {
    "number": 148,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Yabi Ubangiji.",
            "poem": 1
          },
          {
            "noteId": 1212
          },
          {
            "lineBreak": true
          },
          {
            "text": "Yabi Ubangiji daga sammai,",
            "poem": 1
          },
          {
            "text": "yabe shi a bisa sammai.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Yabe shi, dukanku mala’ikunsa,",
            "poem": 1
          },
          {
            "text": "yabe shi, dukanku rundunarsa na sama.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Yabe shi, rana da wata,",
            "poem": 1
          },
          {
            "text": "yabe shi, dukanku taurari masu haskakawa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Yabe shi, ku bisa sammai",
            "poem": 1
          },
          {
            "text": "da kuma ku ruwan bisa sarari.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Bari su yabi sunan Ubangiji,",
            "poem": 1
          },
          {
            "text": "gama ya umarta aka kuwa halicce su.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ya sa su a wurarensu har abada abadin;",
            "poem": 1
          },
          {
            "text": "ya ba da umarnin da ba zai taɓa shuɗe ba.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Yabi Ubangiji daga duniya,",
            "poem": 1
          },
          {
            "text": "ku manyan halittun teku da kuma dukan zurfafan teku,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "walƙiya da ƙanƙara, dusar ƙanƙara da gizagizai,",
            "poem": 1
          },
          {
            "text": "hadirin iskar da suke biyayya da umarnansa,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "ku duwatsu da dukan tuddai,",
            "poem": 1
          },
          {
            "text": "itatuwa masu ’ya’ya da dukan al’ul,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "namun jeji da dukan dabbobin gida,",
            "poem": 1
          },
          {
            "text": "ƙanana halittu da tsuntsaye masu firiya,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "sarakunan duniya da dukan al’ummai,",
            "poem": 1
          },
          {
            "text": "ku sarakuna da dukan masu mulkin duniya,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "samari da ’yan mata,",
            "poem": 1
          },
          {
            "text": "tsofaffi da yara.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Bari su yabi sunan Ubangiji,",
            "poem": 1
          },
          {
            "text": "gama sunansa ne kaɗai mafi ɗaukaka;",
            "poem": 2
          },
          {
            "text": "darajarsa ta fi ƙarfin duniya da sammai.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Ya tayar wa mutanensa ƙaho,",
            "poem": 1
          },
          {
            "noteId": 1213
          },
          {
            "text": "yabon dukan tsarkakansa,",
            "poem": 2
          },
          {
            "text": "na Isra’ila, mutanen da suke kurkusa da zuciyarsa.",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Yabi Ubangiji.",
            "poem": 1
          }
        ]
      }
    ],
