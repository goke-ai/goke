  "chapter": {
    "number": 111,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Yabi Ubangiji.",
            "poem": 1
          },
          {
            "noteId": 1196
          },
          {
            "lineBreak": true
          },
          {
            "text": "Zan ɗaukaka Ubangiji da dukan zuciyata",
            "poem": 1
          },
          {
            "text": "a cikin majalisar masu aikata gaskiya da kuma a cikin taro.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ayyukan Ubangiji da girma suke;",
            "poem": 1
          },
          {
            "text": "dukan waɗanda suke farin ciki da su suna tunaninsu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ayyukansa masu ɗaukaka da kuma daraja ne,",
            "poem": 1
          },
          {
            "text": "adalcinsa kuma zai dawwama har abada.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ya sa a tuna da abubuwa masu banmamaki nasa;",
            "poem": 1
          },
          {
            "text": "Ubangiji mai alheri ne mai tausayi kuma.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Yana ba da abinci ga waɗanda suke tsoronsa;",
            "poem": 1
          },
          {
            "text": "yakan tuna da alkawarinsa har abada.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ya nuna wa mutanensa ikon ayyukansa,",
            "poem": 1
          },
          {
            "text": "yana ba su ƙasashen waɗansu al’ummai.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ayyukan hannuwansa masu aminci ne da kuma gaskiya;",
            "poem": 1
          },
          {
            "text": "dukan farillansa abin dogara ne.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Tsayayyu ne har abada abadin,",
            "poem": 1
          },
          {
            "text": "ana yinsu cikin aminci da kuma gaskiya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Yana ba da ceto ga mutanensa;",
            "poem": 1
          },
          {
            "text": "ya kafa alkawarinsa har abada,",
            "poem": 2
          },
          {
            "text": "tsarki da banrazana ne sunansa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Tsoron Ubangiji ne mafarin hikima;",
            "poem": 1
          },
          {
            "text": "dukan waɗanda suke bin farillansa suna da ganewa mai kyau.",
            "poem": 2
          },
          {
            "text": "Gare shi ne madawwamin yabo.",
            "poem": 2
          }
        ]
      }
    ],
