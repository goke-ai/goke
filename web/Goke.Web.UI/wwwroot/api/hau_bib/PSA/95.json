  "chapter": {
    "number": 95,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ku zo, bari mu rera don farin ciki ga Ubangiji;",
            "poem": 1
          },
          {
            "text": "bari mu yi sowa da ƙarfi ga Dutsen cetonmu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Bari mu zo gabansa da godiya",
            "poem": 1
          },
          {
            "text": "mu rera gare shi da kiɗi da waƙa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Gama Ubangiji Allah mai girma ne,",
            "poem": 1
          },
          {
            "text": "babban Sarki a bisa dukan alloli.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "A hannunsa ne zurfafan duniya suke,",
            "poem": 1
          },
          {
            "text": "ƙwanƙolin dutse kuma nasa ne.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Teku nasa ne, gama shi ne ya yi shi,",
            "poem": 1
          },
          {
            "text": "da hannuwansa kuma ya yi busasshiyar ƙasa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ku zo, bari mu rusuna mu yi sujada,",
            "poem": 1
          },
          {
            "text": "bari mu durƙusa a gaban Ubangiji Mahaliccinmu;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "gama shi ne Allahnmu",
            "poem": 1
          },
          {
            "text": "mu mutanen makiyayansa ne,",
            "poem": 2
          },
          {
            "text": "garke kuwa a ƙarƙashin kulawarsa.",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Yau, in kuka ji muryarsa,",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "“Kada ku taurare zukatanku kamar yadda kuka yi a Meriba,",
            "poem": 1
          },
          {
            "noteId": 1190
          },
          {
            "text": "kamar yadda kuka yi a wancan rana a Massa",
            "poem": 2
          },
          {
            "noteId": 1191
          },
          {
            "text": "a hamada,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "inda kakanninku suka gwada suka kuma jarraba ni,",
            "poem": 1
          },
          {
            "text": "ko da yake sun ga abin da na yi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Shekara arba’in na yi fushi da wancan tsara;",
            "poem": 1
          },
          {
            "text": "na ce, ‘Su mutane ne waɗanda zukatansu suka kauce,",
            "poem": 2
          },
          {
            "text": "kuma ba su san hanyoyina ba.’",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Saboda haka na yi rantsuwa cikin fushina,",
            "poem": 1
          },
          {
            "text": "‘Ba za su taɓa shiga hutuna ba.’ ”",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": [
      {
        "noteId": 1190,
        "caller": "+",
        "text": "Meriba yana nufin faɗa.",
        "reference": {
          "chapter": 95,
          "verse": 8
        }
      },
      {
        "noteId": 1191,
        "caller": "+",
        "text": "Massa yana nufin gwaji.",
