      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Yabi Ubangiji, dukanku bayin Ubangiji",
            "poem": 1
          },
          {
            "text": "waɗanda kuke hidima da dare a gidan Ubangiji.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ku tā da hannuwanku a cikin wurinsa mai tsarki",
            "poem": 1
          },
          {
            "text": "ku kuma yabi Ubangiji.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Bari Ubangiji, Mahaliccin sama da ƙasa,",
            "poem": 1
          },
          {
            "text": "ya albarkace ku daga Sihiyona.",
            "poem": 2
          }
        ]
      }
