  "chapter": {
    "number": 125,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ nrigo."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Chineke onye na-echebe ndị ya"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ndị niile na-atụkwasị Onyenwe anyị obi na-anọgidesi ike dị ka ugwu Zayọn",
            "poem": 1
          },
          {
            "text": "nke a na-apụghị inugharị, nke na-adịgidekwa nʼọnọdụ ya ruo mgbe ebighị ebi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Dịka ugwu niile si gbaa obodo Jerusalem gburugburu,",
            "poem": 1
          },
          {
            "text": "otu a ka Onyenwe anyị na-agba ndị ya gburugburu",
            "poem": 2
          },
          {
            "text": "ugbu a na ruo mgbe ebighị ebi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Mkpara ọchịchị ndị ajọ omume agaghị adịgide",
            "poem": 1
          },
          {
            "noteId": 1692
          },
          {
            "text": "nʼala ndị ezi omume ketara nʼife nza,",
            "poem": 2
          },
          {
            "text": "nʼihi na ndị ezi omume nwere ike",
            "poem": 1
          },
          {
            "text": "itinye aka ha na-ime ihe ọjọọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "O Onyenwe anyị, meere ndị niile na-eme ezi omume ihe ọma,",
            "poem": 1
          },
          {
            "text": "ya bụ, ndị niile obi ha ziri ezi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ma Onyenwe anyị ga-achụpụ ndị niile na-agbaso ụzọ aghụghọ",
            "poem": 1
          },
          {
            "text": "na ndị niile na-eme ajọ ihe.",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Ka udo dịrị Izrel.",
            "poem": 1
          }
        ]
      }
    ],
