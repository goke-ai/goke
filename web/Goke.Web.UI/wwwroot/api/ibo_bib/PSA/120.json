  "chapter": {
    "number": 120,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ nrigo."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Ekpere maka nnapụta"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ana m akpọku Onyenwe anyị nʼime nsogbu m,",
            "poem": 1
          },
          {
            "text": "ọ na-azakwa m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "O Onyenwe anyị, zọpụta m",
            "poem": 1
          },
          {
            "text": "site nʼegbugbere ọnụ okwu ụgha",
            "poem": 2
          },
          {
            "text": "na ire na-ekwu okwu aghụghọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Gịnị ka a ga-eme gị,",
            "poem": 1
          },
          {
            "text": "ọ bụkwa gịnị ọzọ ka a gatụkwasịri gị,",
            "poem": 2
          },
          {
            "text": "gị ire aghụghọ?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ọ ga-eji àkụ dị nkọ nke dike nʼagha taa gị ahụhụ,",
            "poem": 1
          },
          {
            "text": "ya na icheku ọkụ osisi brum na-ere ere.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ahụhụ dịịrị m nʼihi na m bi na Meshek,",
            "poem": 1
          },
          {
            "text": "nʼihi na m bikwa nʼetiti ụlọ ikwu ndị Keda!",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ogologo oge",
            "poem": 1
          },
          {
            "text": "ka m binyere ndị kpọrọ udo asị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Abụ m onye udo;",
            "poem": 1
          },
          {
            "text": "ma mgbe m kwuru okwu, okwu ha bụ okwu agha.",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": []
  },
