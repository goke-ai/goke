  "chapter": {
    "number": 43,
    "content": [
      {
        "type": "heading",
        "content": [
          "Arịrịọ maka enyemaka"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "O Chineke, kpepụta m,",
            "poem": 1
          },
          {
            "text": "kpechitere m ikpe m",
            "poem": 2
          },
          {
            "text": "nʼebe mba na-amaghị Chineke nọ;",
            "poem": 2
          },
          {
            "text": "gbapụta m site nʼaka",
            "poem": 1
          },
          {
            "text": "ndị ajọ omume na ndị aghụghọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ị bụ Chineke, ebe m e wusiri ike.",
            "poem": 1
          },
          {
            "text": "Gịnị mere i ji jụ m?",
            "poem": 2
          },
          {
            "text": "Gịnị mere m ga-eji na-ejegharị nʼiru ụjụ,",
            "poem": 1
          },
          {
            "text": "nʼihi mmegbu nke onye iro?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Zipụ ìhè gị na eziokwu gị,",
            "poem": 1
          },
          {
            "text": "ka ha duo m;",
            "poem": 2
          },
          {
            "text": "ka ha kpọbata m nʼugwu nsọ gị,",
            "poem": 1
          },
          {
            "text": "bụ ebe i bi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Mgbe ahụ, aga m aga nʼebe ịchụ aja Chineke,",
            "poem": 1
          },
          {
            "text": "gakwuru Chineke, bụ ọṅụ m na obi ụtọ m.",
            "poem": 2
          },
          {
            "text": "Aga m eji ụbọ akwara too gị,",
            "poem": 1
          },
          {
            "text": "O Chineke, bụ Chineke m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "O mkpụrụobi m, gịnị mere i ji daa mba?",
            "poem": 1
          },
          {
            "text": "Gịnị mere i ji na-asụ ude nʼime m?",
            "poem": 2
          },
          {
            "text": "Nwee olileanya na Chineke,",
            "poem": 1
          },
          {
            "text": "nʼihi na m ka ga-eto ya ọzọ,",
            "poem": 2
          },
          {
            "text": "onye Nzọpụta m na Chineke m.",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": []
  },
