  "chapter": {
    "number": 32,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma Devid. Abụ Maskil."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Nkwupụta na mgbaghara mmehie"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Onye a gọziri agọzi ka mmadụ ahụ bụ,",
            "poem": 1
          },
          {
            "text": "onye a gbaghaara ajọ omume ya niile,",
            "poem": 2
          },
          {
            "text": "onye e kpuchikwaara mmehie ya niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ngọzị na-adịrị nwoke ahụ,",
            "poem": 1
          },
          {
            "text": "onye Onyenwe anyị na-adịghị agụnyere mmehie ya,",
            "poem": 2
          },
          {
            "text": "onye ahụ nke aghụghọ ọbụla na-adịghịkwa nʼime mmụọ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Mgbe m gbachiri nkịtị,",
            "poem": 1
          },
          {
            "text": "ọkpụkpụ m rere ure",
            "poem": 2
          },
          {
            "text": "site nʼịsụ ude m ogologo ụbọchị niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ehihie niile na abalị niile,",
            "poem": 1
          },
          {
            "text": "ị tara m ahụhụ,",
            "poem": 2
          },
          {
            "text": "ume dị m nʼobi gwụchara,",
            "poem": 1
          },
          {
            "text": "dịka mgbe okpomọkụ nke ọkọchị.",
            "poem": 2
          },
          {
            "noteId": 1607
          },
          {
            "text": "Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Mgbe ahụ, ekwupụtara m mmehie m niile nʼihu gị,",
            "poem": 1
          },
          {
            "text": "kwụsịkwa izo ha ezo.",
            "poem": 2
          },
          {
            "text": "Agwara m onwe m okwu sị,",
            "poem": 1
          },
          {
            "text": "“Aga m ekwupụta mmehie m niile nʼihu Onyenwe anyị.”",
            "poem": 2
          },
          {
            "text": "Ị gbaghakwaara m",
            "poem": 1
          },
          {
            "text": "ikpe ọmụma nke mmehie m niile. Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ya mere, ka ndị niile bụ ndị ezi omume kpọkuo gị nʼekpere",
            "poem": 1
          },
          {
            "text": "mgbe e nwere ike ịchọta gị;",
            "poem": 2
          },
          {
            "text": "nʼụzọ dị otu a, ọ bụrụ na uju mmiri abịa,",
            "poem": 1
          },
          {
            "text": "ọ gaghị erute ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Gị onwe gị bụụrụ m ebe nzuzo;",
            "poem": 1
          },
          {
            "text": "Ị ga-echebekwa m ka nsogbu ọbụla hapụ irute m nso.",
            "poem": 2
          },
          {
            "text": "Ọ bụkwa abụ nzọpụta ka i ji gbaa m gburugburu. Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Aga m eme ka ị nwee uche, zikwa gị ụzọ i kwesiri ịgbaso;",
            "poem": 1
          },
          {
            "text": "Aga m adụ gị ọdụ, anya m ga-adịkwasịkwa nʼahụ gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Adịkwala ka ịnyịnya maọbụ ịnyịnya ibu",
            "poem": 1
          },
          {
            "text": "nke na-enweghị akọnuche,",
            "poem": 2
          },
          {
            "text": "nke a na-aghaghị iji mkpụmkpụ igwe na eriri kechie ọnụ ya, ijide ya,",
            "poem": 1
          },
          {
            "text": "ma emeghị otu a, ọ gaghị abịakwute gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Ọtụtụ ihe mgbu na-abịakwasị ndị na-eme ihe ọjọọ,",
            "poem": 1
          },
          {
            "text": "ma ịhụnanya Onyenwe anyị nke na-enweghị ọgwụgwụ,",
            "poem": 2
          },
          {
            "text": "na-agba onye ahụ tụkwasịrị ya obi gburugburu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ṅụrianụ ọṅụ nʼime Onyenwe anyị, nweekwanụ obi ụtọ, unu ndị ezi omume;",
            "poem": 1
          },
          {
            "text": "bụọ abụ, unu ndị obi ha ziri ezi.",
            "poem": 2
          }
        ]
      }
    ],
