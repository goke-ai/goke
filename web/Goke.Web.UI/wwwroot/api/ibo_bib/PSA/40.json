  "chapter": {
    "number": 40,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma nke dịrị onyeisi abụ. Abụ Ọma Devid."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Abụ ekele"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Eji m ndidi chere Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "ọ tụgharịkwutere m, ọ nụkwaara akwa m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "O sitere nʼolulu na-amị amị,",
            "poem": 1
          },
          {
            "text": "nke apịtị na ụrọ juru, sepụta m;",
            "poem": 2
          },
          {
            "text": "o guzobere ụkwụ m abụọ nʼelu oke nkume,",
            "poem": 1
          },
          {
            "text": "nyekwa m ebe siri ike ka m guzo.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "O tinyere abụ ọhụrụ nʼọnụ m,",
            "poem": 1
          },
          {
            "text": "abụ otuto nye Chineke anyị.",
            "poem": 2
          },
          {
            "text": "Ọtụtụ ga-ahụ ya ma tụọ egwu,",
            "poem": 1
          },
          {
            "text": "ha ga-atụkwasị obi ha nʼime Onyenwe anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Onye a gọziri agọzi ka onye ahụ bụ,",
            "poem": 1
          },
          {
            "text": "onye na-eme Onyenwe anyị ebe ntụkwasị obi ya;",
            "poem": 2
          },
          {
            "text": "onye na-adịghị ele ndị mpako anya",
            "poem": 1
          },
          {
            "text": "maọbụ ndị na-atụgharị gbasoo arụsị.",
            "poem": 2
          },
          {
            "noteId": 1611
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "O Onyenwe anyị Chineke m,",
            "poem": 1
          },
          {
            "text": "ọrụ ebube ị rụrụ dị ọtụtụ,",
            "poem": 2
          },
          {
            "text": "ọ dịghị onye pụrụ ịgụkọta ihe niile",
            "poem": 2
          },
          {
            "text": "ị kwadooro maka inye anyị;",
            "poem": 1
          },
          {
            "text": "a sịkwarị na aga m ekwu ma kọsaa ihe banyere ha,",
            "poem": 2
          },
          {
            "text": "ha karịrị ihe ọnụ pụrụ ikwu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Aja na onyinye",
            "poem": 1
          },
          {
            "noteId": 1612
          },
          {
            "text": "abụghị ihe ị chọrọ",
            "poem": 1
          },
          {
            "text": "ma ị megheela ntị abụọ;",
            "poem": 2
          },
          {
            "text": "aja nsure ọkụ na aja a chụrụ maka mmehie abụghị ihe ị chọrọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Mgbe ahụ, ekwuru m sị, “Lee m, abịala m,",
            "poem": 1
          },
          {
            "text": "e dere banyere m nʼime akwụkwọ mpịakọta.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "O Chineke m, ọ bụ ọchịchọ m ime ihe ị na-achọ,",
            "poem": 1
          },
          {
            "text": "iwu gị dị nʼime obi m.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Ana m anọ nʼetiti ọgbakọ ukwuu na-ekwusa ihe ziri ezi;",
            "poem": 1
          },
          {
            "text": "dịka ị maara, o Onyenwe anyị,",
            "poem": 2
          },
          {
            "text": "anaghị m emechi ọnụ m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Ekpuchighị m ezi omume gị nʼime obi m,",
            "poem": 1
          },
          {
            "text": "ana m ekwupụta maka ikwesi ntụkwasị obi gị na nzọpụta gị.",
            "poem": 2
          },
          {
            "text": "Adịghị m ekpuchi ịhụnanya na eziokwu gị,",
            "poem": 1
          },
          {
            "text": "nʼebe ọgbakọ ukwuu nọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "O Onyenwe anyị, ezonarịla m ebere gị,",
            "poem": 1
          },
          {
            "text": "ka ịhụnanya gị na eziokwu gị chebe m oge niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Nʼihi na ọtụtụ nsogbu a na-apụghị ịgụta ọnụ gbara m gburugburu;",
            "poem": 1
          },
          {
            "text": "mmehie m niile akarịala m, apụghịkwa m ịhụ ụzọ:",
            "poem": 2
          },
          {
            "text": "ha karịrị agịrị isi dị m nʼisi,",
            "poem": 1
          },
          {
            "text": "obi m na-adakwa mba nʼime m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "O Onyenwe anyị, ya masị gị ịzọpụta m;",
            "poem": 1
          },
          {
            "text": "O Onyenwe anyị, bịa ngwangwa inyere m aka.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Ka ndị niile na-achọ ịnapụ m ndụ m,",
            "poem": 1
          },
          {
            "text": "bụrụ ndị a ga-etinye nʼọnọdụ ihere na ọgbaaghara;",
            "poem": 2
          },
          {
            "text": "ka ndị niile ọ na-amasị ịhụ ịla nʼiyi m,",
            "poem": 1
          },
          {
            "text": "laghachi azụ nʼihere.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Ka ndị ahụ na-asị m,",
            "poem": 1
          },
          {
            "text": "“Ehee! Ehee!” kụja nʼihi ihere onwe ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Ma ka ndị niile na-achọ gị",
            "poem": 1
          },
          {
            "text": "ṅụrịa ọṅụ, nweekwa obi ụtọ nʼime gị;",
            "poem": 2
          },
          {
            "text": "ka ndị ahụ hụrụ nzọpụta gị nʼanya na-ekwu mgbe niile,",
            "poem": 1
          },
          {
            "text": "“Onyenwe anyị dị ukwuu.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Ma mụ onwe m bụ m ogbenye na onye nọ na mkpa;",
            "poem": 1
          },
          {
            "text": "ka Onyenwe anyị cheta m.",
            "poem": 2
          },
          {
            "text": "Ị bụ onye inyeaka m na onye mgbapụta m;",
            "poem": 1
          },
          {
            "text": "Ị bụ Chineke m, anọla ọdụ ịbịa.",
            "poem": 2
          }
        ]
      }
    ],
