  "chapter": {
    "number": 71,
    "content": [
      {
        "type": "heading",
        "content": [
          "Ekpere maka ndụ ogologo"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Onyenwe anyị, nʼime gị ka m gbabara izere ndụ;",
            "poem": 1
          },
          {
            "text": "ekwela ka ihere mee m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Site nʼezi omume gị, gbapụta m, mee ka m nwere onwe m;",
            "poem": 1
          },
          {
            "text": "tọọ ntị gị nʼala nụrụ olu m ma zọpụta m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ghọọrọ m oke nkume nke ebe mgbaba,",
            "poem": 1
          },
          {
            "text": "ebe m nwere ike ịgbaba mgbe ọbụla;",
            "poem": 2
          },
          {
            "text": "nye iwu maka nzọpụta m,",
            "poem": 1
          },
          {
            "text": "nʼihi na ị bụ oke nkume m na ebe m e wusiri ike.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "O Chineke m, site nʼaka ndị ajọ omume na ndị mmadụ na-eme ihe ọjọọ,",
            "poem": 1
          },
          {
            "text": "ndị afọ tara mmiri, gbapụta m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Nʼihi na gị onwe gị bụrịị olileanya m, Onyenwe m Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "ntụkwasị obi m site na mgbe m bụ nwantakịrị.",
            "poem": 2
          },
          {
            "noteId": 1642
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ọ bụ gị ka m tụkwasịrị obi, site nʼoge amụrụ m;",
            "poem": 1
          },
          {
            "text": "gị onwe gị wepụtara m site nʼafọ nne m.",
            "poem": 2
          },
          {
            "text": "Aga m anọgide na-eto gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Adịla m ka ihe ama nye ọtụtụ mmadụ,",
            "poem": 1
          },
          {
            "text": "ma ị bụ ebe mgbaba m siri ike.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ọnụ m jupụtara nʼotuto gị,",
            "poem": 1
          },
          {
            "text": "ọ na-ekwupụta ịma mma gị ogologo ụbọchị niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Agbakụtala m azụ mgbe m mere agadi;",
            "poem": 1
          },
          {
            "text": "ahapụkwala m mgbe ike adịghịkwa m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Nʼihi na ndị iro m na-ekwu okwu megide m;",
            "poem": 1
          },
          {
            "text": "ndị ahụ na-achọ igbu m na-agbarịta izu nzuzo.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ha na-asị, “Chineke agbakụtala ya azụ;",
            "poem": 1
          },
          {
            "text": "chụọ ya ọsọ, jidekwa ya,",
            "poem": 2
          },
          {
            "text": "nʼihi na o nweghị onye ọbụla ga-agbapụta ya.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "O Chineke, anọla m ebe dị anya;",
            "poem": 1
          },
          {
            "text": "O Chineke m, bịa ngwangwa nyere m aka.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Ka e were ihere kpuchie ndị na-ebo m ebubo ụgha;",
            "poem": 1
          },
          {
            "text": "ka ihere na nleda anya bụrụ ihe ga-ekpuchi",
            "poem": 2
          },
          {
            "text": "ndị ahụ na-achọ imerụ m ahụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Ma mụ onwe m ga-anọgide na-enwe olileanya mgbe niile;",
            "poem": 1
          },
          {
            "text": "aga m eto gị mgbe niile karịa otu m toro gị na mbụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Ọnụ m ga-ekwu banyere ezi omume gị",
            "poem": 1
          },
          {
            "text": "na nzọpụta gị ogologo ụbọchị niile,",
            "poem": 2
          },
          {
            "text": "ọ bụ ezie na-amaghị m ọtụtụ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Aga m abịa kwusaakwa ịdị ukwu ọrụ gị niile, Onyenwe m Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "aga m ekwusa ezi omume gị, e, naanị nke gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "O Chineke, site na mgbe m bụ nwantakịrị, i kuzielera m ihe,",
            "poem": 1
          },
          {
            "text": "ruo taa, ana m ekwupụta banyere ọrụ ebube gị niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "O Chineke m, ọ bụladị mgbe m mere agadi,",
            "poem": 1
          },
          {
            "text": "mgbe isi awọ juru m isi, ahapụla m,",
            "poem": 2
          },
          {
            "text": "tutu ruo mgbe m ga-ekwupụta ike gị nye ọgbọ nke na-abịa,",
            "poem": 1
          },
          {
            "text": "na ịdị ike gị nye ndị niile na-abịa nʼoge dị nʼihu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "O Chineke, ezi omume gị na-eru nʼeluigwe,",
            "poem": 1
          },
          {
            "text": "gị onye merela ọtụtụ ihe dị ukwu.",
            "poem": 2
          },
          {
            "text": "O Chineke, onye dị ka gị?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Nʼagbanyeghị na i meela ka m gabiga ọtụtụ nsogbu",
            "poem": 1
          },
          {
            "text": "nke dị ilu,",
            "poem": 2
          },
          {
            "text": "ị ga-atụte ndụ m ọzọ;",
            "poem": 2
          },
          {
            "text": "site nʼebe dị omimi nke ụwa,",
            "poem": 1
          },
          {
            "text": "ị ga-adọkpụpụta m ọzọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Ị ga-eme m ka m dịrị ukwuu karịa ka m dị na mbụ.",
            "poem": 1
          },
          {
            "text": "Ị ga-akasịkwa m obi ọzọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "O Chineke m, aga m eji ụbọ akwara too gị,",
            "poem": 1
          },
          {
            "text": "nʼihi ikwesi ntụkwasị obi gị;",
            "poem": 2
          },
          {
            "text": "aga m eji une bụkuo gị abụ otuto,",
            "poem": 1
          },
          {
            "text": "gị Onye nsọ nke Izrel.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 23,
        "content": [
          {
            "text": "Egbugbere ọnụ m ga-eti mkpu ọṅụ",
            "poem": 1
          },
          {
            "text": "mgbe m na-abụku gị abụ otuto,",
            "poem": 2
          },
          {
            "text": "mụ, onye ị gbapụtara.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 24,
        "content": [
          {
            "text": "Ogologo ụbọchị niile, aga m ekwupụta",
            "poem": 1
          },
          {
            "text": "ọrụ ezi omume gị niile,",
            "poem": 2
          },
          {
            "text": "nʼihi na ndị ahụ chọrọ imerụ m ahụ,",
            "poem": 1
          },
          {
            "text": "ka e tinyere nʼọnọdụ ihere na ọgbaaghara.",
            "poem": 2
          }
        ]
      }
    ],
