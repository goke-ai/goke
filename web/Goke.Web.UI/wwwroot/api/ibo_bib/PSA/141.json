  "chapter": {
    "number": 141,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma Devid."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Arịrịọ nke ezi mmadụ"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "O Onyenwe anyị, ana m akpọku gị; bịakwute m ọsịịsọ.",
            "poem": 1
          },
          {
            "text": "Nụrụ olu m mgbe m na-akpọku gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ka ekpere m bịa nʼihu gị dịka ihe na-esi isi ụtọ;",
            "poem": 1
          },
          {
            "text": "ka nchili elu nke m chịlịri aka m abụọ dịrị ka aja a na-achụ nʼanyasị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Nyere m aka ka m mechie ọnụ m, o Onyenwe anyị;",
            "poem": 1
          },
          {
            "text": "na-echekwa egbugbere ọnụ m nche.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ekwela ka obi m nwee mmasị nʼihe ọjọọ,",
            "poem": 1
          },
          {
            "text": "isonye ndị na-eme ihe ọjọọ",
            "poem": 2
          },
          {
            "text": "rụọ ọrụ ajọ omume niile dị iche iche;",
            "poem": 1
          },
          {
            "text": "ekwela ka m soro ha rikọọ ihe ụtọ ha niile.",
            "poem": 2
          },
          {
            "noteId": 1709
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ka onye ezi omume tie m ihe, nke a na-egosi obi ebere;",
            "poem": 1
          },
          {
            "text": "ka ọ baara m mba, nke a bụ mmanụ nʼelu isi m.",
            "poem": 2
          },
          {
            "text": "Isi m agaghị ajụ ya.",
            "poem": 1
          },
          {
            "text": "Otu ọ dị, ekpere m na-emegide ọrụ niile nke ndị na-eme ihe ọjọọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "A ga-esite nʼelu nkume dị elu tụda ndị na-achị ha,",
            "poem": 1
          },
          {
            "text": "mgbe ahụ, ndị ajọ omume ga-amata na okwu m niile bụ eziokwu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ha ga-ekwu sị, “Dịka mmadụ na-egburi, na-akọgharị ala,",
            "poem": 1
          },
          {
            "text": "otu a ka a ghasasịrị ọkpụkpụ anyị nʼọnụ ili.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ma olileanya m dị na gị, Onye kachasị ihe niile elu, bụ Onyenwe anyị",
            "poem": 1
          },
          {
            "text": "nʼime gị ka m gbabara izere ndụ, ararala m nyefee nʼaka ọnwụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Napụta m ka m ghara ịdaba nʼime ọnya igbudu niile nke ndị na-eme ajọ ihe siiri,",
            "poem": 1
          },
          {
            "text": "sitekwa nʼọnya niile ha siiri m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Ka ndị ajọ omume daba nʼime ụgbụ ha,",
            "poem": 1
          },
          {
            "text": "ma mụ onwe m ga-agabiga na-emerụghị ahụ ọbụla.",
            "poem": 2
          }
        ]
      }
    ],
