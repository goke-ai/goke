  "chapter": {
    "number": 122,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ nrigo, nke Devid."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Ekpere maka udo Jerusalem"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Aṅụrịrị m ọṅụ mgbe ha sị m,",
            "poem": 1
          },
          {
            "text": "“Ka anyị gaa nʼụlọnsọ Onyenwe anyị.”",
            "poem": 2
          },
          {
            "noteId": 1690
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ụkwụ anyị eguzola ugbu a",
            "poem": 1
          },
          {
            "text": "nʼọnụ ụzọ ama gị niile, gị obodo Jerusalem.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "E wuru Jerusalem dịka obodo",
            "poem": 1
          },
          {
            "text": "a chịkọtara ọnụ nke ọma.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ọ bụ ebe ahụ ka ebo niile na-arịgo,",
            "poem": 1
          },
          {
            "text": "bụ ebo niile nke Onyenwe anyị,",
            "poem": 2
          },
          {
            "text": "ịbụku aha Onyenwe anyị abụ",
            "poem": 1
          },
          {
            "text": "dịka iwu o nyere ndị Izrel si dị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Nʼebe ahụ ka ocheeze niile nke ikpe ikpe dị,",
            "poem": 1
          },
          {
            "text": "bụ ocheeze niile nke ezinaụlọ Devid.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Kpeenụ ekpere maka udo Jerusalem:",
            "poem": 1
          },
          {
            "text": "“Ka ndị niile hụrụ gị nʼanya nọọ nʼudo.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ka udo dịrị nʼime mgbidi gị niile",
            "poem": 1
          },
          {
            "text": "nchekwa dịrịkwa nʼime ebe nche gị niile e wusiri ike.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Nʼihi ụmụnne m na ndị enyi m,",
            "poem": 1
          },
          {
            "text": "ana m ekwu okwu sị, “Udo dịrị gị.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Nʼihi ụlọ Onyenwe anyị, Chineke anyị,",
            "poem": 1
          },
          {
            "text": "aga m achọ ọganihu gị.",
            "poem": 2
          }
        ]
      }
    ],
