  "chapter": {
    "number": 118,
    "content": [
      {
        "type": "heading",
        "content": [
          "Kelee Onyenwe anyị"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Keleenụ Onyenwe anyị nʼihi na ọ dị mma;",
            "poem": 1
          },
          {
            "text": "ịhụnanya ya na-adịgide ruo mgbe ebighị ebi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ka ụmụ Izrel kwupụta nʼonwe ha sị,",
            "poem": 1
          },
          {
            "text": "“Ịhụnanya ya na-adịgide ruo mgbe ebighị ebi.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ka ndị ezinaụlọ Erọn kwuokwa sị,",
            "poem": 1
          },
          {
            "text": "“Ịhụnanya ya na-adịgide ruo mgbe ebighị ebi.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ka ndị niile na-atụ egwu Onyenwe anyị kwuo sị,",
            "poem": 1
          },
          {
            "text": "“Ịhụnanya ya na-adịgide ruo mgbe ebighị ebi.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Nʼọnọdụ oke ihe mgbu m, akpọkuru m Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "ọ zara m, mee ka m nwere onwe m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Onyenwe anyị nọnyeere m, agaghị m atụ egwu.",
            "poem": 1
          },
          {
            "text": "Gịnị ka mmadụ pụrụ ime m?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Onyenwe anyị nọnyere m; ọ bụ onye na-enyere m aka.",
            "poem": 1
          },
          {
            "text": "Aga m ele ndị iro m anya na mmeri.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ọ kachasị mma ịgbaba nʼime Onyenwe anyị",
            "poem": 1
          },
          {
            "text": "karịa ịtụkwasị ụmụ mmadụ obi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Ọ kachasị mma ịgbaba nʼime Onyenwe anyị",
            "poem": 1
          },
          {
            "text": "karịa ịtụkwasị ndị amara aha ha obi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Mba niile gbara m gburugburu",
            "poem": 1
          },
          {
            "text": "ma site nʼaha Onyenwe anyị aga m ala ha nʼiyi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ha gbara m gburugburu,",
            "poem": 1
          },
          {
            "text": "ma site nʼaha Onyenwe anyị ka m na-egbuda ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ha gbara m gburugburu dịka aṅụ,",
            "poem": 1
          },
          {
            "text": "ma e menyụọla ha dịka ogwu na-ere ọkụ;",
            "poem": 2
          },
          {
            "text": "site nʼaha Onyenwe anyị, ka m na-egbuda ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Akwaghachiri m azụ, agaara m ada,",
            "poem": 1
          },
          {
            "text": "ma Onyenwe anyị nyere m aka.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Onyenwe anyị bụ ike m na abụ m;",
            "poem": 1
          },
          {
            "text": "ọ ghọọla nzọpụta m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Olu iti mkpu ọṅụ na mmeri",
            "poem": 1
          },
          {
            "text": "na-ada nʼime ụlọ ikwu ndị ezi omume:",
            "poem": 2
          },
          {
            "text": "“Aka nri Onyenwe anyị emeela ọtụtụ ihe dị ukwuu!",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Aka nri Onyenwe anyị bụ ihe e weliri elu;",
            "poem": 2
          },
          {
            "text": "aka nri Onyenwe anyị emeela ọtụtụ ihe dị ukwuu!”",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Agaghị m anwụ,",
            "poem": 1
          },
          {
            "text": "kama aga m adị ndụ kwusaara ndị ọzọ ọrụ ebube Onyenwe anyị rụrụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Onyenwe anyị tara m ahụhụ nke ukwuu,",
            "poem": 1
          },
          {
            "text": "ma o nyefeeghị m nʼaka ọnwụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Megheere m ọnụ ụzọ ama nke ezi omume;",
            "poem": 1
          },
          {
            "text": "aga m abanye nye Onyenwe anyị ekele.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Nke a bụ ọnụ ụzọ ụlọnsọ Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "ebe ndị ezi omume ga-esite na ya banye.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Aga m enye gị ekele, nʼihi na ị zara m;",
            "poem": 1
          },
          {
            "text": "ị ghọọla nzọpụta m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "Nkume ahụ ndị na-ewu ụlọ jụrụ,",
            "poem": 1
          },
          {
            "text": "aghọọla nkume isi ntọala",
            "poem": 2
          },
          {
            "noteId": 1683
          },
          {
            "text": "ụlọ;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 23,
        "content": [
          {
            "text": "nke a bụ ọrụ aka Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "ọ bụkwa ihe ịtụnanya nʼanya anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 24,
        "content": [
          {
            "text": "Nke a bụ ụbọchị Onyenwe anyị anyị mere,",
            "poem": 1
          },
          {
            "text": "ka anyị ṅụrịa ọṅụ, nweekwa obi ụtọ nʼime ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 25,
        "content": [
          {
            "text": "O Onyenwe anyị, zọpụta anyị;",
            "poem": 1
          },
          {
            "text": "o Onyenwe anyị, mee ka ihe gaara anyị nke ọma.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 26,
        "content": [
          {
            "text": "Onye a gọziri agọzi ka onye ahụ bụ, nke na-abịa nʼaha Onyenwe anyị.",
            "poem": 1
          },
          {
            "text": "Anyị sitere nʼụlọ Onyenwe anyị na-agọzi gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 27,
        "content": [
          {
            "text": "Onyenwe anyị bụ Chineke,",
            "poem": 1
          },
          {
            "text": "ọ bụ ya mere ka ìhè ya mụkwasị anyị.",
            "poem": 2
          },
          {
            "text": "Chịrị igu nkwụ nʼaka gị, soro ndị ji ọṅụ bata nʼogige Chineke",
            "poem": 1
          },
          {
            "text": "bịaruo nʼebe ịchụ aja.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 28,
        "content": [
          {
            "text": "Ị bụ Chineke m, aga m eto gị;",
            "poem": 1
          },
          {
            "text": "ị bụ Chineke m, aga m ebuli gị elu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 29,
        "content": [
          {
            "text": "Keleenụ Onyenwe anyị nʼihi na ọ dị mma,",
            "poem": 1
          },
          {
            "text": "ịhụnanya ya na-adịgide ruo mgbe ebighị ebi.",
            "poem": 2
          }
        ]
      }
    ],
