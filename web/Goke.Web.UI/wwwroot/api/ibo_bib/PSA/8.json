  "chapter": {
    "number": 8,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma dịrị onyeisi abụ. Abụ Ọma Devid nʼusoro gittit."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Ịdị ebube Chineke na nsọpụrụ nke mmadụ"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "O Onyenwe anyị Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "lee ka ịdị ukwuu nke aha gị si jupụta ụwa niile!",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "I meela ka ebube gị dị elu",
            "poem": 1
          },
          {
            "text": "nʼime eluigwe.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Site nʼotuto",
            "poem": 1
          },
          {
            "noteId": 1578
          },
          {
            "text": "ụmụntakịrị na ụmụ na-aṅụ ara,",
            "poem": 1
          },
          {
            "text": "ị tọọla ntọala siri ike megide ndị iro gị,",
            "poem": 2
          },
          {
            "text": "ka e si otu a mechie ọnụ onye iro na onye ahụ na-abọ ọbọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Mgbe ọbụla m lere anya na mbara eluigwe,",
            "poem": 1
          },
          {
            "text": "hụ ọrụ niile nke aka gị rụrụ,",
            "poem": 2
          },
          {
            "text": "mgbe m lere anya nʼelu hụ ọnwa na kpakpando",
            "poem": 1
          },
          {
            "text": "i tinyere nʼọnọdụ ha,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "gịnị ka mmadụ bụ i ji na-eche banyere ha?",
            "poem": 1
          },
          {
            "text": "Gịnịkwa ka mmadụ bụ iji na-elekọta",
            "poem": 2
          },
          {
            "noteId": 1579
          },
          {
            "text": "ha?",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "I mere ka ha",
            "poem": 1
          },
          {
            "noteId": 1580
          },
          {
            "text": "dị ala nwantakịrị nʼebe ndị mmụọ ozi",
            "poem": 1
          },
          {
            "noteId": 1581
          },
          {
            "text": "nọ,",
            "poem": 1
          },
          {
            "text": "I jikwa ebube na nsọpụrụ kpube ya nʼisi dịka okpueze.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "I mekwaala ya ka ọ bụrụ onyeisi nke ihe niile i kere eke,",
            "poem": 1
          },
          {
            "text": "tinyekwa ihe niile nʼokpuru ụkwụ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ọ na-achị igwe ewu na atụrụ, na ehi,",
            "poem": 1
          },
          {
            "text": "na ụmụ anụ ọhịa ndị ọzọ niile,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "na nnụnụ niile,",
            "poem": 1
          },
          {
            "text": "na azụ niile,",
            "poem": 2
          },
          {
            "text": "na anụ ndị ọzọ na-ebi nʼime mmiri.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "O Onyenwe anyị, Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "lee ka ịdị ukwuu nke aha gị si jupụta ụwa niile.",
            "poem": 2
          }
        ]
      }
    ],
