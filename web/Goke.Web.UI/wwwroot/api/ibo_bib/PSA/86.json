  "chapter": {
    "number": 86,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Ekpere Devid."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Ekpere maka enyemaka"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Gee m ntị, O Onyenwe anyị, ma zakwa m,",
            "poem": 1
          },
          {
            "text": "nʼihi na abụ m ogbenye na onye nọ na mkpa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Chebe ndụ m, nʼihi na m na-erubere gị isi;",
            "poem": 1
          },
          {
            "text": "zọpụta ohu gị onye nke na-atụkwasị gị obi.",
            "poem": 2
          },
          {
            "text": "Ị bụ Chineke m;",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "meere m ebere, O Onyenwe m,",
            "poem": 1
          },
          {
            "text": "nʼihi na ana m akpọku gị ogologo ụbọchị niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Wetara ohu gị ọṅụ, nʼihi na ọ bụ gị, O Onyenwe m,",
            "poem": 1
          },
          {
            "text": "ka m na-eweliri mkpụrụobi m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ị bụ onye na-agbaghara ajọ omume na onye dị mma, O Onyenwe m,",
            "poem": 1
          },
          {
            "text": "i jupụtara nʼịhụnanya nye ndị niile na-akpọku gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "O Onyenwe anyị, nụrụ ekpere m;",
            "poem": 1
          },
          {
            "text": "gekwaa ntị nʼakwa arịrịọ m maka ebere.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Nʼụbọchị nsogbu m, aga m akpọku gị,",
            "poem": 1
          },
          {
            "text": "nʼihi na ị ga-aza m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Nʼetiti chi niile, i nweghị oyiri, O Onyenwe m;",
            "poem": 1
          },
          {
            "text": "ọ dịghị ọrụ ebube ọbụla a pụrụ ị sị na ọ dị ka nke ị na-arụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Mba niile i kere ga-abịa nʼihu gị,",
            "poem": 1
          },
          {
            "text": "fee gị ofufe, O Onyenwe anyị;",
            "poem": 2
          },
          {
            "text": "ha ga-ebutere aha gị ebube.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Nʼihi na ị dị ukwuu na-arụkwa ọrụ ebube dị iche iche;",
            "poem": 1
          },
          {
            "text": "ọ bụ naanị gị bụ Chineke.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Kuziere m ụzọ gị, O Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "ka m dabere nʼikwesị ntụkwasị obi gị;",
            "poem": 2
          },
          {
            "text": "nye m obi na-adịghị agba abụọ,",
            "poem": 1
          },
          {
            "text": "ka m nwee ike tụọ egwu aha gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Aga m eji obi m niile too gị, O Onyenwe anyị, na Chineke m;",
            "poem": 1
          },
          {
            "text": "aga m ebulikwa aha gị elu ruo mgbe niile ebighị ebi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Nʼihi na ịhụnanya gị nʼebe m nọ dị ukwuu;",
            "poem": 1
          },
          {
            "text": "i sitela nʼomimi nke dị ala",
            "poem": 2
          },
          {
            "text": "site nʼala ndị nwụrụ anwụ dọpụta m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "O Chineke, ndị mpako na-emegide m;",
            "poem": 1
          },
          {
            "text": "otù ndị mmadụ na-eme ihe ike na-achọ igbu m,",
            "poem": 2
          },
          {
            "text": "ha bụ ndị na-ejighị gị kpọrọ ihe ọbụla.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Ma gị, Onyenwe anyị, ị bụ Chineke onye ọmịiko na onye amara,",
            "poem": 1
          },
          {
            "text": "iwe adịghị ewe gị ngwangwa, i jupụtara nʼịhụnanya na ikwesi ntụkwasị obi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Chee m ihu, mekwaara m amara.",
            "poem": 1
          },
          {
            "text": "Nye ohu gị ike gị,",
            "poem": 2
          },
          {
            "text": "zọpụtakwa nwa ohu gị nwanyị",
            "poem": 1
          },
          {
            "text": "nʼihi na m na-ejere gị ozi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Gosi m ihe ịrịbama nke ga-egosipụta ịdị mma gị,",
            "poem": 1
          },
          {
            "text": "ka ndị iro m hụ ya si otu a bụrụ ndị ihere ga-eme,",
            "poem": 2
          },
          {
            "text": "nʼihi na gị, O Onyenwe anyị, nyere m aka ma kasịekwa m obi.",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": []
  },
