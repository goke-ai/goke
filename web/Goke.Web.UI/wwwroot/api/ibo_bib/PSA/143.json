  "chapter": {
    "number": 143,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma Devid."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Ekpere maka enyemaka"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "O Onyenwe anyị, nụrụ ekpere m,",
            "poem": 1
          },
          {
            "text": "gekwaa ntị nʼakwa arịrịọ m maka amara;",
            "poem": 2
          },
          {
            "text": "nʼihi ikwesi ntụkwasị obi gị na ezi omume gị,",
            "poem": 1
          },
          {
            "text": "bịa nyere m aka.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Edubatala m nʼọnọdụ ikpe m ikpe,",
            "poem": 1
          },
          {
            "text": "nʼihi na ọ dịghị onye ọbụla dị ndụ bụ onye ezi omume nʼihu gị.",
            "poem": 2
          },
          {
            "noteId": 1710
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Onye iro na-achụ m ọsọ,",
            "poem": 1
          },
          {
            "text": "ọ na-azọpịa ndụ m nʼala;",
            "poem": 2
          },
          {
            "text": "na-emekwa ka m nọdụ nʼọchịchịrị,",
            "poem": 1
          },
          {
            "text": "dịka ndị nwụrụ anwụ nʼogologo oge gara aga.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ya mere mmụọ m na-ada mba nʼime m",
            "poem": 1
          },
          {
            "text": "ụjọ jupụtakwara m nʼobi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ana m echeta ụbọchị niile ndị ahụ gara aga;",
            "poem": 1
          },
          {
            "text": "ana m atụgharị uche nʼọrụ gị niile",
            "poem": 2
          },
          {
            "text": "na-echekwa echiche banyere ihe niile aka gị rụrụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ana m agbasa aka m abụọ nye gị;",
            "poem": 1
          },
          {
            "text": "agụụ gị na-agụ mkpụrụobi m dịka agụụ mmiri ozuzo si agụ ala kpọrọ nkụ. Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "O Onyenwe anyị, zaa m ọsịịsọ;",
            "poem": 1
          },
          {
            "text": "mmụọ m na-ada mba.",
            "poem": 2
          },
          {
            "text": "Ezonarịla m ihu gị,",
            "poem": 1
          },
          {
            "text": "ma ọ bụghị ya, aga m adị ka ndị na-arịda ruo nʼili.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Mee ka m nụrụ okwu ịhụnanya gị nke na-adịghị agharịpụ nʼisi ụtụtụ,",
            "poem": 1
          },
          {
            "text": "nʼihi na ọ bụ gị ka m tụkwasịrị obi.",
            "poem": 2
          },
          {
            "text": "Zi m ụzọ m kwesiri ịgbaso,",
            "poem": 1
          },
          {
            "text": "nʼihi na ọ bụ gị ka m na-eweli mkpụrụobi m nye.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "O Onyenwe anyị, gbapụta m site nʼaka ndị iro m,",
            "poem": 1
          },
          {
            "text": "nʼihi na ọ bụ nʼime gị ka m na-ezo onwe m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Kuziere m ime uche gị,",
            "poem": 1
          },
          {
            "text": "nʼihi na ị bụ Chineke m;",
            "poem": 2
          },
          {
            "text": "ka ezi Mmụọ gị",
            "poem": 1
          },
          {
            "text": "duo m nʼala dị larịị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "O Onyenwe anyị, nʼihi aha gị chebe ndụ m;",
            "poem": 1
          },
          {
            "text": "nʼime ezi omume gị, napụta m site na nsogbu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Nʼime ịhụnanya gị na-adịghị agharịpụ, kpochapụ ndị iro m;",
            "poem": 1
          },
          {
            "text": "laa ndị niile kpọrọ m asị nʼiyi,",
            "poem": 2
          },
          {
            "text": "nʼihi na abụ m ohu gị.",
            "poem": 2
          }
        ]
      }
    ],
