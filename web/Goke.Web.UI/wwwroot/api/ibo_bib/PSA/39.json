  "chapter": {
    "number": 39,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma nke dịrị onyeisi abụ maka Jedutun. Abụ Ọma Devid."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Nkwupụta mmehie"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Asịrị m, “Aga m elezi anya nʼụzọ m niile",
            "poem": 1
          },
          {
            "text": "ma debe ire m ka m ghara ikwuhie",
            "poem": 2
          },
          {
            "noteId": 1610
          },
          {
            "text": ";",
            "poem": 2
          },
          {
            "text": "aga m eji ihe kechie ọnụ m",
            "poem": 1
          },
          {
            "text": "mgbe m nọ nʼetiti ndị ajọ omume.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ma mgbe m mechiri ọnụ m ma nọrọ duu,",
            "poem": 1
          },
          {
            "text": "jụ ikwu ọ bụladị ihe ọbụla dị mma,",
            "poem": 2
          },
          {
            "text": "oke ihe mgbu m mụbara.",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Obi m kporo ọkụ nʼime m,",
            "poem": 2
          },
          {
            "text": "ma mgbe m na-atụgharị ihe ndị a nʼuche, ọkụ ahụ nọ na-ere;",
            "poem": 1
          },
          {
            "text": "mgbe ahụ ekwuru m okwu sị:",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "“O Onyenwe anyị, gosi m ọgwụgwụ nke ndụ m",
            "poem": 1
          },
          {
            "text": "na ọnụọgụgụ ụbọchị m niile;",
            "poem": 2
          },
          {
            "text": "mee ka m mara na ndụ m na-agafe ngwangwa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "I meela ka ụbọchị m niile bụrụ naanị ihe dị nta;",
            "poem": 1
          },
          {
            "text": "ogologo afọ m niile adịghị ka ihe ọbụla nʼihu gị.",
            "poem": 2
          },
          {
            "text": "Ndụ onye ọbụla bụ naanị otu nkuume",
            "poem": 1
          },
          {
            "text": "ọbụladị ndị dịka ha guzosirike. Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "“Nʼezie onye ọbụla na-ejegharị dịka onyinyo",
            "poem": 1
          },
          {
            "text": "nʼefu ka ha na-ekwogharị nʼotu ebe na nke ọzọ, na-akpakọba akụ,",
            "poem": 2
          },
          {
            "text": "na-amaghị onye ọ ga-abụ nke ya nʼikpeazụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "“Ma ugbu a, Onyenwe m, gịnị ka m na-ele anya ya?",
            "poem": 1
          },
          {
            "text": "Olileanya m dị nʼime gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Zọpụta m site na mmehie m niile;",
            "poem": 1
          },
          {
            "text": "ekwela ka m bụrụ ihe ọchị nye ndị nzuzu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Agbachiri m nkịtị, ajụrụ m ikwu okwu,",
            "poem": 1
          },
          {
            "text": "nʼihi na ọ bụ gị mere ihe a.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Kwụsị iti m ihe;",
            "poem": 1
          },
          {
            "text": "ihe otiti nke aka gị ezuola m ahụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ị na-abara ndị mmadụ mba ma na-ata ha ahụhụ nʼihi mmehie ha;",
            "poem": 1
          },
          {
            "text": "ị na-eripịa akụnụba ha dịka nla,",
            "poem": 2
          },
          {
            "text": "mmadụ ọbụla bụ naanị otu nkuume. Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "“O Onyenwe anyị, nụrụ ekpere m,",
            "poem": 1
          },
          {
            "text": "gekwaa ntị nʼakwa m maka enyemaka;",
            "poem": 2
          },
          {
            "text": "emechila ntị gị nye ibe akwa m.",
            "poem": 2
          },
          {
            "text": "Nʼihi ana m ebinyere gị dịka onye ọbịa,",
            "poem": 1
          },
          {
            "text": "dịka onye ije, dịka nna nna m ha mere.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Lepụ anya na-ebe m nọ, ka m nwee ike kporie ndụ ọzọ,",
            "poem": 1
          },
          {
            "text": "tupu m hapụ ebe a laa, a gaghị ahụkwa m ọzọ.”",
            "poem": 2
          }
        ]
      }
    ],
