  "chapter": {
    "number": 133,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ nrigo, nke Devid."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Ngọzị nke ibi nʼotu"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Lee ka o si bụrụ ihe dị mma",
            "poem": 1
          },
          {
            "text": "na ihe ọṅụ mgbe ụmụnne na-ebikọ nʼotu!",
            "poem": 2
          },
          {
            "noteId": 1700
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ọ dị ka mmanụ dị oke ọnụahịa a wụrụ nʼisi,",
            "poem": 1
          },
          {
            "text": "nke na-asọdata ruo nʼahụọnụ,",
            "poem": 2
          },
          {
            "text": "e, nke na-asọdata ruo nʼahụọnụ Erọn,",
            "poem": 1
          },
          {
            "text": "ruokwa nʼolu uwe mwụda ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ọ dị ka a ga-asị na igirigi si na Hemon",
            "poem": 1
          },
          {
            "text": "na-ada nʼelu nʼugwu Zayọn.",
            "poem": 2
          },
          {
            "text": "Nʼihi na nʼebe ahụ ka Onyenwe anyị na-enye onyinye ngọzị ya,",
            "poem": 1
          },
          {
            "text": "ọ bụladị ndụ na-adị ruo mgbe ebighị ebi.",
            "poem": 2
          }
        ]
      }
    ],
