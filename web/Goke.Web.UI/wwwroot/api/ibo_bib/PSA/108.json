  "chapter": {
    "number": 108,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ, Abụ Ọma Devid."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Ekpere nʼoge nsogbu"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Obi m dị njikere, o Chineke;",
            "poem": 1
          },
          {
            "text": "aga m eji mkpụrụobi m niile bụọ abụ ma kụkwaa egwu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Teta! Ụbọ akwara na une!",
            "poem": 1
          },
          {
            "text": "Aga m akpọte chi ọbụbọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Aga m eto gị, O Onyenwe anyị, nʼetiti mba niile;",
            "poem": 1
          },
          {
            "text": "aga m abụ abụ banyere gị nʼetiti ndị niile dị iche iche.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Nʼihi na ịhụnanya gị dị ukwuu karịa eluigwe;",
            "poem": 1
          },
          {
            "text": "ikwesi ntụkwasị obi gị na-erukwa mbara eluigwe.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ka e bulie gị elu, o Chineke, karịa eluigwe niile,",
            "poem": 1
          },
          {
            "text": "ka ebube gị dị nʼelu ụwa niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Were aka nri gị zọpụta anyị ma nyere anyị aka,",
            "poem": 1
          },
          {
            "text": "ka a gbapụta ndị ị hụrụ nʼanya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Chineke esitela nʼịdị nsọ ya kwuo, sị,",
            "poem": 1
          },
          {
            "text": "“Nʼọnọdụ ọṅụ nke mmeri, aga m ekesa Shekem,",
            "poem": 2
          },
          {
            "text": "tụkwaa Ndagwurugwu Sukọt nʼihe ọtụtụ, nyepụ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Gilead bụ nke m, Manase bụkwa nke m;",
            "poem": 1
          },
          {
            "text": "Ifrem bụ okpu igwe m,",
            "poem": 2
          },
          {
            "text": "Juda bụ mkpara eze m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Moab bụ efere ịkwọ aka m,",
            "poem": 1
          },
          {
            "text": "aga m atụsa akpụkpọụkwụ m nʼelu Edọm",
            "poem": 2
          },
          {
            "text": "nʼelu isi Filistia, ana m eti mkpu mmeri.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Onye ga-akpọrọ m gaa nʼobodo e wusiri ike?",
            "poem": 1
          },
          {
            "text": "Onye ga-edu m gaa Edọm?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ọ bụghị gị, o Chineke? Gị onye jụrụ anyị,",
            "poem": 1
          },
          {
            "text": "onye na-adịghị esokwa ndị agha anyị aga agha?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Nyere anyị aka ka anyị pụọ nʼaka ndị iro,",
            "poem": 1
          },
          {
            "text": "nʼihi na inyeaka mmadụ bụ ihe efu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Site na Chineke, anyị ga-enwe mmeri,",
            "poem": 1
          },
          {
            "text": "ọ ga-azọpịa ndị iro anyị niile nʼala.",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": []
  },
