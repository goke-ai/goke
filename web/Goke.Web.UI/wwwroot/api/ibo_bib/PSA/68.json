  "chapter": {
    "number": 68,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma dịrị onyeisi abụ. Nke Devid. Abụ Ọma. Otu Abụ."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Chineke nke ụwa niile"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ka Chineke bilie, ka ndị iro ya bụrụ ndị a chụsasịrị;",
            "poem": 1
          },
          {
            "text": "ka ndị iro ya site nʼihu ya gbalaga.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Dịka ikuku si achụsa anwụrụ ọkụ,",
            "poem": 1
          },
          {
            "noteId": 1637
          },
          {
            "text": "sikwa otu a chụsaa ha.",
            "poem": 1
          },
          {
            "text": "Dịka mmanụ na-ehi ụra si agbaze nʼọkụ,",
            "poem": 2
          },
          {
            "text": "ka ndị na-eme ihe ọjọọ laa nʼiyi nʼihu Chineke.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ka ndị ezi omume bụrụ ndị nwere obi ụtọ,",
            "poem": 1
          },
          {
            "text": "ma ṅụrịa nʼihu Chineke;",
            "poem": 2
          },
          {
            "text": "ka ha bụrụ ndị nwere obi ụtọ, jupụtakwa nʼọṅụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Bụkuo Chineke abụ, bụkuonụ aha ya abụ otuto,",
            "poem": 1
          },
          {
            "text": "toonu onye na-agba igwe ojii dịka ịnyịnya,",
            "poem": 2
          },
          {
            "text": "Onyenwe anyị bụ aha ya, ṅụrịa ọṅụ nʼihu ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ọ bụ nna ndị nke na-enweghị nna, onye na-ekpechitere ndị di ha nwụrụ anwụ ọnụ ha,",
            "poem": 1
          },
          {
            "text": "onye a bụ Chineke, onye bi nʼebe ya dị nsọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Chineke na-eme ka ndị naanị ha nọ nwee ezinaụlọ,",
            "poem": 1
          },
          {
            "text": "ọ na-eji ịbụ abụ dupụta ndị nọ nʼụlọ mkpọrọ,",
            "poem": 2
          },
          {
            "text": "ma ndị nnupu isi ka ọ na-eme ka ha biri, nʼala kpọrọ nkụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Mgbe i buteere ndị gị ụzọ, O Chineke,",
            "poem": 1
          },
          {
            "text": "mgbe i gabigara nʼala tọgbọrọ nʼefu, Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "elu ụwa mara jijiji, eluigwe wụpụrụ mmiri ozuzo,",
            "poem": 1
          },
          {
            "text": "nʼihu Chineke, Onye Saịnaị,",
            "poem": 2
          },
          {
            "text": "nʼihu Chineke, Chineke nke Izrel.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "I nyere mmiri ozuzo nʼebe ọ dị ukwuu, O Chineke,",
            "poem": 1
          },
          {
            "text": "i tutere ihe nketa gị nke ike gwụrụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Ndị gị biri nʼime ya,",
            "poem": 1
          },
          {
            "text": "site nʼoke akụnụba gị, O Chineke, ị rọpụtaara ndị ogbenye mkpa ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Onyenwe anyị kpọsara okwu ahụ,",
            "poem": 1
          },
          {
            "text": "ọnụọgụgụ ndị inyom kwusara ya bụ igwe dị ọtụtụ;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "ndị eze na ndị agha ha ji ọsọ gbalaa;",
            "poem": 1
          },
          {
            "text": "ndị mmadụ kere ihe a dọtara nʼagha, nʼime ọmụma ụlọ ikwu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Ọ bụladị mgbe ị na-arahụ ụra nʼetiti ọgba atụrụ,",
            "poem": 1
          },
          {
            "text": "nku abụọ nke nduru m bụ ihe e ji ọlaọcha kpuchie,",
            "poem": 2
          },
          {
            "text": "jirikwa ọlaedo nke na-enwuke kpuchie abụba ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Mgbe Onye pụrụ ime ihe niile chụsasịrị ndị eze bi nʼala a,",
            "poem": 1
          },
          {
            "text": "ọ dị ka snoo dara nʼelu Zalmon.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Ugwu Bashan, ugwu mara mma nke ukwuu,",
            "poem": 1
          },
          {
            "text": "ugwu Bashan, ugwu nwere nʼelu ya ọtụtụ ugwu dị iche iche.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Gị ugwu nwere ọtụtụ ugwu, gịnị mere i ji ele anya site nʼekworo",
            "poem": 1
          },
          {
            "text": "nʼugwu Chineke họọrọ ị bụ ebe ọ ga-anọ chịa,",
            "poem": 2
          },
          {
            "text": "ebe Onyenwe anyị nʼonwe ya ga-ebi, ruo ebighị ebi?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Ọnụọgụgụ ụgbọ agha Chineke dị iri puku kwụrụ puku, a pụghị ịgụta ha;",
            "poem": 1
          },
          {
            "text": "ha dịkwa puku kwụrụ puku;",
            "poem": 2
          },
          {
            "text": "Onyenwe anyị sitere Saịnaị bata nʼụlọnsọ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Mgbe i rigoro gaa nʼelu,",
            "poem": 1
          },
          {
            "noteId": 1638
          },
          {
            "text": "i duuru ndị a dọtara nʼagha",
            "poem": 2
          },
          {
            "noteId": 1639
          },
          {
            "text": ";",
            "poem": 2
          },
          {
            "text": "ị natara onyinye dị iche iche nʼaka ndị mmadụ,",
            "poem": 2
          },
          {
            "text": "ọ bụladị nʼaka ndị na-enupu isi,",
            "poem": 1
          },
          {
            "text": "ka gị, O Onyenwe anyị Chineke, biri nʼebe ahụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Otuto dịrị Onyenwe anyị, Chineke anyị, na Onye Nzọpụta anyị,",
            "poem": 1
          },
          {
            "text": "Onye na-ebu ibu arọ anyị ụbọchị niile. Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Chineke anyị bụ Chineke na-azọpụta;",
            "poem": 1
          },
          {
            "text": "mgbapụta pụọ nʼọnwụ na-esite nʼaka Onye kachasị ihe niile elu, bụ Onyenwe anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Nʼezie, Chineke ga-egwepịa isi ndị iro ya niile,",
            "poem": 1
          },
          {
            "text": "isi ndị niile na-aga nʼihu ibi ndụ mmehie.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "Onyenwe anyị kwuru sị, “Aga m esite na Bashan kpọlata ha,",
            "poem": 1
          },
          {
            "text": "aga m adọpụta ha site nʼebe omimi nke osimiri,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 23,
        "content": [
          {
            "text": "ka unu nwee ike saa ụkwụ unu nʼime ọbara ndị iro unu,",
            "poem": 1
          },
          {
            "text": "ka ire nkịta unu niile ketakwa oke nke ha.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 24,
        "content": [
          {
            "text": "O Chineke, ahụla igwe ahịrị mmadụ ndị na-eso gị,",
            "poem": 1
          },
          {
            "text": "bụ igwe ahịrị mmadụ nke so Chineke m, na Eze m na-aba nʼebe nsọ ahụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 25,
        "content": [
          {
            "text": "Ndị na-abụ abụ nọ nʼihu, ndị na-eti egwu, na-eso ha nʼazụ;",
            "poem": 1
          },
          {
            "text": "ụmụ agbọghọ na-akpọ ụbọ sokwa ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 26,
        "content": [
          {
            "text": "Too Chineke nʼetiti nzukọ ukwuu ahụ;",
            "poem": 1
          },
          {
            "text": "toonu Onyenwe anyị nʼetiti nzukọ ụmụ Izrel.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 27,
        "content": [
          {
            "text": "Lee Benjamin bụ agbụrụ dịkarịsịrị nta, ka ọ na-edu ha,",
            "poem": 1
          },
          {
            "text": "lekwaa igwe ụmụ ndị eze Juda",
            "poem": 2
          },
          {
            "text": "na nke Zebụlọn na Naftalị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 28,
        "content": [
          {
            "text": "O Chineke, kpọlite ike gị;",
            "poem": 1
          },
          {
            "text": "mee ka ike gị pụta ìhè, O Chineke, dịka i mere na mbụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 29,
        "content": [
          {
            "text": "Nʼihi ụlọnsọ ukwu gị nke dị na Jerusalem,",
            "poem": 1
          },
          {
            "text": "ndị eze ga-ewetara gị onyinye dị iche iche.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 30,
        "content": [
          {
            "text": "Baara anụ ọhịa ahụ zoro nʼetiti riidi mba,",
            "poem": 1
          },
          {
            "text": "igwe oke ehi nọ nʼetiti ụmụ ehi nke mba niile.",
            "poem": 2
          },
          {
            "text": "Site na nweda nʼala, ka o wetara gị ogbe ọlaọcha.",
            "poem": 1
          },
          {
            "text": "Chụsasịa mba niile, bụ ndị na-enwe mmasị nʼibu agha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 31,
        "content": [
          {
            "text": "Ndị nnọchi anya obodo, ga-esite nʼIjipt bịa,",
            "poem": 1
          },
          {
            "text": "Kush ga-etinye onwe ya nʼokpuru Chineke.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 32,
        "content": [
          {
            "text": "Bụkuo Chineke abụ, unu alaeze niile nke ụwa,",
            "poem": 1
          },
          {
            "text": "bụkuo Onyenwe anyị abụ otuto, Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 33,
        "content": [
          {
            "text": "Bụọ abụ nye onye na-agba ịnyịnya nʼeluigwe, e, ọ bụladị eluigwe nke dịrị site na mgbe ochie,",
            "poem": 1
          },
          {
            "text": "onye olu ike ya dịka egbe eluigwe.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 34,
        "content": [
          {
            "text": "Kwusaa ike Chineke,",
            "poem": 1
          },
          {
            "text": "onye ịdị ukwuu nke ịma mma ya na-ekpuchi Izrel,",
            "poem": 2
          },
          {
            "text": "onye ike ya dị nʼeluigwe.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 35,
        "content": [
          {
            "text": "O Chineke, ị dị oke egwu nʼime ụlọnsọ gị;",
            "poem": 1
          },
          {
            "text": "Chineke nke Izrel na-enye ndị ya ume na ike.",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Toonu Chineke!",
            "poem": 1
          }
        ]
      }
    ],
