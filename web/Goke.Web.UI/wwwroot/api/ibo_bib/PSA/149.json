  "chapter": {
    "number": 149,
    "content": [
      {
        "type": "heading",
        "content": [
          "Abụ otuto"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Toonu Onyenwe anyị!",
            "poem": 1
          },
          {
            "lineBreak": true
          },
          {
            "text": "Bụkuonụ Onyenwe anyị abụ ọhụrụ.",
            "poem": 1
          },
          {
            "text": "Bụọnụ abụ ito ya nʼetiti ndị nsọ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ka Izrel ṅụrịa ọṅụ nʼime onye kere ha;",
            "poem": 1
          },
          {
            "text": "ka ndị bi na Zayọn nwee obi ụtọ nʼime Eze",
            "poem": 2
          },
          {
            "noteId": 1715
          },
          {
            "text": "ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ka ha jiri ite egwu too aha ya",
            "poem": 1
          },
          {
            "text": "ka ha jiri ọyọ na ụbọ akwara kpọọrọ ya egwu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Nʼihi na ihe banyere ndị ya na-atọ Onyenwe anyị ụtọ.",
            "poem": 1
          },
          {
            "text": "Ọ na-eji nzọpụta",
            "poem": 2
          },
          {
            "noteId": 1716
          },
          {
            "text": "kpukwasị ndị dị umeala nʼobi nʼisi dịka okpueze.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ka ndị nsọ ṅụrịa ọṅụ nʼime nsọpụrụ a,",
            "poem": 1
          },
          {
            "text": "bụkwaa abụ nʼihi ọṅụ nʼelu ihe ndina ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ka otuto Chineke dịrị nʼọnụ ha,",
            "poem": 1
          },
          {
            "text": "ka mma agha ihu abụọ",
            "poem": 2
          },
          {
            "noteId": 1717
          },
          {
            "text": "dịrịkwa nʼaka ha,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "iji bọọrọ mba niile ọbọ",
            "poem": 1
          },
          {
            "text": "taakwa ndị niile dị iche iche ahụhụ,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "iji mkpọrọ igwe kee ndị eze ha",
            "poem": 1
          },
          {
            "text": "na ndị ha a na-asọpụrụ agbụ igwe,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "imezu ihe ahụ e kpebiri, depụtakwa megide ha.",
            "poem": 1
          },
          {
            "text": "Nke a bụ nsọpụrụ dịrị ndị nsọ ya.",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Toonu Onyenwe anyị.",
            "poem": 1
          }
        ]
      }
    ],
