  "chapter": {
    "number": 24,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma Devid."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Eze onyenwe ebube"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ụwa na ihe niile dị nʼime ya bụ nke Onyenwe anyị;",
            "poem": 1
          },
          {
            "text": "ụwa na ndị niile bi nʼime ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ọ tọrọ ntọala ya nʼime oke osimiri niile,",
            "poem": 1
          },
          {
            "text": "o mekwara ka o guzosie ike.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Onye bụ onye ahụ nwere ike irigo nʼelu ugwu Onyenwe anyị?",
            "poem": 1
          },
          {
            "text": "Onye nwere ike ịbata guzo nʼime ụlọnsọ ya?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ọ bụ onye aka ya emeghị ihe ọjọọ, obi ya dịkwa ọcha,",
            "poem": 1
          },
          {
            "text": "onye na-adịghị atụkwasị obi na chi arụsị,",
            "poem": 2
          },
          {
            "text": "maọbụ ṅụọ iyi aghụghọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ha ga-anata ngọzị site nʼaka Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "natakwa mkpepụta site nʼaka Chineke Onye nzọpụta ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Otu a ka ọ ga-adịrị ọgbọ ndị ya,",
            "poem": 1
          },
          {
            "text": "bụ ndị na-achọ ihu gị, Chineke nke Jekọb. Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Welitenụ isi unu, unu ọnụ ụzọ ama,",
            "poem": 1
          },
          {
            "text": "meenụ ka isi unu bụrụ ihe e welitere, unu ọnụ ụzọ ama ochie,",
            "poem": 2
          },
          {
            "text": "ka eze ebube nwee ike ịbata.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Onye bụ eze ebube",
            "poem": 1
          },
          {
            "noteId": 1598
          },
          {
            "text": "a?",
            "poem": 1
          },
          {
            "text": "Ọ bụ Onyenwe anyị, onye bụ dimkpa na dike.",
            "poem": 2
          },
          {
            "text": "Ọ bụ Onyenwe anyị, onye bụ dike nʼagha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Welitenụ isi unu, unu ọnụ ụzọ ama,",
            "poem": 1
          },
          {
            "text": "buliekwanụ ha elu, unu ọnụ ụzọ ama ochie,",
            "poem": 2
          },
          {
            "text": "ka Eze ebube nwee ike ịbatakwa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Onye bụ eze ebube a?",
            "poem": 1
          },
          {
            "text": "Ọ bụ Onyenwe anyị, Onye pụrụ ime ihe niile,",
            "poem": 2
          },
          {
            "text": "ọ bụ ya bụ Eze ebube! Sela",
            "poem": 2
          }
        ]
      }
    ],
