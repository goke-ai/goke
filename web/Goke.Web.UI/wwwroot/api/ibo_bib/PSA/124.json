  "chapter": {
    "number": 124,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ nrigo. Nke Devid."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Ekele maka mgbapụta"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "A sị na Onyenwe anyị adịnyereghị anyị,",
            "poem": 1
          },
          {
            "text": "ka Izrel kwuo,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "a sị na Onyenwe anyị adịnyereghị anyị,",
            "poem": 1
          },
          {
            "text": "mgbe ndị mmadụ biliri imegide anyị,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "mgbe iwe ha rere dịka ọkụ megide anyị,",
            "poem": 1
          },
          {
            "text": "ha gaara eloda anyị na ndụ;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "idee mmiri gaara iri anyị,",
            "poem": 1
          },
          {
            "text": "oke mmiri gaara ebufu anyị,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "ebili mmiri niile",
            "poem": 1
          },
          {
            "text": "gaara ebufu anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ma otuto dịrị Onyenwe anyị, onye na-ekweghị",
            "poem": 1
          },
          {
            "text": "ka ha jiri eze ha dọrie anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Anyị agbanarịla ha dịka nwa nnụnụ si",
            "poem": 1
          },
          {
            "text": "gbanarị ọnya osi ọnya chọrọ iji jide ya;",
            "poem": 2
          },
          {
            "text": "ọnya ahụ agbajiela,",
            "poem": 1
          },
          {
            "text": "anyị ewezugakwala onwe anyị, nwere onwe anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Inyeaka anyị dị nʼaha Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "onye kere eluigwe na ụwa.",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": []
  },
