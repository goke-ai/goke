  "chapter": {
    "number": 84,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma nke dịrị onyeisi Abụ. Abụ Ọma ụmụ Kora nʼusoro gittit."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Agụụ nke ụlọnsọ Chineke"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Lee ka ebe obibi gị sị maa mma,",
            "poem": 1
          },
          {
            "text": "O Onyenwe anyị, Onye pụrụ ime ihe niile!",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ọ na-agụsị mkpụrụobi m agụụ ike, e mkpụrụobi m na-adakwa mba,",
            "poem": 1
          },
          {
            "text": "ịbata nʼogige Onyenwe anyị;",
            "poem": 2
          },
          {
            "text": "mkpụrụobi m na anụ ahụ m na-eti mkpu",
            "poem": 1
          },
          {
            "text": "na-achọ Chineke dị ndụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ọ bụladị ụmụ nnụnụ nta nke eluigwe, achọtala ebe obibi;",
            "poem": 1
          },
          {
            "text": "eleke achọtakwarala onwe ya akwụ,",
            "poem": 2
          },
          {
            "text": "ebe ọ ga na-edebe ụmụ ya,",
            "poem": 2
          },
          {
            "text": "nʼakụkụ ebe ịchụ aja nsọ gị,",
            "poem": 1
          },
          {
            "text": "O Onyenwe anyị, Onye pụrụ ime ihe niile, Eze m na Chineke m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ndị a gọziri agọzi ka ndị bi nʼụlọ gị bụ;",
            "poem": 1
          },
          {
            "text": "ha na-eto gị mgbe niile. Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ndị a gọziri agọzi ka ha bụ, bụ ndị ike ha nwere dị na gị,",
            "poem": 1
          },
          {
            "text": "ndị kpebiri nʼobi ha ịga njem.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Mgbe ha na-esite na Ndagwurugwu Baka na-agafe,",
            "poem": 1
          },
          {
            "text": "ha na-eme ya ka ọ bụrụ ebe isi iyi dị iche iche na-asọpụta;",
            "poem": 2
          },
          {
            "text": "mmiri ozuzo mbụ na-ewere ngọzị niile kpuchie ya.",
            "poem": 2
          },
          {
            "noteId": 1661
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ike ha na-abawanye abawanye,",
            "poem": 1
          },
          {
            "text": "tutu ha niile nʼotu na otu, abịa guzo nʼihu Chineke na Zayọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "O Onyenwe anyị, Onye pụrụ ime ihe niile, nụrụ ekpere m.",
            "poem": 1
          },
          {
            "text": "Gee m ntị, gị Chineke nke Jekọb.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Lekwasị anya nʼọta anyị, O Chineke,",
            "poem": 1
          },
          {
            "text": "jiri ihuọma lekwasị onye gị e tere mmanụ anya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Otu mkpụrụ ụbọchị a nọrọ nʼime ogige gị,",
            "poem": 1
          },
          {
            "text": "karịrị puku ụbọchị a nọrọ ebe ọzọ;",
            "poem": 2
          },
          {
            "text": "ọ ga-akara m mma ịbụ onye nche nʼọnụ ụzọ ụlọnsọ Chineke m,",
            "poem": 1
          },
          {
            "text": "karịa ibi nʼụlọ ikwu nke ndị ajọ omume.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Onyenwe anyị Chineke bụ anyanwụ",
            "poem": 1
          },
          {
            "noteId": 1662
          },
          {
            "text": "na ọta;",
            "poem": 1
          },
          {
            "text": "Onyenwe anyị na-enye ihuọma na nsọpụrụ;",
            "poem": 2
          },
          {
            "text": "ọ naghị egbochi ihe ọma ọbụla",
            "poem": 1
          },
          {
            "text": "site nʼebe ndị na-eje ije nʼizuoke nọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "O Onyenwe anyị, Onye pụrụ ime ihe niile,",
            "poem": 1
          },
          {
            "text": "ngọzị na-adịrị ndị niile na-atụkwasị gị obi.",
            "poem": 2
          }
        ]
      }
    ],
