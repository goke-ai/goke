  "chapter": {
    "number": 130,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ nrigo."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Mkpụrụobi na-ele anya Chineke"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "O Onyenwe anyị, ọ bụ site nʼebe dị omimi ka m na-ebeku gị akwa.",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Onyenwe anyị, nụrụ olu m.",
            "poem": 2
          },
          {
            "text": "Ka ntị gị na-aṅakwa ntị",
            "poem": 1
          },
          {
            "text": "nʼolu arịrịọ m, maka amara.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "O Onyenwe anyị, a sị na i detuo mmehie niile,",
            "poem": 1
          },
          {
            "text": "Onyenwe anyị, onye pụrụ iguzo?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ma ọ bụ nʼaka gị ka mgbaghara mmehie",
            "poem": 1
          },
          {
            "noteId": 1697
          },
          {
            "text": "dị;",
            "poem": 1
          },
          {
            "text": "ime ka anyị tụọ gị egwu, ma jeere gị ozi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ana m echere Onyenwe anyị, mkpụrụobi m na-echekwa ya,",
            "poem": 1
          },
          {
            "text": "ọ bụ nʼokwu ya ka olileanya m dị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Mkpụrụobi m na-echere Onyenwe anyị",
            "poem": 1
          },
          {
            "text": "karịa ndị nche abalị na-echere chị ọbụbọ,",
            "poem": 2
          },
          {
            "text": "e, karịa ndị nche abalị na-echere chị ọbụbọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ndị Izrel, ka olileanya unu dịrị na Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "nʼihi na ịhụnanya ya na-adịgide adịgide,",
            "poem": 2
          },
          {
            "text": "nʼaka ya ka nzọpụta zuruoke dịkwa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ya onwe ya, ga-agbapụta Izrel",
            "poem": 1
          },
          {
            "text": "site na mmehie ya niile.",
            "poem": 2
          }
        ]
      }
    ],
