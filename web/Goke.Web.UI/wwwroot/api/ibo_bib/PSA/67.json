  "chapter": {
    "number": 67,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma dịrị onyeisi abụ. Abụ eji ụbọ akwara akpọ. Abụ Ọma. Otu Abụ."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Abụ ekele"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ka Chineke meere anyị amara ma gọzie anyị,",
            "poem": 1
          },
          {
            "text": "ma mee ka ihu ya chakwasị anyị,",
            "poem": 2
          },
          {
            "noteId": 1636
          },
          {
            "text": "Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ka a mara ụzọ gị niile nʼụwa,",
            "poem": 1
          },
          {
            "text": "marakwa nzọpụta gị nʼetiti mba niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ka mmadụ niile too gị, O Chineke;",
            "poem": 1
          },
          {
            "text": "ka mmadụ niile too gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ka mba niile ṅụrịa ma bụọ abụ ọṅụ,",
            "poem": 1
          },
          {
            "text": "nʼihi na ị na-eji ikpe ziri ezi na-achị ndị mmadụ,",
            "poem": 2
          },
          {
            "text": "na-edukwa mba niile nke ụwa. Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ka mmadụ niile too gị, O Chineke;",
            "poem": 1
          },
          {
            "text": "ka mmadụ niile too gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Mgbe ahụ, ala ga-amịpụta ihe owuwe ubi ya,",
            "poem": 1
          },
          {
            "text": "Chineke, onye bụ Chineke anyị, ga-agọzikwa anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Chineke ga-agọzi anyị,",
            "poem": 1
          },
          {
            "text": "nsọtụ niile nke ụwa ga-atụ egwu ya.",
            "poem": 2
          }
        ]
      }
    ],
