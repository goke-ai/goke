  "chapter": {
    "number": 63,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma Devid, mgbe ọ nọ nʼọzara Juda."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Onye ihe Chineke na-agụsị agụụ ike"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "O Chineke, ị bụ Chineke m,",
            "poem": 1
          },
          {
            "text": "e ji m obi m niile na-achọ gị;",
            "poem": 2
          },
          {
            "text": "akpịrị na-akpọ mkpụrụobi m nkụ,",
            "poem": 1
          },
          {
            "text": "anụ ahụ m na-achọsi gị ike,",
            "poem": 2
          },
          {
            "text": "nʼala kpọrọ nkụ na nke ịda mba",
            "poem": 1
          },
          {
            "text": "ebe mmiri na-adịghị nʼime ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ahụla m gị nʼụlọnsọ gị,",
            "poem": 1
          },
          {
            "text": "hụkwa ike gị na ebube gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Nʼihi na ịhụnanya gị dị mma karịa ndụ,",
            "poem": 1
          },
          {
            "text": "aga m eji egbugbere ọnụ m jaa gị mma.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ogologo ụbọchị m niile ka m ga-eto gị,",
            "poem": 1
          },
          {
            "text": "aga m eweli aka m abụọ elu kpọkuo aha gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Mkpụrụobi m ga-enwe afọ ojuju dịka onye ji ezi nri rijuo afọ;",
            "poem": 1
          },
          {
            "text": "aga m eji egbugbere ọnụ jupụtara nʼabụ too gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Nʼelu ihe ndina m, ana m echeta gị,",
            "poem": 1
          },
          {
            "text": "ana m atụgharị uche nʼihe banyere gị, ogologo abalị niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Nʼihi na ị bụ onye inyeaka m,",
            "poem": 1
          },
          {
            "text": "ana m abụ abụ nʼokpuru ndo nke nku gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Mkpụrụobi m na-arapara gị nʼahụ;",
            "poem": 1
          },
          {
            "text": "aka nri gị na-ejigide m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "A ga-ala ndị na-achọ ndụ m nʼiyi;",
            "poem": 1
          },
          {
            "text": "ha ga-arịda ruo ebe omimi nke ụwa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "A ga-eji mma agha laa ha nʼiyi,",
            "poem": 1
          },
          {
            "text": "ha ga-abụkwa nri nye nkịta ọhịa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ma eze ga-aṅụrị ọṅụ nʼime Chineke;",
            "poem": 1
          },
          {
            "text": "ndị niile na-eji aha Chineke aṅụ iyi ga-eto ya,",
            "poem": 2
          },
          {
            "text": "ma a ga-eme ka ọnụ ndị niile na-agha ụgha dere duu.",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": []
  },
