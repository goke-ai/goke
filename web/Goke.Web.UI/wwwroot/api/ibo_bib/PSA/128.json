  "chapter": {
    "number": 128,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ nrigo."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Ngọzị dịrị ndị na-atụ egwu Onyenwe anyị"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ngọzị na-adịrị ndị niile na-atụ egwu Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "ndị na-aga ije nʼirube isi nye ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ị ga-eri mkpụrụ ọrụ aka gị;",
            "poem": 1
          },
          {
            "text": "ọtụtụ ngọzị na ihe ịga nke ọma ga-abụ nke gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Nwunye gị ga-adị ka osisi vaịnị",
            "poem": 1
          },
          {
            "text": "na-amị mkpụrụ nʼime ụlọ gị;",
            "poem": 2
          },
          {
            "text": "ụmụ gị ndị ikom ga-adịkwa ka ome osisi oliv",
            "poem": 1
          },
          {
            "text": "gburugburu tebul gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Otu a ka a ga-esi gọzie nwoke ahụ",
            "poem": 1
          },
          {
            "text": "na-atụ egwu Onyenwe anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ka Onyenwe anyị gọzie gị site na Zayọn",
            "poem": 1
          },
          {
            "text": "ụbọchị niile nke ndụ gị;",
            "poem": 2
          },
          {
            "text": "ka i hụkwa ọdịmma Jerusalem,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "ka ị dị ndụ hụ ụmụ ụmụ gị.",
            "poem": 1
          },
          {
            "text": "Ka udo dịrị Izrel.",
            "poem": 2
          },
          {
            "noteId": 1694
          }
        ]
      }
    ],
