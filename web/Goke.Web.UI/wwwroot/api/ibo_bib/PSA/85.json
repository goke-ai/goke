  "chapter": {
    "number": 85,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma nke dịrị onyeisi Abụ. Abụ Ọma ụmụ Kora."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Ekpere maka ihuọma"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "O Onyenwe anyị, i gosila ala gị ihuọma;",
            "poem": 1
          },
          {
            "text": "i nyeghachila Jekọb akụnụba ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "I gbagharala ajọ omume ndị gị,",
            "poem": 1
          },
          {
            "text": "kpuchikwaa mmehie ha niile. Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ị kwụsịla iwe oke iwe gị,",
            "poem": 1
          },
          {
            "text": "I sikwala nʼịdị ọkụ nke iwe gị chigharịa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Kpọghachi anyị azụ, O Chineke, Onye Nzọpụta anyị,",
            "poem": 1
          },
          {
            "text": "wezuga iwe gị site nʼebe anyị nọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ọ bụ ruo mgbe ebighị ebi, ka ị ga na-eweso anyị iwe:",
            "poem": 1
          },
          {
            "text": "Ị ga-eme ka iwe gị dịgide ruo ọgbọ niile?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ị gaghị eme ka anyị dịkwa ndụ ọzọ",
            "poem": 1
          },
          {
            "text": "ka ndị gị ṅụrịa ọṅụ nʼime gị?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "O Onyenwe anyị, gosi anyị ịhụnanya gị nke na-adịgide,",
            "poem": 1
          },
          {
            "text": "nyekwa anyị nzọpụta gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Aga m ege ntị mara ihe Chineke, onye bụ Onyenwe anyị ga-ekwu;",
            "poem": 1
          },
          {
            "text": "ọ na-ekwe ndị ya, ndị nsọ ya nkwa udo,",
            "poem": 2
          },
          {
            "text": "ma ka ha ghara ịlọghachi na nzuzu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Nʼezie, nzọpụta ya dị nso",
            "poem": 1
          },
          {
            "noteId": 1663
          },
          {
            "text": "ndị na-atụ egwu ya,",
            "poem": 1
          },
          {
            "text": "ka ebube ya si otu a dịgide nʼala anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Ịhụnanya na ikwesi ntụkwasị obi ezukọtala;",
            "poem": 1
          },
          {
            "text": "ezi omume na udo esutula ọnụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ikwesi ntụkwasị obi ga-esite nʼala pupụta,",
            "poem": 1
          },
          {
            "text": "ezi omume ga-esite nʼeluigwe ledata anya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Nʼezie, Onyenwe anyị ga-enye ihe dị mma,",
            "poem": 1
          },
          {
            "text": "ala anyị ga-amịkwa mkpụrụ ubi ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Ezi omume ga-aga nʼihu ya,",
            "poem": 1
          },
          {
            "text": "idoziri ya ụzọ ọ ga-esi gaa.",
            "poem": 2
          }
        ]
      }
    ],
