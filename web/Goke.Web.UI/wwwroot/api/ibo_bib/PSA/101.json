  "chapter": {
    "number": 101,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma Devid."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Nkwa eze"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Aga m abụ abụ banyere ịhụnanya gị na ikpe gị ziri ezi;",
            "poem": 1
          },
          {
            "text": "o Onyenwe anyị, ọ bụ gị ka m ga-abụku abụ otuto.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Aga m enwe uche ị hụ na m biri ndụ na-enweghị ịta ụta ọbụla,",
            "poem": 1
          },
          {
            "text": "olee mgbe ị ga-abịakwute m?",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Aga m ejikwa obi nke a na-agaghị ata ụta ọbụla",
            "poem": 1
          },
          {
            "text": "jegharịa nʼime ụlọ m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Agaghị m edo ihe jọgburu onwe ya",
            "poem": 1
          },
          {
            "text": "nʼihu m.",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Ana m akpọ ọrụ ndị na-ekwesighị ntụkwasị obi asị.",
            "poem": 1
          },
          {
            "text": "Agaghị m enwekwa oke nʼime ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ndị obi ha gbagọrọ agbagọ agaghị anọ m nso.",
            "poem": 1
          },
          {
            "text": "Agakwaghị m etinye aka nʼọrụ ọjọọ ọbụla.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Onye ọbụla na-ekwulu onye agbataobi ya na nzuzo,",
            "poem": 1
          },
          {
            "text": "onye ahụ ka m ga-emechi ọnụ;",
            "poem": 2
          },
          {
            "text": "agaghị m anabata",
            "poem": 1
          },
          {
            "text": "onye ọbụla dị mpako ma nwee obi nganga.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Aga m elekwasị ndị kwesiri ntụkwasị obi nʼala a anya,",
            "poem": 1
          },
          {
            "text": "ka mụ na ha biri;",
            "poem": 2
          },
          {
            "text": "onye ahụ na-eje ije na-enweghị ịta ụta ọbụla",
            "poem": 1
          },
          {
            "text": "ga-ejere m ozi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Onye ọbụla na-aghọ aghụghọ",
            "poem": 1
          },
          {
            "text": "agaghị ebi nʼụlọ m;",
            "poem": 2
          },
          {
            "text": "onye ọbụla na-ekwu okwu ụgha",
            "poem": 1
          },
          {
            "text": "agaghị eguzokwa nʼihu m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ụtụtụ niile ka m na-eme ka ha dere duu",
            "poem": 1
          },
          {
            "text": "bụ ndị ajọ omume niile nọ nʼala a;",
            "poem": 2
          },
          {
            "text": "Aga m ebipụ ndị niile na-eme ajọ ihe",
            "poem": 1
          },
          {
            "text": "site nʼobodo Onyenwe anyị.",
            "poem": 2
          }
        ]
      }
    ],
