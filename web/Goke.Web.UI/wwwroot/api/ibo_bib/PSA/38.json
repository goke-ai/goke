  "chapter": {
    "number": 38,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma Devid. Arịrịọ."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Arịrịọ nʼoge nsogbu"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Biko, Onyenwe anyị, abarala m mba nʼiwe,",
            "poem": 1
          },
          {
            "text": "maọbụ taa m ahụhụ mgbe iwe gị dị ọkụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Nʼihi na àkụ gị niile amapuola m ahụ,",
            "poem": 1
          },
          {
            "text": "aka gị adakwasịkwala m, dịka ọ bụ ọkpọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Nʼihi oke iwe gị, ọ dịghị izuoke dị nʼime anụ ahụ;",
            "poem": 1
          },
          {
            "text": "enweghịkwa m udo nʼime ọkpụkpụ m niile nʼihi mmehie m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Mmetụta ikpe ọmụma m na-adakwasị m",
            "poem": 1
          },
          {
            "text": "dịka ibu arọ nke dị arọ karịa m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ọnya m na-ere ure na-asọ oyi",
            "poem": 1
          },
          {
            "text": "nʼihi mmehie enweghị uche m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Abụ m onye a nyidara ma kwatukwaa nʼala",
            "poem": 1
          },
          {
            "text": "ogologo ụbọchị niile, ana m agagharị nʼiru ụjụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Azụ na-egbu m mgbu nke ukwuu",
            "poem": 1
          },
          {
            "text": "ahụ adịghịkwa m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ume ọbụla adịghịkwa nʼime. Adị m ka onye e gwepịara egwepịa.",
            "poem": 1
          },
          {
            "text": "Ana m asụ ude nʼihi oke ihe mgbu nke obi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "O Onyenwe m, ị maara agụụ niile nke na-agụ obi m;",
            "poem": 1
          },
          {
            "text": "ịsụ ude m ezokwaghị ezo nʼebe ị nọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Obi na-alọ m mmiri, ike na-agwụkwa m,",
            "poem": 1
          },
          {
            "text": "ìhè esitekwala nʼanya m pụọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ndị m hụrụ nʼanya na ndị enyi m na-asọ m oyi nʼihi ọnya m,",
            "poem": 1
          },
          {
            "text": "ndị agbataobi m na-anọ m ebe dị anya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ndị na-achọ igbu m na-esiri m ọnya,",
            "poem": 1
          },
          {
            "text": "ndị ahụ chọrọ imerụ m ahụ na-ekwu maka ihe dakwasịrị m;",
            "poem": 2
          },
          {
            "text": "ogologo ụbọchị niile ha na-agba izuzu aghụghọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Adị m ka onye ntị chiri, onye na-apụghị ịnụ ihe ọbụla,",
            "poem": 1
          },
          {
            "text": "dịka onye ogbu, onye na-apụghị imeghe ọnụ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Adịla m dịka onye na-anaghị anụ ihe,",
            "poem": 1
          },
          {
            "text": "onye ọnụ ya apụghị ịsa okwu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "O Onyenwe anyị, ana m eche gị;",
            "poem": 1
          },
          {
            "text": "o Onyenwe m Chineke m, ị ga-aza.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Nʼihi na asịrị m, “Ekwela ka ha ṅụrịa ọṅụ nʼisi m;",
            "poem": 1
          },
          {
            "text": "maọbụ bulie onwe ha elu nʼebe m nọ mgbe ụkwụ m na-agbuchapụ.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Nʼihi na lee, ana m achọ ịda ada.",
            "poem": 1
          },
          {
            "text": "Anọkwa m nʼọnọdụ oke mgbu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Ana m ekwupụta mmehie m;",
            "poem": 1
          },
          {
            "text": "mmehie m na-ewute m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Ndị iro m, ndị na-eme ihe ike dị ọtụtụ,",
            "poem": 1
          },
          {
            "text": "ndị na-akpọ m asị, na-enweghị ihe m mere, bara ụba.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Ndị na-eji ihe ọjọọ akwụghachi m ezi ihe,",
            "poem": 1
          },
          {
            "text": "na-ebo ebubo megide m",
            "poem": 2
          },
          {
            "text": "nʼagbanyeghị na m na-achọ ime naanị ezi ihe.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "O Onyenwe anyị, ahapụkwala m;",
            "poem": 1
          },
          {
            "text": "O Chineke m, anọkwala m ebe dị anya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "Bịa ọsịịsọ inyere m aka",
            "poem": 1
          },
          {
            "text": "Onyenwe m na Onye nzọpụta m.",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": []
