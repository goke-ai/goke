  "chapter": {
    "number": 111,
    "content": [
      {
        "type": "heading",
        "content": [
          "Toonu Onyenwe anyị"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Toonu Onyenwe anyị!",
            "poem": 1
          },
          {
            "lineBreak": true
          },
          {
            "text": "Aga m eji obi m niile jaa Onyenwe anyị mma",
            "poem": 1
          },
          {
            "text": "nʼime nzukọ ndị ụzọ ha ziri ezi na nʼime ọgbakọ ukwuu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ọrụ niile Onyenwe anyị na-arụ dị ukwuu;",
            "poem": 1
          },
          {
            "text": "ndị niile na-enwe mmasị na ha na-atụgharị uche nʼime ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ọrụ ya jupụtara nʼebube na ịma mma,",
            "poem": 1
          },
          {
            "text": "ezi omume ya na-adịgidekwa ruo mgbe ebighị ebi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "O meela ka e cheta ọrụ ebube ya niile;",
            "poem": 1
          },
          {
            "text": "Onyenwe anyị bụ onye amara na onye ọmịiko.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ọ na-egboro ndị na-atụ egwu ya mkpa ihe oriri;",
            "poem": 1
          },
          {
            "text": "ọ na-echetakwa ọgbụgba ndụ ya ruo mgbe ebighị ebi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "O gosila ndị ya ike nke ọrụ ya niile,",
            "poem": 1
          },
          {
            "text": "site nʼinye ha ala niile nke ndị mba ọzọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ọrụ aka ya niile bụ ihe kwesiri ekwesi na ikpe ziri ezi;",
            "poem": 1
          },
          {
            "text": "ụkpụrụ ya niile bụ ihe kwesiri ntụkwasị obi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "E hiwere ha ịdị ruo mgbe niile ebighị ebi,",
            "poem": 1
          },
          {
            "text": "ha bụ ihe e mere nʼikwesị ntụkwasị obi na izi ezi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "O wetaara ndị ya mgbapụta;",
            "poem": 1
          },
          {
            "text": "o meela ka ọgbụgba ndụ ya guzosie ike ruo ebighị ebi,",
            "poem": 2
          },
          {
            "text": "aha ya dị nsọ dịkwa oke egwu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Ịtụ egwu Onyenwe anyị bụ mmalite amamihe;",
            "poem": 1
          },
          {
            "text": "ndị niile na-edebe ụkpụrụ ya nwere ezi nghọta.",
            "poem": 2
          },
          {
            "text": "Otuto ya na-adị ruo mgbe ebighị ebi.",
            "poem": 2
          }
        ]
      }
    ],
