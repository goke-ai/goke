  "chapter": {
    "number": 29,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma Devid."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Olu Onyenwe anyị"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Toonu Onyenwe anyị, unu ndị bi nʼeluigwe,",
            "poem": 1
          },
          {
            "text": "nyenụ Onyenwe anyị nsọpụrụ nʼihi ebube na ike ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Nyenụ Onyenwe anyị nsọpụrụ kwesiri aha ya;",
            "poem": 1
          },
          {
            "text": "kpọọ isiala nye Onyenwe anyị nʼebube nke ịdị nsọ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Olu Onyenwe anyị na-ada nʼelu osimiri;",
            "poem": 1
          },
          {
            "text": "olu Chineke dị ebube na-ada ike ike,",
            "poem": 2
          },
          {
            "text": "olu Onyenwe anyị na-ada dịka egbe eluigwe",
            "poem": 2
          },
          {
            "noteId": 1603
          },
          {
            "text": "nʼelu oke osimiri.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Olu Onyenwe anyị dị ike,",
            "poem": 1
          },
          {
            "text": "olu Onyenwe anyị jupụtakwara nʼebube.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Olu Onyenwe anyị na-etiwa osisi ukwu sida;",
            "poem": 1
          },
          {
            "text": "Onyenwe anyị na-etiwasị osisi ukwu sida nke Lebanọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ọ na-eme ka Lebanọn wulie elu dịka nwa ehi,",
            "poem": 1
          },
          {
            "text": "ọ na-eme ka Siriọn",
            "poem": 2
          },
          {
            "noteId": 1604
          },
          {
            "text": "wulie elu dịka nwa atụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Olu Onyenwe anyị na-agba dịka egbe eluigwe",
            "poem": 1
          },
          {
            "text": "mgbe amụma na-egbu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Olu Onyenwe anyị na-eme ka ọzara maa jijiji;",
            "poem": 1
          },
          {
            "text": "Onyenwe anyị na-eme ka ọzara Kadesh maa jijiji.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Olu Onyenwe anyị na-eme ka osisi ook gbagọọ",
            "poem": 1
          },
          {
            "noteId": 1605
          },
          {
            "text": "na-agbuchapụkwa akwụkwọ niile nke oke ọhịa.",
            "poem": 2
          },
          {
            "text": "Nʼime ụlọnsọ ukwuu ya mmadụ niile na-eti, “Ebube!”",
            "poem": 1
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Onyenwe anyị nọ nʼocheeze ya nʼelu uju mmiri;",
            "poem": 1
          },
          {
            "text": "Onyenwe anyị na-achị dịka eze ruo mgbe ebighị ebi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Onyenwe anyị na-enye ndị ya ike,",
            "poem": 1
          },
          {
            "text": "ọ ga-ejikwa udo gọzie ndị ya.",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": [
      {
        "noteId": 1603,
        "caller": "+",
        "text": "Job 37:4-5; Abụ 18:13",
        "reference": {
          "chapter": 29,
          "verse": 3
        }
      },
      {
        "noteId": 1604,
        "caller": "+",
        "text": "Nke a bụ aha ndị Fonikisi bi na mpaghara ugwu Hemon na-akpọ ugwu ahụ. Hemon dị nʼoke ala nkwa ahụ nʼebe ugwu. Dit 3:9; Abụ 42:6",
        "reference": {
          "chapter": 29,
          "verse": 6
        }
      },
      {
        "noteId": 1605,
        "caller": "+",
        "text": "Ime na-eme nne ele",
        "reference": {
          "chapter": 29,
          "verse": 9
        }
      }
    ]
