  "chapter": {
    "number": 113,
    "content": [
      {
        "type": "heading",
        "content": [
          "Abụ otuto"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Toonu Onyenwe anyị!",
            "poem": 1
          },
          {
            "lineBreak": true
          },
          {
            "text": "Toonu Onyenwe anyị unu ndị ohu ya.",
            "poem": 1
          },
          {
            "text": "Toonu aha Onyenwe anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ka aha Onyenwe anyị bụrụ ihe a gọziri agọzi",
            "poem": 1
          },
          {
            "text": "site ugbu a ruo mgbe ebighị ebi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Site nʼọwụwa anyanwụ ruo nʼọdịda ya,",
            "poem": 1
          },
          {
            "text": "aha Onyenwe anyị ga-abụ ihe a na-eto eto.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Onyenwe anyị dị elu karịa mba niile,",
            "poem": 1
          },
          {
            "text": "ebube ya dị elu karịa eluigwe.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Onye dị ka Onyenwe anyị Chineke anyị,",
            "poem": 1
          },
          {
            "text": "onye nọkwasịrị ocheeze nʼebe dị elu,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "onye na-eme onwe ya ka ọ dị ala ilekwasị",
            "poem": 1
          },
          {
            "text": "eluigwe na ụwa anya?",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ọ na-esi nʼuzuzu bulie ogbenye elu",
            "poem": 1
          },
          {
            "text": "sitekwa nʼebe ikpofu ahịhịa bulie onye nọ na mkpa elu;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "ọ na-eme ka ya na ụmụ eze ndị ikom nọkọọ,",
            "poem": 1
          },
          {
            "text": "ọ na-eme ka o soro ndị a maara aha ha nʼetiti ụmụ mmadụ nọkọọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Ọ na-eme ka nwanyị aga biri nʼụlọ nke ya",
            "poem": 1
          },
          {
            "text": "dịka nne mụrụ ọtụtụ ụmụ ọṅụ juru obi.",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Toonu Onyenwe anyị.",
            "poem": 1
          }
        ]
      }
    ],
