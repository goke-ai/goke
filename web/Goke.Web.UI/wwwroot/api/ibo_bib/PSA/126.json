  "chapter": {
    "number": 126,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ nrigo."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Abụ ekele maka mgbapụta"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Mgbe Onyenwe anyị kpọghachitere ndị e mere ka ha lọta na Zayọn,",
            "poem": 1
          },
          {
            "noteId": 1693
          },
          {
            "text": "anyị dị ka ndị na-arọ nrọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ọchị juru anyị ọnụ,",
            "poem": 1
          },
          {
            "text": "abụ ọṅụ jukwara anyị ire.",
            "poem": 2
          },
          {
            "text": "Mgbe ahụ, e kwukwara ya na mba niile sị,",
            "poem": 1
          },
          {
            "text": "“Onyenwe anyị emeerela ha ọtụtụ ihe dị ukwuu.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Onyenwe anyị emeerala anyị ọtụtụ ihe dị ukwuu,",
            "poem": 1
          },
          {
            "text": "obi anyị jupụtakwara nʼọṅụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Nyeghachi anyị akụnụba anyị, o Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "dịka mmiri iyi na-asọ nʼala Negev.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ndị niile na-eji anya mmiri na-arụ ọrụ ịkụ mkpụrụ",
            "poem": 1
          },
          {
            "text": "ga-emesịa jiri abụ ọṅụ wee ihe ubi ahụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Onye ji ịkwa akwa",
            "poem": 1
          },
          {
            "text": "buru mkpụrụ ubi ọ ga-agha na-aga,",
            "poem": 2
          },
          {
            "text": "ga-emesịa jiri abụ ọṅụ na-alọghachi azụ,",
            "poem": 1
          },
          {
            "text": "mgbe ha bu ukwu ọka ha.",
            "poem": 2
          }
        ]
      }
    ],
