  "chapter": {
    "number": 74,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ maskil nke Asaf."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Ekpere maka enyemaka"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Chineke, gịnị mere i ji jụ anyị ruo mgbe ebighị ebi?",
            "poem": 1
          },
          {
            "text": "Gịnị mere iwe gị ji akwụ anwụrụ megide igwe atụrụ ndị ị na-azụ?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Cheta mba ahụ nke ị zụtara na mgbe ochie,",
            "poem": 1
          },
          {
            "text": "ebo ahụ bụ ihe nketa gị, ndị ị gbapụtara,",
            "poem": 2
          },
          {
            "text": "nʼugwu Zayọn ebe i biri.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Tụgharịa nzọ ụkwụ gị bịa na mbibi ebighị ebi ndị a.",
            "poem": 1
          },
          {
            "text": "Ọ bụ onye iro wetara ịla nʼiyi a niile nʼụlọnsọ a.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ndị iro gị gbọrọ ụja nʼebe i zutere anyị.",
            "poem": 1
          },
          {
            "text": "Ha weliri ọkọlọtọ ha dịka ihe ịrịbama.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ha kpara agwa dịka ndị ikom ji anyụike",
            "poem": 1
          },
          {
            "text": "ha ga-eji gbutuo ọtụtụ osisi dị nʼoke ọhịa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ha ji anyụike na egbugbu ha",
            "poem": 1
          },
          {
            "text": "kụjisie ihe niile eji amachi ihe bụ nke egbunyere ihe.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ha kpọkwara ụlọnsọ gị ọkụ, mee ya ka o repịa.",
            "poem": 1
          },
          {
            "text": "Ha merụrụ ụlọ obibi ahụ a kpọkwasịrị aha gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ha sịrị nʼobi ha, “Anyị ga-etipịa ha kpamkpam!”",
            "poem": 1
          },
          {
            "text": "Ha kpọrọ ebe niile a na-anọ efe Chineke nʼala a ọkụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Enyeghị anyị ihe ịrịbama ọbụla site nʼaka Chineke.",
            "poem": 1
          },
          {
            "text": "Ọ dịkwaghị ndị amụma fọdụrụ.",
            "poem": 2
          },
          {
            "text": "Ọ dịkwaghị onye ọbụla nʼetiti anyị onye maara, mgbe ihe a ga-agwụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "O Chineke, ruo ole mgbe ka ndị iro ga-anọgide na-achị gị ọchị?",
            "poem": 1
          },
          {
            "text": "Onye iro, ọ ga-eleli aha gị ruo mgbe ebighị ebi?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Gịnị mere ị na-eseghachi aka gị azụ, e aka nri gị?",
            "poem": 1
          },
          {
            "text": "Sepụta aka gị laa ha nʼiyi!",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ma gị, O Chineke, bụrịị eze m site na mgbe ochie.",
            "poem": 1
          },
          {
            "text": "Ọ bụ gị na-arụpụta nzọpụta nʼelu ụwa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Site nʼike gị dị ukwuu i mere ka osimiri kewaa abụọ.",
            "poem": 1
          },
          {
            "text": "Ọ bụ gị gwepịara isi anụ ọjọọ ahụ nʼime mmiri.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Ọ bụ gị gwepịara isi anụ ọjọọ ahụ a na-akpọ leviatan,",
            "poem": 1
          },
          {
            "text": "mee ka ọ ghọọ nri nye ụmụ anụmanụ bi nʼọzara.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Ọ bụ gị mere ka isi iyi na mmiri iyi na-asọ.",
            "poem": 1
          },
          {
            "text": "I mere ka osimiri na-eru eru mgbe niile taa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Ehihie na abalị bụ nke gị.",
            "poem": 1
          },
          {
            "text": "I mere ka anyanwụ na ọnwa guzosie ike.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Ọ bụ gị kewapụtara oke ala niile nke ụwa.",
            "poem": 1
          },
          {
            "text": "Ọ bụ gị mere ka ọkọchị na udu mmiri dị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "O Onyenwe anyị, cheta otu onye iro si chịa gị ọchị,",
            "poem": 1
          },
          {
            "text": "na otu ndị nzuzu si kwaa aha gị emo.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Ewerala ndụ nduru gị nyefee anụ ọhịa ọjọọ nʼaka.",
            "poem": 1
          },
          {
            "text": "Echefukwala ndụ ndị gị a na-akpagbu ruo mgbe ebighị ebi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Chetakwa ọgbụgba ndụ gị,",
            "poem": 1
          },
          {
            "text": "nʼihi na ihe ike na-ejupụta ebe niile gbara ọchịchịrị nʼala a.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Ekwela ka ndị a na-akpagbu gbaghachi azụ nʼihere;",
            "poem": 1
          },
          {
            "text": "ka ndị ogbenye na ndị nọ na mkpa too aha gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "O Chineke, bilie, kpechitara onwe gị ikpe.",
            "poem": 1
          },
          {
            "text": "Chetakwa otu ndị nzuzu si kọọ gị ọnụ ogologo ụbọchị niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 23,
        "content": [
          {
            "text": "Elepụla mkpọtụ nke ndị iro gị niile anya,",
            "poem": 1
          },
          {
            "text": "olu mkpọtụ nke ndị iro gị na-aga nʼihu na-ala elu.",
            "poem": 2
          }
        ]
      }
    ],
