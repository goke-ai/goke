  "chapter": {
    "number": 145,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma otuto, nke Devid."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Ịdị mma nke Chineke"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Aga m ebuli gị elu, Chineke m onye bụ eze;",
            "poem": 1
          },
          {
            "text": "aga m eto aha gị, site nʼebighị ebi ruo ebighị ebi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ụbọchị niile ka m ga-eto gị",
            "poem": 1
          },
          {
            "text": "jaakwa aha gị mma site nʼebighị ebi ruo ebighị ebi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Onyenwe anyị dị ukwuu, bụrụkwa onye kwesiri ịnata otuto;",
            "poem": 1
          },
          {
            "text": "ịdị ukwuu ya karịrị ihe onye ọbụla pụrụ ịghọta.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Otu ọgbọ ga-eto ọrụ gị niile nʼihu ọgbọ ọzọ;",
            "poem": 1
          },
          {
            "text": "ha ga-ekwupụta maka ọrụ ukwuu gị niile dị iche iche.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ha ga-ekwupụta ihe banyere ịdị ebube nke ịma mma gị,",
            "poem": 1
          },
          {
            "text": "aga m atụgharịkwa uche nʼọrụ ebube gị niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ha ga-ekwupụta ihe banyere ike nke ọrụ ịtụnanya gị niile,",
            "poem": 1
          },
          {
            "text": "aga m ekwusakwa ọrụ ukwuu gị niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ha ga-eme mmemme nʼihi ịdị mma gị dị ukwuu",
            "poem": 1
          },
          {
            "text": "jirikwa ọṅụ bụọ abụ banyere ezi omume gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Onyenwe anyị na-eme amara nweekwa obi ọmịiko,",
            "poem": 1
          },
          {
            "text": "ọ dịghị ewe iwe ọsịịsọ, ọ bara ụba nʼịhụnanya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Onyenwe anyị dị mma nʼebe mmadụ niile dị;",
            "poem": 1
          },
          {
            "text": "o nwere ọmịiko nʼebe ihe niile o kere eke dị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "O Onyenwe anyị, ihe niile i kere eke ga-eto gị;",
            "poem": 1
          },
          {
            "text": "ndị nsọ gị na-aja gị mma.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ha ga-ekwupụta banyere ebube nke alaeze gị.",
            "poem": 1
          },
          {
            "text": "Kwupụtakwa maka ịdị ike gị,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "ka mmadụ niile mata ihe banyere ọrụ ukwuu gị niile dị iche iche",
            "poem": 1
          },
          {
            "text": "na ebube nke ịdị mma alaeze gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Alaeze gị bụ alaeze na-adịgide ruo mgbe ebighị ebi,",
            "poem": 1
          },
          {
            "text": "ọchịchị gị na-adịgidekwa site nʼọgbọ ruo nʼọgbọ.",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Onyenwe anyị na-emezu nkwa ya niile",
            "poem": 1
          },
          {
            "text": "na-ahụkwa ihe niile o kere eke nʼanya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Onyenwe anyị na-ekulite ndị niile dara ada",
            "poem": 1
          },
          {
            "text": "na-akwagide ndị niile dara mba.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Olileanya ihe niile dị na gị,",
            "poem": 1
          },
          {
            "text": "ị na-enye ha nri ha mgbe oge ya ruru.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "I na-asaghepụ aka gị",
            "poem": 1
          },
          {
            "text": "na-emezu ọchịchọ ihe niile dị ndụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Ụzọ Onyenwe anyị niile ziri ezi;",
            "poem": 1
          },
          {
            "text": "ọ na-ahụkwa ihe niile o kere eke nʼanya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Onyenwe anyị na-anọ nso ndị niile na-akpọku ya,",
            "poem": 1
          },
          {
            "text": "bụ ndị niile na-akpọku ya nʼeziokwu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Ọ na-emezu ọchịchọ niile nke ndị na-atụ egwu ya;",
            "poem": 1
          },
          {
            "text": "ọ na-anụ akwa ha na-azọpụtakwa ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Onyenwe anyị na-echebe ndị niile hụrụ ya nʼanya,",
            "poem": 1
          },
          {
            "text": "ma ọ ga-ala ndị ajọ omume niile nʼiyi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Ọnụ m ga-ekwupụta otuto Onyenwe anyị;",
            "poem": 1
          },
          {
            "text": "ka ihe niile e kere eke too aha nsọ ya",
            "poem": 2
          },
          {
            "text": "ruo mgbe ebighị ebi.",
            "poem": 2
          }
        ]
      }
    ],
