  "chapter": {
    "number": 48,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Otu Abụ. Abụ Ọma nke ụmụ Kora."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Abụ nke Zayọn"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Onyenwe anyị dị ukwuu, bụrụkwa onye kwesiri ịnata otuto,",
            "poem": 1
          },
          {
            "text": "nʼime obodo Chineke anyị, nʼugwu nsọ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ọ mara mma nʼịdị elu ya,",
            "poem": 1
          },
          {
            "text": "bụkwa ọṅụ elu ụwa niile.",
            "poem": 2
          },
          {
            "text": "Ugwu Zayọn, obodo eze ahụ dị ukwuu,",
            "poem": 1
          },
          {
            "text": "dịka ebe kachasị elu nke Zafọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Chineke nọ nʼime ebe ya e wusiri ike;",
            "poem": 1
          },
          {
            "text": "o gosila onwe ya dịka ebe mgbaba ha e wusiri ike.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Mgbe ndị eze jikọtara ndị agha ha ọnụ,",
            "poem": 1
          },
          {
            "text": "mgbe ha bịakọtara nso,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "ihe ha hụrụ juru ha anya;",
            "poem": 1
          },
          {
            "text": "nʼihi oke egwu, ha gbara ọsọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ahụ ọma jijiji bidoro ha nʼebe ahụ,",
            "poem": 1
          },
          {
            "text": "ihe mgbu ha dịka nke nwanyị na-achọ ịmụ nwa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ị lara ha nʼiyi dịka oke ifufe si nʼọwụwa anyanwụ",
            "poem": 1
          },
          {
            "text": "si ala ụgbọ mmiri ukwuu nke Tashish nʼiyi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Dịka anyị nụrụ,",
            "poem": 1
          },
          {
            "text": "otu a ka anyị hụrụ,",
            "poem": 2
          },
          {
            "text": "nʼime obodo Onyenwe anyị, Onye pụrụ ime ihe niile,",
            "poem": 1
          },
          {
            "text": "nʼime obodo Chineke anyị:",
            "poem": 2
          },
          {
            "text": "Chineke na-eme ka o guzosie ike",
            "poem": 1
          },
          {
            "text": "ruo mgbe ebighị ebi. Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "O Chineke, nʼime ụlọnsọ ukwuu gị,",
            "poem": 1
          },
          {
            "text": "ka anyị nọ na-atụgharị uche nʼịhụnanya gị nke na-enweghị ọgwụgwụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "O Chineke, dịka aha gị,",
            "poem": 1
          },
          {
            "text": "otuto gị na-eru na nsọtụ ụwa niile;",
            "poem": 2
          },
          {
            "text": "aka nri gị jupụtara nʼezi omume.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ugwu Zayọn na-aṅụrị ọṅụ,",
            "poem": 1
          },
          {
            "text": "obodo nta niile nke Juda nwere obi ụtọ",
            "poem": 2
          },
          {
            "text": "nʼihi ikpe gị niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Gaanụ Zayọn gburugburu, Jegharịa ya gburugburu,",
            "poem": 1
          },
          {
            "text": "gụọkwanụ ụlọ elu ya niile ọnụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Lezienụ mgbidi ya niile anya,",
            "poem": 1
          },
          {
            "text": "nyochakwaanụ ebe ya niile e wusiri ike,",
            "poem": 2
          },
          {
            "text": "ka unu nwee ike ịkọrọ",
            "poem": 1
          },
          {
            "text": "ọgbọ na-abịa nʼihu akụkọ ndị a.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Nʼihi na Chineke a, bụ Chineke anyị ruo mgbe niile ebighị ebi,",
            "poem": 1
          },
          {
            "text": "ọ ga-abụ onyendu anyị, ọ bụladị ruo nʼọgwụgwụ.",
            "poem": 2
          }
        ]
      }
    ],
