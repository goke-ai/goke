  "chapter": {
    "number": 97,
    "content": [
      {
        "type": "heading",
        "content": [
          "Chineke onye na-achị ihe niile"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Onyenwe anyị bụ eze,",
            "poem": 1
          },
          {
            "noteId": 1671
          },
          {
            "text": "ka ụwa niile nwee obi ụtọ;",
            "poem": 1
          },
          {
            "text": "ka ndị niile bi nʼebe dị anya ṅụrịa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Igwe ojii na oke ọchịchịrị gbara ya gburugburu;",
            "poem": 1
          },
          {
            "text": "ezi omume na ikpe ziri ezi bụ ntọala ocheeze ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ọkụ na-enwu enwu na-aga nʼihu ya",
            "poem": 1
          },
          {
            "text": "ọ na-erepịasị ndị iro ya nʼakụkụ ya ọbụla.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Amụma ya na-acha chazuo elu ụwa niile;",
            "poem": 1
          },
          {
            "text": "ụwa niile na-ahụ ya na-ama jijiji.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ugwu niile na-agbaze dịka mmanụ nʼihu Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "nʼihu Onyenwe ụwa niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Eluigwe na-ekwuwapụta ezi omume ya;",
            "poem": 1
          },
          {
            "text": "ndị niile bi nʼụwa na-ahụkwa ebube ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "A na-eme ka ihere mee ndị niile na-efe arụsị niile a pịrị apị ofufe,",
            "poem": 1
          },
          {
            "text": "ndị na-anya isi na chi niile e jiri aka mee",
            "poem": 2
          },
          {
            "text": "feenụ ya ofufe, unu chi niile!",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Zayọn na-anụ ma na-aṅụrịkwa ọṅụ,",
            "poem": 1
          },
          {
            "text": "obodo nta niile nke Juda na-enwekwa obi ụtọ",
            "poem": 2
          },
          {
            "text": "nʼihi ikpe gị, O Onyenwe anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Nʼihi na gị, O Onyenwe anyị, bụ Onye kachasị ihe niile elu nʼụwa niile;",
            "poem": 1
          },
          {
            "text": "ị bụ onye e buliri elu karịa chi ndị ọzọ niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Ka unu, ndị hụrụ Onyenwe anyị nʼanya, kpọọ ihe ọjọọ asị,",
            "poem": 1
          },
          {
            "text": "nʼihi na ọ na-echebe ndụ ndị nke ya, ndị tụkwasịrị ya obi",
            "poem": 2
          },
          {
            "text": "ma na-anapụtakwa ha site nʼaka ndị ajọ omume.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ìhè na-amụkwasị onye ezi omume,",
            "poem": 1
          },
          {
            "text": "ọṅụ na-adịkwa nʼime obi onye na-eme ihe ziri ezi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ṅụrịanụ ọṅụ nʼime Onyenwe anyị, unu ndị ezi omume,",
            "poem": 1
          },
          {
            "text": "tookwanụ aha nsọ ya.",
            "poem": 2
          }
        ]
      }
    ],
