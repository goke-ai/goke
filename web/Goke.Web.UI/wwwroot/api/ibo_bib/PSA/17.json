  "chapter": {
    "number": 17,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Ekpere Devid."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Ekpere maka nnapụta"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "O Onyenwe anyị, nụrụ arịrịọ m ziri ezi,",
            "poem": 1
          },
          {
            "text": "nụrụkwa mkpu akwa m.",
            "poem": 2
          },
          {
            "text": "Gee ntị nʼekpere m",
            "poem": 1
          },
          {
            "text": "nʼihi na o siteghị nʼegbugbere ọnụ na-ekwu okwu aghụghọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ka mkpepụta m si nʼaka gị bịa;",
            "poem": 1
          },
          {
            "text": "ka anya gị hụ ihe ziri ezi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ọ bụ ezie na ị nyochaala obi m,",
            "poem": 1
          },
          {
            "text": "i letala m nʼabalị, nwalekwaa m,",
            "poem": 2
          },
          {
            "text": "ị ga-achọpụta na o nweghị ihe ọ ọjọọ bụla m zubere,",
            "poem": 1
          },
          {
            "text": "na ọnụ m ekwuhiebeghị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ndị mmadụ agbalịala inye m ngarị",
            "poem": 1
          },
          {
            "text": "ma anaghị m ekwe ka mụ na ndị na-eme ihe ike nwee mmekọrịta",
            "poem": 2
          },
          {
            "text": "nʼihi iwu sitere nʼegbugbere ọnụ gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Nzọ ụkwụ m akpafubeghị site nʼụzọ gị,",
            "poem": 1
          },
          {
            "text": "ụkwụ m agbụchapụghịkwa site nʼọnọdụ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ana m akpọku gị, O Chineke, nʼihi na amaara m na ị ga-aza m;",
            "poem": 1
          },
          {
            "text": "gee m ntị ma nụrụ ekpere m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Zipụta ịhụnanya gị dị oke ebube;",
            "poem": 1
          },
          {
            "text": "gị onye na-eji aka nri gị dị ike azọpụta",
            "poem": 2
          },
          {
            "text": "ndị niile gbabara na gị site nʼaka ndị iro ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Chebe m dịka mkpụrụ anya gị;",
            "poem": 1
          },
          {
            "text": "zoo m na ndo nke nku gị,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "site nʼaka ndị ajọ omume gbara m gburugburu,",
            "poem": 1
          },
          {
            "text": "ọ bụladị ndị iro m, ndị chọrọ igbu m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Ha anaghị eme ebere, ha bụ ndị afọ tara mmiri,",
            "poem": 1
          },
          {
            "text": "okwu ịnya isi juru ha ọnụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ha achọpụtala ebe m nọ, ugbu a, ha gbara m gburugburu,",
            "poem": 1
          },
          {
            "text": "ha ji anya nkọ, jikerekwa ịtụ m nʼala.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ha ka ọdụm agụụ na-agụ nke zoro onwe ya nʼọhịa,",
            "poem": 1
          },
          {
            "text": "dịka ọdụm dị ike na-eche ihe ọ ga-adọgbu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Onyenwe anyị, bilie guzogide ha, tigbuo ha.",
            "poem": 1
          },
          {
            "text": "Jiri mma agha gị zọpụta m site nʼaka ndị ajọ omume.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "O Onyenwe anyị, zọpụta m site nʼaka ndị dị otu a,",
            "poem": 1
          },
          {
            "text": "site nʼaka ndị ụgwọ ọrụ ha bụ nʼụwa a.",
            "poem": 2
          },
          {
            "text": "Ka ihe ịkpakọbara debere ndị ajọ omume jupụta afọ ha,",
            "poem": 1
          },
          {
            "text": "ka afọ jukwa ụmụ site nʼihe ndị a,",
            "poem": 2
          },
          {
            "text": "ka ha hapụrụ ihe fọdụrụ nye ụmụntakịrị ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Ma mụ onwe m ga-ahụ ihu gị site nʼezi omume;",
            "poem": 1
          },
          {
            "text": "mgbe m ga-ebili, afọ ga-eju m nke ọma, nʼihi na aga m ahụ gị ihu na ihu.",
            "poem": 2
          }
        ]
      }
    ],
