  "chapter": {
    "number": 148,
    "content": [
      {
        "type": "heading",
        "content": [
          "Toonu Onyenwe ihe niile"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Toonu Onyenwe anyị.",
            "poem": 1
          },
          {
            "lineBreak": true
          },
          {
            "text": "Toonu Onyenwe anyị site nʼeluigwe,",
            "poem": 1
          },
          {
            "text": "toonu ya nʼebe niile kachasị elu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Toonu ya, ndị mmụọ ozi ya niile,",
            "poem": 1
          },
          {
            "text": "toonu ya, unu usuu ndị agha ya niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Toonu ya, anyanwụ na ọnwa",
            "poem": 1
          },
          {
            "text": "toonu ya, kpakpando niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Toonu ya, unu eluigwe kachasị elu",
            "poem": 1
          },
          {
            "text": "na unu mmiri dị nʼelu mbara eluigwe.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ka ha niile too aha Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "nʼihi na ha niile malitere ịdị site nʼokwu ọnụ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "O doro ha nʼọnọdụ ha ga-anọ ruo mgbe ebighị ebi;",
            "poem": 1
          },
          {
            "text": "o nyere iwu nke a na-agaghị agabiga.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Toonu Onyenwe anyị, site nʼụwa,",
            "poem": 1
          },
          {
            "text": "unu oke anụ mmiri niile na ogbu mmiri niile,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "na amụma, na mkpụrụ mmiri, na snoo na igwe ojii niile,",
            "poem": 1
          },
          {
            "text": "na oke ifufe niile na-eme ihe o nyere nʼiwu,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "unu ugwu ukwu niile, na ugwu nta niile",
            "poem": 1
          },
          {
            "text": "na osisi niile na-amị mkpụrụ na osisi sida niile",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "na anụ ọhịa, na anụ ụlọ niile,",
            "poem": 1
          },
          {
            "text": "na ihe niile na-akpụ akpụ nʼala na nnụnụ niile,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "ndị eze niile nke ụwa na mba niile dị iche iche,",
            "poem": 1
          },
          {
            "text": "unu ụmụ eze na ndị niile na-achị achị nʼụwa,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "ụmụ okorobịa na ụmụ agbọghọ,",
            "poem": 1
          },
          {
            "text": "ndị okenye na ụmụntakịrị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Ka ha niile too aha Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "nʼihi na ọ bụ naanị aha ya ka e buliri elu;",
            "poem": 2
          },
          {
            "text": "ebube ya dị ukwuu karịa eluigwe na ụwa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "O meela ka mpi",
            "poem": 1
          },
          {
            "noteId": 1714
          },
          {
            "text": "ndị ya dị elu,",
            "poem": 1
          },
          {
            "text": "otuto ndị nsọ ya niile,",
            "poem": 2
          },
          {
            "text": "nke Izrel ndị ọ hụrụ nʼanya.",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Toonu Onyenwe anyị.",
            "poem": 1
          }
        ]
      }
    ],
