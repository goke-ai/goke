  "chapter": {
    "number": 93,
    "content": [
      {
        "type": "heading",
        "content": [
          "Ịdị ukwuu nke Onyenwe anyị"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Onyenwe anyị bụ eze, ọ na-eyikwasị ịma mma dịka uwe;",
            "poem": 1
          },
          {
            "text": "Onyenwe anyị na-eyikwasị ịma mma dịka uwe, ọ dịkwa ike nke ukwuu;",
            "poem": 2
          },
          {
            "text": "nʼezie, ụwa niile na-eguzosikwa ike; apụghịkwa iwezuga ya nʼọnọdụ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ocheeze gị eguzosiela ike siterị na mgbe ochie;",
            "poem": 1
          },
          {
            "text": "ị na-adịkwa site nʼebighị ebi ruo ebighị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Oke osimiri niile amaliela elu, O Onyenwe anyị;",
            "poem": 1
          },
          {
            "text": "oke osimiri niile eweliela olu ha elu;",
            "poem": 2
          },
          {
            "text": "ebili mmiri niile nke oke osimiri na-amali elu na-eme mkpọtụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Onyenwe anyị onye nọ nʼelu, dị ukwuu",
            "poem": 1
          },
          {
            "text": "karịa mkpọtụ nke oke osimiri niile na-eme,",
            "poem": 2
          },
          {
            "text": "ma karịakwa ụda nke ebili mmiri nke oke osimiri.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ụkpụrụ gị niile, Onyenwe anyị na-eguzosi ike;",
            "poem": 1
          },
          {
            "text": "ịdị nsọ bụ ihe e ji chọọ ụlọ gị mma",
            "poem": 2
          },
          {
            "text": "ruo ụbọchị na-enweghị ọgwụgwụ.",
            "poem": 2
          }
        ]
      }
    ],
