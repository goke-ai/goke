  "chapter": {
    "number": 66,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ dịrị onyeisi abụ. Otu Abụ. Abụ Ọma."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Abụ ekele"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ụwa niile, jirinụ ọṅụ tikuo Chineke!",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Bụọ abụ banyere ebube aha ya;",
            "poem": 2
          },
          {
            "text": "meenụ ka otuto ya bụrụ ihe dị ebube!",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Gwa Chineke okwu sị ya, “Lee ka ọrụ gị niile si dị oke egwu!",
            "poem": 1
          },
          {
            "text": "Ike gị dị nnọọ ukwu,",
            "poem": 2
          },
          {
            "text": "ya mere ndị iro gị, na-ada nʼihu gị nʼihi ụjọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ndị niile bi nʼụwa na-akpọ isiala nye gị;",
            "poem": 1
          },
          {
            "text": "ha na-abụku gị abụ otuto,",
            "poem": 2
          },
          {
            "text": "ha na-abụ abụ otuto nye aha gị.” Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Bịa lee ihe Chineke mere,",
            "poem": 1
          },
          {
            "text": "ụdị ọrụ oke egwu ọ na-arụ nʼihi mmadụ!",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "O mere ka osimiri ghọọ ala akọrọ,",
            "poem": 1
          },
          {
            "noteId": 1634
          },
          {
            "text": "ha ji ụkwụ si nʼetiti osimiri gafee",
            "poem": 2
          },
          {
            "noteId": 1635
          },
          {
            "text": ";",
            "poem": 2
          },
          {
            "text": "bịanụ ka anyị ṅụrịa nʼime ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ọ na-achị ruo mgbe ebighị ebi site nʼike ya,",
            "poem": 1
          },
          {
            "text": "anya ya na-elekwa mba niile,",
            "poem": 2
          },
          {
            "text": "ka ndị nnupu isi ghara ịfụlị onwe ha elu megide ya. Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Toonu Chineke anyị, unu mmadụ niile,",
            "poem": 1
          },
          {
            "text": "kwere ka a nụ ụda otuto ya;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "O chebela ndụ anyị,",
            "poem": 1
          },
          {
            "text": "ma mee ka ụkwụ anyị ghara ịmịchapụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Nʼihi na ọ bụ gị, O Chineke, nwara anyị;",
            "poem": 1
          },
          {
            "text": "ị nụchara anyị dịka ọlaọcha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ị kpọbatara anyị nʼụlọ mkpọrọ,",
            "poem": 1
          },
          {
            "text": "ma bokwasị ibu dị arọ nʼazụ anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "I kwere ka ndị mmadụ rịa anyị elu;",
            "poem": 1
          },
          {
            "text": "anyị esitela nʼime ọkụ na nʼime mmiri gafee.",
            "poem": 2
          },
          {
            "text": "Ị kpọbatala anyị ebe anyị nwezuru ihe niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Aga m e ji aja nsure ọkụ bata nʼime ụlọnsọ ukwu gị,",
            "poem": 1
          },
          {
            "text": "imezu nkwa m kwere gị",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "nʼihi na nke a bụ ihe m ji ọnụ m kwuo,",
            "poem": 1
          },
          {
            "text": "ọ bụkwa nke a ka egbugbere ọnụ m kwupụtara na m ga-eme nʼoge nsogbu m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Aga m eji anụ ndị gbara abụba chụọrọ gị aja,",
            "poem": 1
          },
          {
            "text": "ya na aja onyinye nke ọtụtụ ebule;",
            "poem": 2
          },
          {
            "text": "Aga m achụkwa aja onyinye nke ọtụtụ oke ehi na mkpi. Sela",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Bịanụ nụrụ, unu niile na-atụ egwu Chineke;",
            "poem": 1
          },
          {
            "text": "ka m kọọrọ unu ihe o meere m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Eji m ọnụ m kpọkuo ya;",
            "poem": 1
          },
          {
            "text": "otuto ya dị m nʼire.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Ọ bụrụ na mmehie na-amasị m nʼime obi m,",
            "poem": 1
          },
          {
            "text": "Onyenwe anyị agaraghị ege m ntị;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "ma Chineke gere ntị nʼezie,",
            "poem": 1
          },
          {
            "text": "nụrụkwa olu arịrịọ m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Otuto dịrị Chineke,",
            "poem": 1
          },
          {
            "text": "onye na-ajụghị ekpere m",
            "poem": 2
          },
          {
            "text": "maọbụ ghara izi m ịhụnanya ya!",
            "poem": 2
          }
        ]
      }
    ],
