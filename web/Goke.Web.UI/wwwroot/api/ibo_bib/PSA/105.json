  "chapter": {
    "number": 105,
    "content": [
      {
        "type": "heading",
        "content": [
          "Toonu Chineke onye na-edebe ọgbụgba ndụ"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Nyenụ Onyenwe anyị otuto, kwupụtakwa aha ya.",
            "poem": 1
          },
          {
            "text": "Meenụ ka amata nʼetiti mba niile banyere ihe niile ọ rụrụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Bụkuo ya abụ, bụkuo ya abụ otuto;",
            "poem": 1
          },
          {
            "text": "kwupụtanụ ọrụ ihe ịrịbama ya niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ṅụrianụ nʼime aha nsọ ya;",
            "poem": 1
          },
          {
            "text": "ka obi ndị niile na-achọ Onyenwe anyị ṅụrịa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Na-achọnụ Onyenwe anyị na ike ya,",
            "poem": 1
          },
          {
            "text": "chọọnụ ihu ya mgbe niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Chetanụ ọrụ ebube niile nke ọ rụrụ,",
            "poem": 1
          },
          {
            "text": "ihe ịrịbama ya niile na ikpe ziri ezi niile si nʼọnụ ya pụta,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "unu ụmụ ụmụ Ebraham, bụ ohu ya,",
            "poem": 1
          },
          {
            "text": "unu ụmụ ụmụ Jekọb, ndị ọ họpụtara.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ya onwe ya bụ Onyenwe anyị Chineke anyị;",
            "poem": 1
          },
          {
            "text": "ikpe ya dị nʼụwa niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ọ na-echeta ọgbụgba ndụ ya ruo mgbe ebighị ebi,",
            "poem": 1
          },
          {
            "text": "bụ nkwa o kwere nye puku ọgbọ,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "ọgbụgba ndụ ahụ nke ya na Ebraham gbara",
            "poem": 1
          },
          {
            "text": "na iyi ọ ṅụrụ nye Aịzik.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "O mere ka o guzoro nye Jekọb dịka ụkpụrụ,",
            "poem": 1
          },
          {
            "text": "ma nʼebe Izrel nọ, dịka ọgbụgba ndụ ebighị ebi:",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "“Aga m enye gị ala Kenan",
            "poem": 1
          },
          {
            "text": "dịka oke i ga-eketa.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Mgbe ha dị ole na ole nʼọnụọgụgụ,",
            "poem": 1
          },
          {
            "text": "nʼezie ha dị ole na ole, bụrụkwa ọbịa nʼime ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Ha wagharịrị site nʼotu mba gaa na mba ọzọ,",
            "poem": 1
          },
          {
            "text": "sitekwa nʼotu alaeze gaa alaeze ọzọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "O kweghị ka onye ọbụla mekpaa ha ahụ;",
            "poem": 1
          },
          {
            "text": "ọ baara ndị eze mba nʼihi ha;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "sị, “Unu emetụkwala ndị m e tere mmanụ aka;",
            "poem": 1
          },
          {
            "text": "unu emekpakwala ndị amụma m ahụ.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "O zitere ụnwụ nʼala ahụ,",
            "poem": 1
          },
          {
            "text": "bibie ụzọ niile ha si enweta ihe oriri;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "o zipụrụ otu nwoke ka ọ gaa nʼihu ha,",
            "poem": 1
          },
          {
            "text": "Josef, onye e refuru dịka ohu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Ha ji mkpọrọ igwe merụọ ya ahụ nʼụkwụ,",
            "poem": 1
          },
          {
            "text": "nyanyekwa ya mkpọrọ igwe nʼolu,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "tutu ruo mgbe ihe ahụ o kwuru na ọ ga-eme mezuru,",
            "poem": 1
          },
          {
            "text": "tutu ruo mgbe okwu Onyenwe anyị nwapụtara ya dịka onye eziokwu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Eze ahụ zipụrụ ozi ka a tọpụ ya,",
            "poem": 1
          },
          {
            "text": "onye ahụ na-achị ndị dị iche iche mere ka o nwere onwe ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "O mere ya ka ọ bụrụ onyenwe ezinaụlọ ya,",
            "poem": 1
          },
          {
            "text": "onye na-achị ihe niile o nwere,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "ikuziri ụmụ eze ndị ikom dịka o si masị ya,",
            "poem": 1
          },
          {
            "text": "na izi ndị okenye ya amamihe.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 23,
        "content": [
          {
            "text": "Mgbe ahụ, Izrel bara nʼala Ijipt;",
            "poem": 1
          },
          {
            "text": "Jekọb bikwara nʼala Ham dịka onye si mba ọzọ bịa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 24,
        "content": [
          {
            "text": "Onyenwe anyị mere ka ndị ya mụbaa nke ukwuu;",
            "poem": 1
          },
          {
            "text": "o mere ka ndị ya dị ukwuu nʼọnụọgụgụ karịa ndị iro ha,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 25,
        "content": [
          {
            "text": "ndị ọ tụgharịrị obi ha ka ha kpọọ ndị ya asị,",
            "poem": 1
          },
          {
            "text": "ka ha gbaa izu nzuzo megide ndị ohu ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 26,
        "content": [
          {
            "text": "O zipụrụ Mosis, ohu ya,",
            "poem": 1
          },
          {
            "text": "na Erọn, onye ọ họpụtara.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 27,
        "content": [
          {
            "text": "Ha mere ọtụtụ ihe ịrịbama dị iche iche nʼetiti ha,",
            "poem": 1
          },
          {
            "text": "ọrụ ebube ya dị iche iche nʼala Ham.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 28,
        "content": [
          {
            "text": "O zipụrụ ọchịchịrị, mee ka ala ahụ gbaa ọchịchịrị,",
            "poem": 1
          },
          {
            "text": "nʼihi na ha nupuru isi megide okwu ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 29,
        "content": [
          {
            "text": "O mere ka mmiri ha niile ghọọ ọbara,",
            "poem": 1
          },
          {
            "text": "nke mere ka azụ niile dị nʼime ya nwụsịa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 30,
        "content": [
          {
            "text": "Ala ha niile jupụtara nʼawọ",
            "poem": 1
          },
          {
            "text": "ọ bụladị nʼime ọnụụlọ ndina nke ndị na-achị achị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 31,
        "content": [
          {
            "text": "O kwuru okwu, igwe ijiji na anwụ nta",
            "poem": 1
          },
          {
            "text": "dị iche iche ejupụta nʼala ha niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 32,
        "content": [
          {
            "text": "Nʼọnọdụ mmiri ozuzo, o zitere ha",
            "poem": 1
          },
          {
            "text": "mkpụrụ mmiri na amụma egbe eluigwe nʼala ha niile;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 33,
        "content": [
          {
            "text": "o tijisịrị osisi vaịnị na osisi fiig niile ha,",
            "poem": 1
          },
          {
            "text": "kwatusịakwa osisi niile dị nʼala ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 34,
        "content": [
          {
            "text": "O kwuru okwu, igurube na ụkpana",
            "poem": 1
          },
          {
            "text": "a na-apụghị ịgụta ọnụ jupụtara ebe niile;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 35,
        "content": [
          {
            "text": "ha richapụrụ ihe niile bụ ahịhịa ndụ dị nʼala ahụ,",
            "poem": 1
          },
          {
            "text": "ripịakwa mkpụrụ niile ha kụrụ nʼubi ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 36,
        "content": [
          {
            "text": "Mgbe ahụ, o gburu ndị niile e buru ụzọ mụọ nʼala ahụ,",
            "poem": 1
          },
          {
            "text": "mkpụrụ mbụ nke ndị ikom ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 37,
        "content": [
          {
            "text": "Ọ kpọpụtara ụmụ Izrel, ndị chịịrị ọlaọcha na ọlaedo,",
            "poem": 1
          },
          {
            "text": "ọ dịghị otu onye nʼime ebo ya niile nke ahụ na-esighị ike.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 38,
        "content": [
          {
            "text": "Obi dị Ijipt ụtọ mgbe ha pụrụ,",
            "poem": 1
          },
          {
            "text": "nʼihi na oke egwu ndị Izrel dakwasịrị ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 39,
        "content": [
          {
            "text": "Ọ gbasara igwe ojii dịka ihe mkpuchi,",
            "poem": 1
          },
          {
            "text": "na ọkụ na-enye ìhè nʼabalị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 40,
        "content": [
          {
            "text": "Ha rịọrọ ya, o zitere ha ụmụ nnụnụ kweel,",
            "poem": 1
          },
          {
            "text": "jirikwa achịcha si nʼeluigwe nyejuo ha afọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 41,
        "content": [
          {
            "text": "O tiwara nkume ahụ, mmiri sọpụtara;",
            "poem": 1
          },
          {
            "text": "ọ sọgharịrị nʼime ọzara dịka iyi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 42,
        "content": [
          {
            "text": "Nʼihi na o chetara nkwa ya dị nsọ,",
            "poem": 1
          },
          {
            "text": "nke o kwere ohu ya Ebraham.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 43,
        "content": [
          {
            "text": "O ji aṅụrị dupụta ndị ya,",
            "poem": 1
          },
          {
            "text": "jirikwa ọnọdụ iti mkpu ọṅụ dupụta ndị ya ọ họpụtara;",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 44,
        "content": [
          {
            "text": "o nyere ha ala niile nke mba dị iche iche,",
            "poem": 1
          },
          {
            "text": "ha ghọrọ ndị nnweta nke ihe ndị ọzọ rụpụta,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 45,
        "content": [
          {
            "text": "ka ha nwee ike debe ụkpụrụ ya niile",
            "poem": 1
          },
          {
            "text": "ma na-erubekwa isi nʼiwu ya niile.",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Toonu Onyenwe anyị.",
            "poem": 1
          }
        ]
      }
    ],
