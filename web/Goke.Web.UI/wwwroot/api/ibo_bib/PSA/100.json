  "chapter": {
    "number": 100,
    "content": [
      {
        "type": "hebrew_subtitle",
        "content": [
          "Abụ Ọma maka ikele ekele afọ ojuju."
        ]
      },
      {
        "type": "heading",
        "content": [
          "Toonu Chineke"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Tikusienụ Onyenwe anyị mkpu ọṅụ ike, ụwa niile.",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Jiri obi ụtọ fee Onyenwe anyị ofufe;",
            "poem": 2
          },
          {
            "text": "jirinụ ịbụ abụ ọṅụ bịa nʼihu ya",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Matanụ na Onyenwe anyị bụ Chineke.",
            "poem": 1
          },
          {
            "text": "Ọ bụ ya onwe ya kere anyị,",
            "poem": 2
          },
          {
            "noteId": 1673
          },
          {
            "text": "ọ bụkwa ya nwe anyị,",
            "poem": 2
          },
          {
            "text": "anyị bụ ndị ya, igwe atụrụ nke ọ na-azụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Jirinụ ekele banye nʼọnụ ụzọ ama ya",
            "poem": 1
          },
          {
            "text": "jiri otuto baa nʼogige ya;",
            "poem": 2
          },
          {
            "text": "keleenụ ya, tookwa aha ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Nʼihi na Onyenwe anyị dị mma, ịhụnanya ya na-adịgide ruo mgbe ebighị ebi.",
            "poem": 1
          },
          {
            "text": "Ruo ọgbọ niile ka ikwesi ntụkwasị obi ya na-adịgidekwa.",
            "poem": 2
          }
        ]
      }
    ],
