  "chapter": {
    "number": 114,
    "content": [
      {
        "type": "heading",
        "content": [
          "Abụ ncheta ngabiga"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Mgbe ụmụ Izrel si nʼala Ijipt pụta,",
            "poem": 1
          },
          {
            "text": "mgbe ezinaụlọ Jekọb sitekwara nʼebe ndị asụsụ ha dị iche pụta,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Juda ghọrọ ebe nsọ Chineke,",
            "poem": 1
          },
          {
            "text": "Izrel ghọrọ ala ọ na-achị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Osimiri hụrụ ya, gbaa ọsọ,",
            "poem": 1
          },
          {
            "noteId": 1680
          },
          {
            "text": "osimiri Jọdan hụkwara ya chigharịa laa azụ",
            "poem": 2
          },
          {
            "noteId": 1681
          },
          {
            "text": ";",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ugwu ukwu niile wụliri elu dịka ebule;",
            "poem": 1
          },
          {
            "text": "ugwu nta niile wụlikwara elu dịka ụmụ atụrụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Gịnị mere gị oke osimiri, i ji na-agba ọsọ?",
            "poem": 1
          },
          {
            "text": "Osimiri Jọdan, gịnị na-eme gị i ji chigharịa laa azụ?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Gịnị na-eme unu, ugwu ukwu niile unu ji na-awụli elu dịka ụmụ ebule?",
            "poem": 1
          },
          {
            "text": "Gịnị na-emekwa unu, ugwu nta niile, unu ji na-awụli elu dịka ụmụ atụrụ?",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ka ụwa niile maa jijiji nʼihu Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "ka ọ maakwa jijiji nʼihu Chineke nke Jekọb,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "onye mere ka oke nkume ghọọ ọdọ mmiri,",
            "poem": 1
          },
          {
            "text": "onye mere ka ọmụ nkume ghọọ isi iyi mmiri.",
            "poem": 2
          },
          {
            "noteId": 1682
          }
        ]
      }
    ],
