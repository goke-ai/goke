  "chapter": {
    "number": 135,
    "content": [
      {
        "type": "heading",
        "content": [
          "Toonu Onyenwe anyị"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Toonu Onyenwe anyị.",
            "poem": 1
          },
          {
            "lineBreak": true
          },
          {
            "text": "Toonu aha Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "toonu ya, unu ndị ohu Onyenwe anyị niile,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "unu ndị na-eje ozi nʼime ụlọ Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "nʼime ogige ụlọ Chineke anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Toonu Onyenwe anyị, nʼihi na Onyenwe anyị dị mma;",
            "poem": 1
          },
          {
            "text": "bụkuo aha ya abụ otuto, nʼihi na ọ bụ ihe ziri ezi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Nʼihi na Onyenwe anyị ahọpụtala Jekọb nye onwe ya,",
            "poem": 1
          },
          {
            "text": "ọ họpụtala Izrel ka ọ bụrụ ihe oke ọnụahịa nke aka ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Amaara m na Onyenwe anyị dị ukwuu,",
            "poem": 1
          },
          {
            "text": "na Onyenwe anyị dị ukwuu karịa chi niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Onyenwe anyị na-eme ihe ọbụla masịrị ya",
            "poem": 1
          },
          {
            "text": "nʼeluigwe na elu ụwa",
            "poem": 2
          },
          {
            "text": "na nʼoke osimiri na nʼime ogbu mmiri niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ọ bụ ya na-eme ka igwe ojii na-apụta site nʼakụkụ niile nke ụwa.",
            "poem": 1
          },
          {
            "text": "Ọ bụkwa ya na-ezite amụma nke na-eweta mmiri ozuzo.",
            "poem": 2
          },
          {
            "text": "Ọ na-ewepụtakwa ifufe site nʼụlọ ifufe ya niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "O tigburu ụmụ niile e buru ụzọ mụọ nʼala Ijipt,",
            "poem": 1
          },
          {
            "text": "ụmụ mmadụ buru ụzọ mụọ na ụmụ anụ ụlọ e buru ụzọ mụọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "O zipụrụ ihe ịrịbama ya dị iche iche na ọrụ ebube ya niile nʼetiti unu, o Ijipt,",
            "poem": 1
          },
          {
            "text": "megide Fero na ndị niile na-ejere ya ozi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "O tigburu ọtụtụ mba dị ukwuu nʼọnụọgụgụ,",
            "poem": 1
          },
          {
            "text": "tigbukwaa ndị eze ha, ndị dị ike nʼagha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ụfọdụ nʼime ha bụ Saịhọn, eze ndị Amọrait,",
            "poem": 1
          },
          {
            "text": "na Ọg eze Bashan,",
            "poem": 2
          },
          {
            "text": "na ndị eze niile dị na Kenan.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "O weere ala ndị obodo ahụ niile nye ndị ya,",
            "poem": 1
          },
          {
            "text": "nye ya ụmụ Izrel dịka ihe nketa ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "O Onyenwe anyị, aha gị na-adịgide ruo mgbe ebighị ebi",
            "poem": 1
          },
          {
            "text": "a maara aha gị ruo ọgbọ niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Nʼihi na Onyenwe anyị ga-ekpepụta ndị ya.",
            "poem": 1
          },
          {
            "text": "Ọ ga-enwekwa ọmịiko nʼahụ ndị ohu ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Chi ndị mba ọzọ bụ arụsị e jiri aka kpụọ,",
            "poem": 1
          },
          {
            "text": "nke e ji ọlaọcha na ọlaedo kpụọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Ha nwere ọnụ ma ha adịghị ekwu okwu,",
            "poem": 1
          },
          {
            "text": "ha nwere anya ma ha adịghị ahụ ụzọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Ha nwere ntị ma ha adịghị anụ ihe,",
            "poem": 1
          },
          {
            "text": "ọ dịkwaghị nkuume dị nʼime ọnụ ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Ndị na-eme ha ga-adị ka ha.",
            "poem": 1
          },
          {
            "text": "Ọ bụkwa otu a ka ndị na-atụkwasị ha obi ga-adị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Ndị Izrel, toonu Onyenwe anyị.",
            "poem": 1
          },
          {
            "text": "Unu ndị ụlọ Erọn, toonu Onyenwe anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Unu ndị ụlọ Livayị, toonu Onyenwe anyị;",
            "poem": 1
          },
          {
            "text": "unu ndị na-atụ egwu ya, toonu Onyenwe anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Ka otuto dịrị Onyenwe anyị site na Zayọn,",
            "poem": 1
          },
          {
            "text": "nye onye ahụ na-ebi na Jerusalem.",
            "poem": 2
          },
          {
            "lineBreak": true
          },
          {
            "text": "Toonu Onyenwe anyị.",
            "poem": 1
          }
        ]
      }
    ],
