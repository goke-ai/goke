  },
  "chapter": {
    "number": 4,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Leenụ ka i si maa mma, onye m hụrụ nʼanya,",
            "poem": 1
          },
          {
            "text": "e, ị mara mma!",
            "poem": 2
          },
          {
            "noteId": 1847
          },
          {
            "text": "Anya gị abụọ nke dị nʼazụ akwa mkpuchi ihu gị dị ka nduru.",
            "poem": 2
          },
          {
            "text": "Agịrị isi gị dị ka igwe ewu",
            "poem": 1
          },
          {
            "noteId": 1848
          },
          {
            "text": "nke na-arịda nʼakụkụ ugwu Gilead.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Eze gị dị ka igwe atụrụ a ka kpachapụrụ ajị ha ọhụrụ,",
            "poem": 1
          },
          {
            "text": "nke si nʼebe a sapụrụ ha ahụ na-arịpụta.",
            "poem": 2
          },
          {
            "text": "Nke ọbụla chi ụmụ abụọ,",
            "poem": 1
          },
          {
            "text": "ọ dịghịkwa nke gba aka nwa nʼetiti ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Egbugbere ọnụ gị abụọ dị ka ogho uhie.",
            "poem": 1
          },
          {
            "text": "Ọnụ gị dị mma ile anya.",
            "poem": 2
          },
          {
            "text": "Agba nti gị abụọ nke dị nʼime akwa mkpuchi gị",
            "poem": 1
          },
          {
            "text": "dị ka pomegranet",
            "poem": 2
          },
          {
            "noteId": 1849
          },
          {
            "text": "e kere abụọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Olu gị dị ka ụlọ elu Devid,",
            "poem": 1
          },
          {
            "text": "nke e wuziri nke ọma.",
            "poem": 2
          },
          {
            "text": "E konyere puku ọta na ya,",
            "poem": 1
          },
          {
            "text": "ha niile bụ ọta nke ndị ọka nʼagha.",
            "poem": 2
          },
          {
            "noteId": 1850
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ara gị abụọ dị ka ụmụ mgbada abụọ,",
            "poem": 1
          },
          {
            "text": "nke otu nne mụrụ nʼotu oge,",
            "poem": 2
          },
          {
            "text": "ndị na-ata nri nʼetiti okoko urodi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Tutu chi ụtụtụ abọọ,",
            "poem": 1
          },
          {
            "text": "tutu onyinyo agbalaga,",
            "poem": 2
          },
          {
            "text": "aga m aga nʼugwu ụda máá",
            "poem": 1
          },
          {
            "text": "na nʼugwu insensi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ị mara mma nke ukwuu, onye m hụrụ nʼanya,",
            "poem": 1
          },
          {
            "text": "ọ dịkwaghị ntụpọ",
            "poem": 2
          },
          {
            "noteId": 1851
          },
          {
            "text": "ọbụla dị gị nʼahụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Si na Lebanọn soro m, nwunye m lụrụ ọhụrụ.",
            "poem": 1
          },
          {
            "text": "Si na Lebanọn bịa soro m;",
            "poem": 2
          },
          {
            "text": "rịdata site nʼugwu Amana,",
            "poem": 1
          },
          {
            "text": "nʼugwu Senia, na nʼugwu Hemon,",
            "poem": 2
          },
          {
            "text": "site nʼọgba ọdụm",
            "poem": 1
          },
          {
            "text": "nʼugwu niile ebe agụ bi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "I riela m obi, nwanne m nwanyị, nwunye ọhụrụ m;",
            "poem": 1
          },
          {
            "text": "i riela m obi,",
            "poem": 2
          },
          {
            "text": "i werela ile anya gị",
            "poem": 1
          },
          {
            "text": "na ihe olu i yi rie obi m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Lee ka ịhụnanya gị si dị ụtọ, nwanne m nwanyị, nwunye ọhụrụ m.",
            "poem": 1
          },
          {
            "text": "Lee ka ịhụnanya gị si dị mma nke ukwuu karịa mmanya,",
            "poem": 2
          },
          {
            "text": "isisi ụtọ nke mmanụ otite gị",
            "poem": 1
          },
          {
            "text": "na-esi isi ọma karịa mmanụ ụda ọbụla.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Egbugbere ọnụ gị dị ụtọ dịka mmanụ aṅụ dị, nwunye ọhụrụ m,",
            "poem": 1
          },
          {
            "text": "mmiri ara ehi na mmanụ aṅụ dị nʼokpuru ire gị,",
            "poem": 2
          },
          {
            "text": "isisi ụtọ nke uwe gị",
            "poem": 1
          },
          {
            "text": "na-esi ka Lebanọn",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ubi a gbara ogige, ma gbachiekwa, ka nwanne m nwanyị, nwunye ọhụrụ bụ.",
            "poem": 1
          },
          {
            "text": "Ị bụ isi iyi nke a gbara ogige, rachiekwa.",
            "poem": 2
          },
          {
            "noteId": 1852
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Ihe ọkụkọ dị nʼime ogige gị bụ pomegranet",
            "poem": 1
          },
          {
            "text": "nwere ezi mkpụrụ dị oke ọnụahịa,",
            "poem": 2
          },
          {
            "text": "nke nwere henna na naad,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "naad na safron,",
            "poem": 2
          },
          {
            "text": "kalamus na sinamọn,",
            "poem": 2
          },
          {
            "text": "na osisi nke ihe nsure ọkụ na-esi isi ụtọ,",
            "poem": 2
          },
          {
            "text": "na ụda máá, na aloos",
            "poem": 2
          },
          {
            "text": "na ụda kachasị mma dị iche iche na-esi isi ụtọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "I bụ isi iyi nke ubi a gbara ogige,",
            "poem": 1
          },
          {
            "text": "olulu mmiri dị ndụ",
            "poem": 2
          },
          {
            "text": "nke si na Lebanọn asọdata.",
            "poem": 2
          },
          {
            "noteId": 1853
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Teta nʼụra gị ifufe si nʼugwu;",
            "poem": 1
          },
          {
            "noteId": 1854
          },
          {
            "text": "bịakwa gị ifufe si na ndịda,",
            "poem": 2
          },
          {
            "text": "fekwasị ubi m a gbara ogige,",
            "poem": 1
          },
          {
            "text": "ka isisi ụda ya dị ụtọ fesaa ebe niile,",
            "poem": 2
          },
          {
            "text": "ka onye m hụrụ nʼanya bịa nʼubi ya a gbara ogige,",
            "poem": 1
          },
          {
            "text": "ka ọ bịa rie mkpụrụ dị iche iche dị nʼime ya nke dị oke ọnụahịa.",
            "poem": 2
          }
        ]
      }
    ],
    "footnotes": [
      {
