  "chapter": {
    "number": 41,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "“Ị pụrụ iji nko azụ jide Leviatan",
            "poem": 1
          },
          {
            "text": "maọbụ jiri ụdọ kegide ire ya bịada ya?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ị pụrụ ikenye eriri nʼimi ya",
            "poem": 1
          },
          {
            "text": "maọbụ were ngu igwe mapuo agba ya?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ọ ga-arịọ gị arịrịọ ka i meere ya ebere?",
            "poem": 1
          },
          {
            "text": "Ọ ga-eji olu dị nro gwa gị okwu?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ọ ga-ekwe ka gị na ya baa nʼọgbụgba ndụ",
            "poem": 1
          },
          {
            "text": "ka i were ya dịka ohu gị ogologo ndụ ya niile?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ị ga-eji ya gwurie egwu dịka nnụnụ?",
            "poem": 1
          },
          {
            "text": "Maọbụ kee ya eriri nye ya ụmụ agbọghọ nọ nʼụlọ gị?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ndị ahịa ha ga-eji ya zụọ ahịa?",
            "poem": 1
          },
          {
            "text": "Ha ga-eke ya nʼetiti ndị mgbereahịa?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ị pụrụ iji igwe nko mejuo akpụkpọ ahụ ya",
            "poem": 1
          },
          {
            "text": "maọbụ iji ùbe ndị ọkụ azụ dupuo isi ya?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ọ bụrụ na ị tụkwasị ya aka,",
            "poem": 1
          },
          {
            "text": "ị gaghị echefu ọgụ ọ ga-alụso gị. Ị gakwaghị anwa ya ọzọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Ọ bụ ime ihe nzuzu bụ mmadụ ịgbalị ijide ya.",
            "poem": 1
          },
          {
            "text": "Naanị ilekwasị ya anya na-eyi egwu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Nʼihi nke a, o nweghị onye nwere obi ike ịkpasu ya iwe.",
            "poem": 1
          },
          {
            "text": "Ọ bụ onye pụrụ iguzogide m?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Olee onye m ji ụgwọ nke m na-aghaghị ịkwụ?",
            "poem": 1
          },
          {
            "text": "Ihe niile dị nʼokpuru eluigwe bụ nke m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "“Agaghị m akwụsị ikwu ihe banyere ụkwụ na aka Leviatan,",
            "poem": 1
          },
          {
            "text": "na ike ya, na ụdịdị ya mara mma.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Onye nwere ike ikpughepụ ihe e ji kpuchie ya ahụ?",
            "poem": 1
          },
          {
            "text": "Onye nwekwara ike ịbanye nʼagba ya apịajiri okpukpu abụọ?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Onye pụrụ imeghe ụzọ nke ọnụ ya,",
            "poem": 1
          },
          {
            "text": "nke eze dị oke egwu gbara gburugburu?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Azụ ya nwere ọta",
            "poem": 1
          },
          {
            "noteId": 1132
          },
          {
            "text": "ndị e doro nʼusoro",
            "poem": 1
          },
          {
            "text": "nke e mechisịrị ike ka ha rapara nʼahụ ibe ha,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "nke ọbụla dị ibe ya nso,",
            "poem": 1
          },
          {
            "text": "na ikuku enweghị ike gafee nʼetiti ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "E jikọtara ha ọnụ nke ọma,",
            "poem": 1
          },
          {
            "text": "ha na-ejisi onwe ha ike, enweghị ike ikewa ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Ìhè na-enwupụta mgbe ọbụla o zere uzere;",
            "poem": 1
          },
          {
            "text": "anya ya abụọ na-achakwa dịka anwụ ụtụtụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Ire ọkụ na-asọpụta site nʼọnụ ya,",
            "poem": 1
          },
          {
            "text": "icheku ọkụ na-ekepụtakwa nʼọnụ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Anwụrụ ọkụ na-esi nʼimi ya na-apụta,",
            "poem": 1
          },
          {
            "text": "dịka ukuru ọkụ nke na-esi nʼime mmiri na-agbọ nʼelu osisi achara.",
            "poem": 2
          },
          {
            "noteId": 1133
          }
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Ume ọ na-ekupụta na-afụnwu ọkụ icheku.",
            "poem": 1
          },
          {
            "text": "Ire ọkụ na-esitekwa nʼọnụ ya na-amapụ dịka àkụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "Ike nwere ọnọdụ nʼolu ya,",
            "poem": 1
          },
          {
            "text": "obi ịlọ mmiri na-aga ya nʼihu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 23,
        "content": [
          {
            "text": "Akpụkpọ anụ ahụ ya bụ ihe jikọtara ọnụ nke ọma,",
            "poem": 1
          },
          {
            "text": "ha siri ike na-apụghị inugharị ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 24,
        "content": [
          {
            "text": "Obi ya dị ike dịka nkume,",
            "poem": 1
          },
          {
            "text": "dịka nkume e ji akwọ ihe.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 25,
        "content": [
          {
            "text": "Mgbe o bilitere ọtọ, ndị dike na-atụ oke ụjọ;",
            "poem": 1
          },
          {
            "text": "ha na-alaghachi azụ nʼihi mmagharị ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 26,
        "content": [
          {
            "text": "O nweghị mma agha ọbụla pụrụ igbochi ya,",
            "poem": 1
          },
          {
            "text": "o nwekwaghị ùbe igwe, maọbụ àkụ maọbụ ihe ọbụla a pịrị ọnụ ya apị nke pụrụ igbochi ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 27,
        "content": [
          {
            "text": "Nʼebe ọ nọ, igwe dị ka ahịhịa;",
            "poem": 1
          },
          {
            "text": "bronz dịkwa ka osisi rere ure nʼebe ọ nọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 28,
        "content": [
          {
            "text": "Àkụ adịghị eme ka ọ gbalaga,",
            "poem": 1
          },
          {
            "text": "ịtụ ya nkume dị ka ịtụ ya igbugbo ọka.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 29,
        "content": [
          {
            "text": "Mkpọrọ osisi na-adị ya ka ahịhịa ọka,",
            "poem": 1
          },
          {
            "text": "ọ na-achị ọchị na mkpọtụ arụa na-eme.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 30,
        "content": [
          {
            "text": "Okpuru afọ ya dị ka ejuju dị nkọ,",
            "poem": 1
          },
          {
            "text": "nke na-ahapụ ụzọ nʼapịtị dịka igwe eji azọcha mkpụrụ ọka site nʼogbe ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 31,
        "content": [
          {
            "text": "Ọ na-eme mmiri dị ogbu ka ọ gbọọ dịka ite;",
            "poem": 1
          },
          {
            "text": "ọ na-akpagharị oke osimiri ka ọ dị ka mmanụ otite dị nʼite.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 32,
        "content": [
          {
            "text": "Ọ na-ahapụ ụfụfụ nʼazụ mgbe ọ na-aga;",
            "poem": 1
          },
          {
            "text": "mmadụ ga-eche na ọ osimiri nwere isi awọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 33,
        "content": [
          {
            "text": "O nweghị ihe ọzọ dị nʼụwa yiri ya,",
            "poem": 1
          },
          {
            "text": "o nweghị ihe na-emenye ya ụjọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 34,
        "content": [
          {
            "text": "Ọ na-eleda ndị mpako niile anya.",
            "poem": 1
          },
          {
            "text": "Ọ bụkwa eze nʼebe ndị na-anya isi nọ.”",
            "poem": 2
          }
        ]
      }
    ],
