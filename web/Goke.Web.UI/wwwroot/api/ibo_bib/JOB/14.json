  "chapter": {
    "number": 14,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "“Mmadụ nke nwanyị mụrụ",
            "poem": 1
          },
          {
            "text": "nwere ụbọchị ole na ole nke jupụtara na nsogbu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ọ na-awalite dịka okoko osisi, ma kpọnwụọkwa mgbe na-adịghị anya,",
            "poem": 1
          },
          {
            "text": "dịka onyinyo, ọ na-agabiga agabiga.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ị na-elegide onye dị otu a anya?",
            "poem": 1
          },
          {
            "text": "Ị ga-akpọ ya ikpe nʼihu gị?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Onye pụrụ iwepụta ihe dị ọcha site nʼihe na-adịghị ọcha?",
            "poem": 1
          },
          {
            "text": "Ọ dịghị onye ọ bụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ị kpebiela ụbọchị ndụ nke mmadụ;",
            "poem": 1
          },
          {
            "text": "ị kabiela ọnụọgụgụ nke ọnwa ya niile,",
            "poem": 2
          },
          {
            "text": "kpaakwa oke ebe ọ na-agaghị agabiga.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Nʼihi nke a, wepụ anya gị nʼebe ọ nọ, hapụ ya ka ọ nọọrọ onwe ya",
            "poem": 1
          },
          {
            "text": "ruo mgbe ụbọchị ya ga-ezu dịka onye e goro ọrụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "“Ma olileanya dịịrị osisi;",
            "poem": 1
          },
          {
            "text": "nʼihi na ọ bụrụ na e gbutuo ya, ọ ga-epuchikwa,",
            "poem": 2
          },
          {
            "text": "alaka ọhụrụ ya aghaghị ịwapụtakwa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "A sịkwarị na mgbọrọgwụ ya emeela ochie nʼala,",
            "poem": 1
          },
          {
            "text": "maọbụ na ogwe ya anwụọ nʼaja,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "mgbe ọbụla mmiri zokwasịrị ya,",
            "poem": 1
          },
          {
            "text": "ọ na-epupụta dịka ihe a kụrụ akụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Ma mmadụ na-anwụ, e lie ya;",
            "poem": 1
          },
          {
            "text": "ọ na-ekubikwa ume si otu a gabiga.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Dịka mmiri dị nʼoke osimiri si ata,",
            "poem": 1
          },
          {
            "text": "maọbụ ka mmiri dị nʼiyi si ata nʼoge ọkọchị,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "otu a ka mmadụ si e dinaa ma ọ dịghị etetakwa ọzọ;",
            "poem": 1
          },
          {
            "text": "tutu ruo mgbe eluigwe na-agaghị adịkwa; mmadụ ọbụla apụghị i si nʼụra kpọtee ya,",
            "poem": 2
          },
          {
            "text": "agaghị akpọtekwa ha site nʼụra ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "“A sị na ị ga-ezo m nʼala mmụọ,",
            "poem": 1
          },
          {
            "text": "na ị ga-ezobe m tutu ruo mgbe iwe gị gabigara.",
            "poem": 2
          },
          {
            "text": "A sị na ị ga-akanyere m oge",
            "poem": 1
          },
          {
            "text": "ma mesịa cheta m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Ọ bụrụ na mmadụ anwụọ, ọ ga-adị ndụ ọzọ?",
            "poem": 1
          },
          {
            "text": "Ụbọchị niile nke ọrụ ahụhụ m,",
            "poem": 2
          },
          {
            "text": "aga m echere mgbe nnapụta m ga-abịa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Ị ga-akpọ, aga m azakwa gị,",
            "poem": 1
          },
          {
            "text": "agụụ ọrụ aka gị ga-agụ gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Nʼihi mgbe ahụ ị ga-agụ nzọ ụkwụ m niile ọnụ,",
            "poem": 1
          },
          {
            "text": "ma ị gaghị agụkọ mmehie m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "A ga-ekechi njehie m nʼakpa,",
            "poem": 1
          },
          {
            "text": "ị ga-ekpuchi mmehie m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "“Ma dịka ugwu si erichasi daa,",
            "poem": 1
          },
          {
            "text": "na dịka a na-enupu oke nkume nʼọnọdụ ya,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "dịka mmiri si akwọchasị nkume,",
            "poem": 1
          },
          {
            "text": "na dịka oke mmiri ozuzo si akwazekwa aja ala,",
            "poem": 2
          },
          {
            "text": "otu a ka i si eme ka olileanya mmadụ laa nʼiyi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Ị na-egosi ya na ị karịrị ya ike, ma ha na-agabiga;",
            "poem": 1
          },
          {
            "text": "ị na-eme ka ihu ya gbanwee, ma zilaga ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Ọ bụrụ na a na-asọpụrụ ụmụ ha, ha adịghị amata nke a;",
            "poem": 1
          },
          {
            "text": "ọ bụrụkwa na a na-eweda ha nʼala, ha ọ dịghị ahụ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "Ihe mgbu nke anụ ahụ ya ka ọ na-ama",
            "poem": 1
          },
          {
            "text": "ma na-erukwara naanị onwe ya ụjụ.”",
            "poem": 2
          }
        ]
      }
    ],
