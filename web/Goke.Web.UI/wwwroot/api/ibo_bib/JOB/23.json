  "chapter": {
    "number": 23,
    "content": [
      {
        "type": "heading",
        "content": [
          "Okwu Job"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          "Mgbe ahụ, Job zara sị:"
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "“Ọ bụladị taa, mkpesa m dị ilu;",
            "poem": 1
          },
          {
            "text": "aka ya dị arọ megide m nʼagbanyeghị ịsụ ude m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "A sịkwa na m maara ebe m ga-achọta ya;",
            "poem": 1
          },
          {
            "text": "a sịkwa na m nwere ike ijeru ebe obibi ya!",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Aga m edozi mkpesa m nʼusoro nʼihu ya",
            "poem": 1
          },
          {
            "text": "were ịrụ ụka mejuo ọnụ m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Aga m achọpụta ihe o ga-aza m",
            "poem": 1
          },
          {
            "text": "ma tuleekwa ihe ọ ga-agwa m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ọ ga-eji ike dị egwu megide m?",
            "poem": 1
          },
          {
            "text": "Mba, ọ gaghị ebo m ebubo.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Nʼebe ahụ, onye ziri ezi ga-eme mkpesa nʼihu ya.",
            "poem": 1
          },
          {
            "text": "Nʼebe ahụ, a ga-anapụtakwa m site nʼaka onye ikpe m rụọ mgbe ebighị ebi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "“Ma ọ bụrụ na m agaa nʼọwụwa anyanwụ ọ nọghị nʼebe ahụ;",
            "poem": 1
          },
          {
            "text": "ọ bụrụ na m gaa nʼọdịda anyanwụ, agaghị m achọta ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Mgbe ọ nọ nʼọrụ nʼugwu anaghị m ahụ ya,",
            "poem": 1
          },
          {
            "text": "mgbe ọ lọghachiri na ndịda anaghị m ahụkwa ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Ma ọ maara ụzọ m na-aga,",
            "poem": 1
          },
          {
            "text": "mgbe ọ nwapụtasịrị m, aga m apụta dịka ọlaedo,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "ụkwụ m abụọ na-agbaso nzọ ụkwụ ya;",
            "poem": 1
          },
          {
            "text": "ana m eso ụzọ ya na-atụgharịghị nʼaka nri maọbụ aka ekpe.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Esibeghị m nʼiwu nke si nʼọnụ ya pụta wezuga onwe m;",
            "poem": 1
          },
          {
            "text": "a na m echekwa okwu ọnụ ya karịa nri nke m na-eri kwa ụbọchị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "“Ma o guzo naanị ya, onye pụrụ iguzogide ya?",
            "poem": 1
          },
          {
            "text": "Ọ na-eme ihe ọbụla masịrị ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Ọ na-emezu ụkpụrụ ya niile o zubere megide m,",
            "poem": 1
          },
          {
            "text": "ọ ka nwekwara nzube ndị ọzọ yiri nke a o dobere imegide m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Nʼihi ya ka obi ji ama m jijiji nʼihu ya;",
            "poem": 1
          },
          {
            "text": "mgbe m chetara banyere ihe ndị a niile, ana m atụ egwu ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Chineke emeela ka obi m daa mba;",
            "poem": 1
          },
          {
            "text": "Onye pụrụ ime ihe niile emeela ka m maa jijiji.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Ma ọchịchịrị emeghị ka m mechie ọnụ m,",
            "poem": 1
          },
          {
            "text": "bụ oke ọchịchịrị nke kpuchiri ihu m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      }
    ],
