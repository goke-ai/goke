  "chapter": {
    "number": 10,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "“Ike ịdị ndụ agwụla m.",
            "poem": 1
          },
          {
            "text": "Hapụnụ m ka m kwuo ihe dị m nʼobi,",
            "poem": 2
          },
          {
            "text": "kwupụta ya dịka obi ilu nke mkpụrụobi m si dị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ihe m ga-agwa Chineke bụ nke a: Apụtala maa m ikpe,",
            "poem": 1
          },
          {
            "text": "kama gwa m ihe mere i ji ama m ikpe.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ọ dị gị mma nʼobi imegbu m,",
            "poem": 1
          },
          {
            "text": "si otu a leda ọrụ aka gị anya,",
            "poem": 2
          },
          {
            "text": "mgbe ị na-anabata atụmatụ nke ndị na-emebi iwu?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Ị nwere anya nke anụ ahụ?",
            "poem": 1
          },
          {
            "text": "Ị na-ahụ ụzọ dịka mmadụ si ahụ?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ụbọchị nke gị, ha dịka nke ndị nwere anụ ahụ,",
            "poem": 1
          },
          {
            "text": "ka afọ nke gị, ha dịka nke mmadụ dị ike?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Nke ga-eme na i na-achọpụta ikpe ọmụma m",
            "poem": 1
          },
          {
            "text": "ma nyochaa mmehie m?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ọ bụ ezie na ị maara na ikpe amaghị m,",
            "poem": 1
          },
          {
            "text": "na ọ dịghị onye pụrụ ịnapụta m site nʼaka gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "“Ọ bụ aka gị kpụrụ m kee m.",
            "poem": 1
          },
          {
            "noteId": 1105
          },
          {
            "text": "Ọ ga-atụgharịa ugbu a bibie m?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Cheta na ị kpụrụ m dịka ụrọ.",
            "poem": 1
          },
          {
            "text": "Ị ga-eme ka m laghachi nʼaja ọzọ?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Ọ bụ na ị wụpụghị m dịka mmiri ara ehi",
            "poem": 1
          },
          {
            "text": "mee ka m kpụkọta dịka mmiri ara rahụrụ arahụ?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Yikwasị m akpụkpọ ahụ na anụ ahụ,",
            "poem": 1
          },
          {
            "text": "werekwa ọkpụkpụ na akwara kpaa m dịka uwe?",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "I nyere m ndụ gosikwa m obiọma gị,",
            "poem": 1
          },
          {
            "text": "nʼịdị mma gị chebekwara mmụọ m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "“Ma nke a bụ ihe i zoro nʼime obi gị,",
            "poem": 1
          },
          {
            "text": "amakwa m na ọ bụ ihe dị gị nʼuche.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Ọ bụrụ na m mehiere, ị ga na-ele m anya,",
            "poem": 1
          },
          {
            "text": "ị gaghị ekwe ka m ghara ịta ahụhụ nʼihi mmehie m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Ọ bụrụ na ikpe mara m, ahụhụ na-adịrị m!",
            "poem": 1
          },
          {
            "text": "A sịkwarị na aka m dị ọcha, apụghị m iweli isi m elu",
            "poem": 2
          },
          {
            "text": "nʼihi na ejupụtara m nʼihere,",
            "poem": 1
          },
          {
            "text": "bụrụkwa onye e mibara nʼime nsogbu m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Ọ bụrụ na m welie m isi elu, dịka ọdụm ị ga-eji nwayọọ na-eso m nʼazụ",
            "poem": 1
          },
          {
            "text": "ma werekwa ike gị dị egwu megide m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "I na-eweta ndị ama ọhụrụ megide m,",
            "poem": 1
          },
          {
            "text": "mee ka iwe gị baa ụba nʼebe m nọ;",
            "poem": 2
          },
          {
            "text": "ị na-eme ka ndị agha gị na-abịakwute m, otu nʼelu ibe ya dịka ebili mmiri.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "“Gịnị mere i ji wepụta m site nʼafọ nne m?",
            "poem": 1
          },
          {
            "text": "Ọ gaara aka m mma ịnwụ tupu anya ọbụla ahụ m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "A sịkwarị na amụpụtaghị m",
            "poem": 1
          },
          {
            "text": "maọbụ na e sitere nʼafọ nne m buba m nʼime ili!",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Ụbọchị ndụ m ọ dịghị ole na ole?",
            "poem": 1
          },
          {
            "text": "Si nʼebe m nọ wezuga onwe gị ka m nwetu ọṅụ nwa mgbe nta,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "tupu m hapụ laa nʼala ọchịchịrị na onyinyo nke ọnwụ",
            "poem": 1
          },
          {
            "text": "ebe m na-agaghị esi pụtakwa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "Nʼala nke oke ọchịchịrị,",
            "poem": 1
          },
          {
            "text": "ala nke onyinyo ọnwụ, nke usoro na-adịghị,",
            "poem": 2
          },
          {
            "text": "ebe ọbụladị ìhè dịkwa ka ọchịchịrị.”",
            "poem": 2
          }
        ]
      }
    ],
