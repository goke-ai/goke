      {
        "noteId": 918,
        "caller": "+",
        "text": "Nʼime akwụkwọ ndị mgbe ochie, aha ị ga-ahụ bụ Sinim.",
        "reference": {
          "chapter": 49,
          "verse": 12
        }
      },
      {
        "noteId": 919,
        "caller": "+",
        "text": "Ndị a nọ na mba ọzọ mụọ ga-alọta",
        "reference": {
          "chapter": 49,
          "verse": 20
        }
      },
      {
        "noteId": 920,
        "caller": "+",
        "text": "Ndị onye ezi omume dọtara nʼagha, a ga-eme ka ha wezuga onwe ha.",
        "reference": {
          "chapter": 49,
          "verse": 24
        }
      }
    ]
