  "chapter": {
    "number": 25,
    "content": [
      {
        "type": "heading",
        "content": [
          "Toonu Onyenwe anyị"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "O Onyenwe anyị, ị bụ Chineke m;",
            "poem": 1
          },
          {
            "text": "aga m ebuli gị elu, nye aha gị otuto,",
            "poem": 2
          },
          {
            "text": "nʼihi na site na ntụkwasị obi zuruoke,",
            "poem": 1
          },
          {
            "text": "ị rụọla ọtụtụ ihe ịrịbama,",
            "poem": 2
          },
          {
            "text": "nke i tụrụ atụmatụ ha siterị nʼoge gara aga.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Nʼihi na i meela ka obodo ghọọ mkpọmkpọ ebe;",
            "poem": 1
          },
          {
            "text": "mee ka obodo e wusiri ike ghọọ ebe dakpọrọ adakpọ;",
            "poem": 2
          },
          {
            "text": "meekwa ka ebe siri ike nke ndị mba ọzọ ghọọ ebe obodo na-adịghịzi,",
            "poem": 1
          },
          {
            "text": "agaghị ewukwa ya ruo mgbe ebighị ebi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Nʼihi nke a, ndị niile dị ike ga-asọpụrụ gị.",
            "poem": 1
          },
          {
            "text": "Obodo ndị ahụ na-emenye ndị mba ọzọ egwu ga-atụ gị egwu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Nʼihi na ị bụụrụ onye na-enweghị ike ebe mgbaba;",
            "poem": 1
          },
          {
            "text": "ị bụkwaara ndị ogbenye ebe mgbaba nʼoge mkpa ha,",
            "poem": 2
          },
          {
            "text": "ebe nchebe siri ike nʼoge oke mmiri ozuzo,",
            "poem": 1
          },
          {
            "text": "na ndo nʼoge okpomọkụ.",
            "poem": 2
          },
          {
            "text": "Nʼihi na nkuume nke ndị na-adịghị emere mmadụ ebere",
            "poem": 1
          },
          {
            "text": "dị ka oke mmiri ozuzo nke na-amakwasị mgbidi,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "dịkwa ka okpomọkụ nke na-adị nʼọzara.",
            "poem": 2
          },
          {
            "text": "Ị na-eme ka mkpọtụ nke ndị mba ọzọ kwụsị,",
            "poem": 1
          },
          {
            "text": "dịka ndo nke igwe ojii na-eweta si eme ka okpomọkụ dajụọ,",
            "poem": 2
          },
          {
            "text": "otu a ka abụ ọma nke ndị na-adịghị eme ebere si kwụsị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Nʼelu ugwu a, ka Onyenwe anyị, Onye pụrụ ime ihe niile",
            "poem": 1
          },
          {
            "text": "ga-anọ kwadooro ụwa niile oke oriri.",
            "poem": 2
          },
          {
            "text": "Ọ ga-abụ oke oriri nke ga-adịrị ndị niile.",
            "poem": 1
          },
          {
            "text": "Oke oriri nke ezi anụ na mmanya dị mma ga-adị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Nʼelu ugwu a, ka ọ ga-ewepụ ihe ndị ahụ niile na-eweta ihu mgbarụ,",
            "poem": 1
          },
          {
            "text": "nke dị ka akwa e ji kpuchie mmadụ niile,",
            "poem": 2
          },
          {
            "text": "nke kpuchikwara ihu ndị mba niile.",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ọ ga-elomi ọnwụ ruo mgbe ebighị ebi.",
            "poem": 2
          },
          {
            "text": "Onye kachasị ihe niile elu, bụ Onyenwe anyị ga-ehicha anya mmiri niile",
            "poem": 1
          },
          {
            "text": "site nʼihu mmadụ niile,",
            "poem": 2
          },
          {
            "text": "ọ ga-ewezuga ihe ịta ụta",
            "poem": 1
          },
          {
            "noteId": 888
          },
          {
            "text": "niile",
            "poem": 1
          },
          {
            "text": "dakwasịrị ndị ya site nʼụwa niile.",
            "poem": 2
          },
          {
            "text": "Onyenwe anyị ekwuola ya.",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          "Nʼụbọchị ahụ, ndị mmadụ ga-asị,",
          {
            "text": "“Nke a bụ Chineke anyị nʼezie,",
            "poem": 1
          },
          {
            "text": "anyị tụkwasịrị ya obi, ọ zọpụtakwara anyị.",
            "poem": 2
          },
          {
            "text": "Nke a bụ Onyenwe anyị ahụ, anyị tụkwasịrị ya obi,",
            "poem": 1
          },
          {
            "text": "ka anyị nwee ọṅụ ma ṅụrịakwa na nzọpụta ya.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Nʼihi na aka Onyenwe anyị ga-adịgide nʼelu ugwu a,",
            "poem": 1
          },
          {
            "text": "ma a ga-azọtọ ndị Moab nʼọnọdụ ha,",
            "poem": 2
          },
          {
            "text": "dịka esi azọda ahịhịa dị nʼokpuru ụkwụ mmadụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ha ga-agbasa aka ha abụọ nʼime ya,",
            "poem": 1
          },
          {
            "text": "dịka onye na-egwu mmiri si agbasapụ aka ya,",
            "poem": 2
          },
          {
            "text": "ma Chineke ga-eweda mpako ha niile,",
            "poem": 1
          },
          {
            "text": "nʼagbanyeghị ọrụ aghụghọ niile nke aka ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ọ ga-akwatu mgbidi gị niile dị elu nke e wusiri ike,",
            "poem": 1
          },
          {
            "text": "mee ka ha daruo ala,",
            "poem": 2
          },
          {
            "text": "ọ ga-eme ka ha daruo ala,",
            "poem": 1
          },
          {
            "text": "ruo mgbe ha ghọrọ uzuzu.",
            "poem": 2
          }
        ]
      }
    ],
