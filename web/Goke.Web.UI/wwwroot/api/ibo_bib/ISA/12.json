  "chapter": {
    "number": 12,
    "content": [
      {
        "type": "heading",
        "content": [
          "Abụ Otuto"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          "Nʼụbọchị ahụ, ị ga-ekwu sị,",
          {
            "text": "“Aga m eto gị Onyenwe anyị.",
            "poem": 1
          },
          {
            "text": "Ọ bụ ezie na ị wesara m iwe,",
            "poem": 2
          },
          {
            "text": "ma ugbu a, iwe gị adajụọla;",
            "poem": 1
          },
          {
            "text": "ị kasịkwara m obi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Nʼezie, Chineke bụ onye nzọpụta m;",
            "poem": 1
          },
          {
            "text": "aga m atụkwasị ya obi, ụjọ agaghị atụkwa m.",
            "poem": 2
          },
          {
            "text": "Onyenwe anyị, Onyenwe anyị ya onwe ya, bụ ike m bụrụkwa abụ m,",
            "poem": 1
          },
          {
            "text": "ọ ghọrọla m onye nzọpụta.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ị ga-eji ọṅụ dọrọ mmiri",
            "poem": 1
          },
          {
            "text": "site nʼolulu mmiri nke nzọpụta.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          "Nʼụbọchị ahụ, ị ga-ekwukwa sị,",
          {
            "text": "“Nyenụ Onyenwe anyị otuto, Kwupụtakwa aha ya",
            "poem": 1
          },
          {
            "noteId": 855
          },
          {
            "text": ";",
            "poem": 1
          },
          {
            "text": "meenụ ka amata nʼetiti mba niile ihe niile ọ rụrụ",
            "poem": 2
          },
          {
            "noteId": 856
          },
          {
            "text": ";",
            "poem": 2
          },
          {
            "text": "kwusaanụ na aha ya bu ihe e buliri elu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Bụkuonụ Onyenwe anyị abụ, nʼihi na o meela oke ihe dị ebube.",
            "poem": 1
          },
          {
            "text": "Meekwanụ ka ụwa niile mara ihe ọ rụrụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ka ndị niile bi na Zayọn tie mkpu ọṅụ,",
            "poem": 1
          },
          {
            "noteId": 857
          },
          {
            "text": "bụkwaa abụ.",
            "poem": 1
          },
          {
            "text": "Nʼihi na Onye nsọ nke Izrel.",
            "poem": 2
          },
          {
            "noteId": 858
          },
          {
            "text": "dị ukwuu, e, ọ dị ike nke ukwuu, bụ Onye ahụ na-ebi nʼetiti unu.”",
            "poem": 2
          },
          {
            "noteId": 859
          }
        ]
      }
    ],
