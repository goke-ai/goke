  "chapter": {
    "number": 34,
    "content": [
      {
        "type": "heading",
        "content": [
          "Ahụhụ dịrị mba niile"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Bịanụ nso, unu ndị mba niile, ṅaa ntị.",
            "poem": 1
          },
          {
            "text": "Geenụ ntị, unu mba niile nke ụwa.",
            "poem": 2
          },
          {
            "text": "Ka ụwa na ihe niile dị nʼime ya,",
            "poem": 1
          },
          {
            "text": "na ihe niile na-esi nʼime ya apụta nụrụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Nʼihi na Onyenwe anyị na-eweso mba niile nke ụwa iwe,",
            "poem": 1
          },
          {
            "text": "iwe ya dị ọkụ megide ndị agha ha niile.",
            "poem": 2
          },
          {
            "text": "Ọ ga-ala ha nʼiyi kpamkpam,",
            "poem": 1
          },
          {
            "text": "nyefee ha nʼaka ndị ga-egbu ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ndị ha e gburu egbu ka a ga-atụpụ.",
            "poem": 1
          },
          {
            "text": "Ozu ha ga-esikwa isi.",
            "poem": 2
          },
          {
            "text": "Ugwu niile ga-ejupụta nʼọbara ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Nʼoge ahụ, kpakpando niile nke eluigwe ga-agbaze,",
            "poem": 1
          },
          {
            "text": "a ga-apịakọtakwa mbara eluigwe dịka akwụkwọ,",
            "poem": 2
          },
          {
            "text": "kpakpando niile ga-ada",
            "poem": 1
          },
          {
            "text": "dịka ahịhịa osisi vaịnị kpọrọ nkụ,",
            "poem": 2
          },
          {
            "text": "dịka mkpụrụ osisi fiig chakpọrọ achakpọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Nʼihi na mma agha m arụchaala ọrụ ya nʼeluigwe,",
            "poem": 1
          },
          {
            "text": "lezie anya ihe gaje ime, ọ ga-adakwasị Edọm,",
            "poem": 2
          },
          {
            "text": "ndị m mara ikpe mbibi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Mma agha Onyenwe anyị jupụtara nʼọbara,",
            "poem": 1
          },
          {
            "text": "abụba kpuchikwara ya,",
            "poem": 2
          },
          {
            "text": "ọbara ụmụ atụrụ na mkpi",
            "poem": 1
          },
          {
            "text": "na abụba sitere nʼakụrụ ebule",
            "poem": 2
          },
          {
            "text": "nʼihi na Onyenwe anyị nwere ịchụ aja na Bozra.",
            "poem": 1
          },
          {
            "text": "O nwekwara oke ogbugbu ga-adị nʼala Edọm.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Oke ehi ọhịa ga-esokwa ha daa,",
            "poem": 1
          },
          {
            "text": "ha na oke ehi ukwu na oke ehi nta.",
            "poem": 2
          },
          {
            "text": "Ala ga-ejupụta nʼọbara ha.",
            "poem": 1
          },
          {
            "text": "Uzuzu nke obodo ahụ ga-amịkọrọ ọbara ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Nʼihi na Onyenwe anyị nwere ụbọchị ahụ bụ ụbọchị ịbọ ọbọ.",
            "poem": 1
          },
          {
            "text": "Ọ bụ afọ ịkwụghachi Edọm ihe ha mere Zayọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Isi iyi ndị Edọm niile ga-agbanwe gbajie ojii,",
            "poem": 1
          },
          {
            "text": "uzuzu dị nʼala ga na-ere ọkụ dịka sọlfọ (ntụ ọkụ),",
            "poem": 2
          },
          {
            "text": "ala ahụ niile ga-enwuru ọkụ dị ka unyi ojii.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Ọ gaghị anyụ anyụ nʼabalị maọbụ nʼehihie;",
            "poem": 1
          },
          {
            "text": "anwụrụ ọkụ ya ga na-ala nʼelu ruo ebighị ebi.",
            "poem": 2
          },
          {
            "text": "Ala ahụ ga-atọgbọrọ nʼefu site nʼọgbọ ruo nʼọgbọ;",
            "poem": 1
          },
          {
            "text": "ọ dịghị onye ga-esitekwa nʼebe ahụ gafee ọzọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ikwighịkwighị nke ọzara na okwolo ga-enweta ya,",
            "poem": 1
          },
          {
            "text": "ikwighịkwighị ukwu na ugolọma ga-akpa akwụ ha birikwa nʼebe ahụ.",
            "poem": 2
          },
          {
            "text": "Chineke ga-eseti nʼelu Edọm,",
            "poem": 1
          },
          {
            "text": "eriri ọtụtụ nke ọgbaaghara,",
            "poem": 2
          },
          {
            "text": "na ihe ịtụ mgbidi nke e tikpọrọ etikpọ, tọgbọrọ nʼefu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ndị ya a na-asọpụrụ agaghị ahụkwa ihe ọbụla nʼebe ahụ nke ha ga-akpọ alaeze.",
            "poem": 1
          },
          {
            "text": "Otu a kwa, ụmụ ndị eze ya niile agaghị adịkwa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Ogwu ga-etochi ụlọ ukwu ya niile;",
            "poem": 1
          },
          {
            "text": "ebe ya niile e wusiri ike ga-aghọ ahịhịa ogwu,",
            "poem": 2
          },
          {
            "text": "ọ ga-abụkwa ụlọ nkịta ọhịa",
            "poem": 1
          },
          {
            "text": "na nke ikwighịkwighị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Anụ ọhịa niile nke ọzara ga-ezute nkịta ọhịa nʼebe ahụ.",
            "poem": 1
          },
          {
            "text": "Ụmụ mkpi ọhịa ga-awagharị nʼebe ahụ na-ebeku ibe ha akwa.",
            "poem": 2
          },
          {
            "text": "Nʼebe ahụ ka ụmụ anụ ọhịa na-akpagharị nʼabalị ga-ezo onwe ha,",
            "poem": 1
          },
          {
            "text": "chọtakwara onwe ha ọnọdụ ebe izuike.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Ikwighịkwighị ga-akpa akwụ ya nʼebe ahụ.",
            "poem": 1
          },
          {
            "text": "Ọ ga-anọkwa nʼebe ahụ yie akwa, zụọ ụmụ ya,",
            "poem": 2
          },
          {
            "text": "lekọtaa ha site nʼiji nku ya kpuchie ha.",
            "poem": 2
          },
          {
            "text": "Ebe ahụ kwa ka ndagbụ ga-ezukọkwa,",
            "poem": 1
          },
          {
            "text": "nke ọbụla na ibe ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          "Nyochaa akwụkwọ nsọ ka ị gụọ ihe Onyenwe anyị gaje ime:",
          {
            "text": "Ọ dịghị anụ ọhịa ọbụla ga-efu.",
            "poem": 1
          },
          {
            "text": "Ọ dịghị nke ọbụla ga-anọ nʼebe ahụ hapụ ịkpọ ibe ya,",
            "poem": 2
          },
          {
            "text": "nʼihi na ọnụ ya enyela iwu a.",
            "poem": 1
          },
          {
            "text": "Mmụọ ya ga-achịkọtakwa ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Ọ bụ ya onwe ya ga-ekewa ala ahụ,",
            "poem": 1
          },
          {
            "text": "jiri ihe ọtụtụ nʼaka keere ha oke ahụ.",
            "poem": 2
          },
          {
            "text": "Ha ga-enweta ya ruo mgbe ebighị ebi,",
            "poem": 1
          },
          {
            "text": "nwekwa ebe obibi nʼebe ahụ site nʼọgbọ ruo nʼọgbọ.",
            "poem": 2
          }
        ]
      }
    ],
