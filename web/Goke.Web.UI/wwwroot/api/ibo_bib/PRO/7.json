  "chapter": {
    "number": 7,
    "content": [
      {
        "type": "heading",
        "content": [
          "Ịdọ aka na ntị banyere nwanyị na-akwa iko"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Na-agbaso okwu m nwa m, nọgide na ya,",
            "poem": 1
          },
          {
            "text": "dokwaa iwu m niile nʼime obi gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Debe iwu m ka ị dị ndụ,",
            "poem": 1
          },
          {
            "noteId": 1538
          },
          {
            "text": "chee ozizi m nche dịka ọ bụ mkpụrụ anya gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Kee ha na mkpịsịaka gị,",
            "poem": 1
          },
          {
            "text": "deekwa ha nʼelu mbadamba nkume, bụ obi gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Hụ amamihe nʼanya dịka nwaagbọghọ ị hụrụ nʼanya;",
            "poem": 1
          },
          {
            "text": "hụkwa inwe nghọta nʼanya dịka a ga-asị na ọ bụ nwanne gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Kwee ka ha debe gị, ka ị ghara ijekwuru nwanyị na-akwa iko,",
            "poem": 1
          },
          {
            "text": "ka ị gharakwa ịṅa ntị nʼokwu ire ụtọ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Nʼihi na otu ụbọchị,",
            "poem": 1
          },
          {
            "text": "esi m na oghereikuku dị nʼụlọ m lepụ anya,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "hụ nʼetiti ndị na-enweghị uche,",
            "poem": 1
          },
          {
            "text": "ahụtara m otu nwokorobịa nʼetiti ha,",
            "poem": 2
          },
          {
            "text": "onye amamihe kọrọ,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "ka ọ na-aga nʼụzọ dị nso ebe nkuku ụlọ",
            "poem": 1
          },
          {
            "text": "nʼakụkụ ụlọ nwanyị ihere na-adịghị eme,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "nʼoge anyasị, dịka chi na-eji,",
            "poem": 1
          },
          {
            "text": "mgbe ọchịchịrị na-agbachi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Mgbe ahụ, nwanyị ahụ pụtara zute ya.",
            "poem": 1
          },
          {
            "text": "O yi uwe dịka nwanyị akwụna, jiri aghụghọ na-abịakwute ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "(Ọ na-eme ụzụ na-aṅaghị ntị ịnụ ihe ọbụla.",
            "poem": 1
          },
          {
            "text": "Ụkwụ ya anaghị anọgide nʼụlọ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ọ na-akwụgharị ma nʼokporoụzọ ma nʼama;",
            "poem": 1
          },
          {
            "text": "na-amịgharị nʼebe nzuzo niile.)",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "O weere aka ya abụọ jikụọ nwokorobịa ahụ nʼolu, sutu ya ọnụ,",
            "poem": 1
          },
          {
            "text": "were ihu nke ihere na-adịghị, sị:",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "“Emezuru m nkwa niile m kwere taa,",
            "poem": 1
          },
          {
            "text": "enwekwara m nri nke sitere nʼihe aja udo m chụrụ nʼụlọ m,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Ọ bụ gị ka m na-achọkwute,",
            "poem": 1
          },
          {
            "text": "lee gị ka ị na-abịa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Agbasaala m akwa mara mma nʼelu ihe ndina m,",
            "poem": 1
          },
          {
            "text": "bụ akwa ọcha mara mma tụrụ ọtụtụ agwa nke e ji ogho ndị Ijipt kpaa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Efesakwala m mmanụ isi ọma nke máá, aloos na sinamọn",
            "poem": 1
          },
          {
            "text": "nʼelu nʼihe ndina m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Bịa, ka anyị were ịhụnanya ṅụjụbiga afọ anyị;",
            "poem": 1
          },
          {
            "text": "ka anyị mee onwe anyị obi ụtọ site ugbu a ruo ụtụtụ echi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Nʼihi na di m anọghị nʼụlọ",
            "poem": 1
          },
          {
            "text": "o jeela njem dị anya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "O ji akpa ego juru gaa njem,",
            "poem": 1
          },
          {
            "text": "abalị ole na ole ga-agafekwa tupu ọ lọta.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Otu a ka o si jiri okwu ire ụtọ ya megharịa nwokorobịa ahụ isi tutu o kwenyere ya.",
            "poem": 1
          },
          {
            "text": "O nwekwaghị ike ịnapụta onwe ya site nʼokwu nrafu nke nwanyị ahụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "O sooro nwanyị ahụ dịka ehi",
            "poem": 1
          },
          {
            "text": "a na-adọkpụ na-aga ebe a ga-egbu ya,",
            "poem": 2
          },
          {
            "text": "dịka ele",
            "poem": 1
          },
          {
            "noteId": 1539
          },
          {
            "text": "nke na-aba nʼọnya,",
            "poem": 1
          }
        ]
      },
      {
        "type": "verse",
        "number": 23,
        "content": [
          {
            "text": "nke na-eche ka e were àkụ gbata ya nʼobi.",
            "poem": 2
          },
          {
            "text": "Nwoke ahụ dịka nnụnụ nke na-efeba nʼọnya",
            "poem": 1
          },
          {
            "text": "na-amaghị ihe na-eche ya pụrụ iwepụ ndu ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 24,
        "content": [
          {
            "text": "Geenụ ntị, ụmụ m, ma ọ bụghị naanị ige ntị,",
            "poem": 1
          },
          {
            "text": "kama meekwanụ ihe m na-ekwu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 25,
        "content": [
          {
            "text": "Ekwela ka agụụ nke obi gị chịa gị;",
            "poem": 1
          },
          {
            "text": "ejela nwanyị dị otu a nso; gbaara ya ọsọ, ka ọ ghara ịnwa gị nwetakwa gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 26,
        "content": [
          {
            "text": "Nʼihi na o mebiela ndụ ọtụtụ mmadụ,",
            "poem": 1
          },
          {
            "text": "ọtụtụ mmadụ amaala nʼọnya ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 27,
        "content": [
          {
            "text": "Ụlọ ya bụ okporoụzọ nke na-eduba nʼili,",
            "poem": 1
          },
          {
            "text": "nke na-edubakwa nʼala ọnwụ.",
            "poem": 2
          }
        ]
      }
    ],
