  "chapter": {
    "number": 10,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          "Ilu dị iche iche nke Solomọn:",
          {
            "text": "Nwata maara ihe na-eme ka nna ya ṅụrịa ọṅụ;",
            "poem": 1
          },
          {
            "text": "ma nwantakịrị dị nzuzu na-ewetara nne ya nʼiru ụjụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Akụ a kpatara nʼụzọ na-ezighị ezi adịghị aba uru,",
            "poem": 1
          },
          {
            "text": "ma ezi omume na-azọpụta mmadụ site nʼọnwụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Onyenwe anyị agaghị ekwe ka onye ezi omume nwụọ nʼagụụ,",
            "poem": 1
          },
          {
            "text": "ọ gakwaghị ekwe ka ihe nọgide na-agara onye ajọ omume nke ọma.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Aka ndị umengwụ na-eweta ogbenye,",
            "poem": 1
          },
          {
            "text": "ma aka na-arụsị ọrụ ike na-eweta ụba.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Onye na-ekpokọta nʼoge ọkọchị bụ nwa maara ihe,",
            "poem": 1
          },
          {
            "text": "ma nwa ahụ nke na-arahụ ụra nʼoge ọrụ bụ nwa na-eweta ihere.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Onye ezi omume na-anata ngọzị,",
            "poem": 1
          },
          {
            "text": "ma naanị nkọcha jupụtara nʼọnụ onye ajọ omume.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Aha onye ezi omume ka eji enye ngọzị,",
            "poem": 1
          },
          {
            "text": "ma aha onye ajọ omume ga-ere ure.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Onye obi ya maara ihe na-anabata ihe e nyere nʼiwu,",
            "poem": 1
          },
          {
            "text": "ma onye nzuzu ekwurekwu ga-ala nʼiyi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Onye ụzọ ya ziri ezi na-aga ije nʼatụghị egwu,",
            "poem": 1
          },
          {
            "text": "ma onye ụzọ ya ezighị ezi ka a ga-achọpụta.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Onye ọbụla na-egbu ikuanya na-ebute ihe mwute,",
            "poem": 1
          },
          {
            "text": "onye nzuzu ekwurekwu ga-abịa nʼịla nʼiyi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ọnụ onye ezi omume bụ isi iyi nke ndụ,",
            "poem": 1
          },
          {
            "text": "ma naanị nkọcha jupụtara nʼọnụ onye ajọ omume.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ịkpọ asị na-akpali esemokwu,",
            "poem": 1
          },
          {
            "text": "ma ịhụnanya na-ekpuchi mmehie niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Ndị nwere nghọta na-ekwu okwu nwere isi,",
            "poem": 1
          },
          {
            "text": "ma ndị na-enweghị uche ka a na-eti ihe dịka ohu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Onye amamihe na-akpakọba ihe ọmụma,",
            "poem": 1
          },
          {
            "text": "ma onye nzuzu na-eji ọnụ ya akpọ ịla nʼiyi oku.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Akụ ndị ọgaranya bụrụ ha obodo e wusiri ike,",
            "poem": 1
          },
          {
            "text": "ma ụbịam bụ ịla nʼiyi nke ndị ogbenye.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Ụgwọ ọrụ onye ezi omume bụ ndụ,",
            "poem": 1
          },
          {
            "text": "ma ihe nweta nke onye ajọ omume bụ mmehie na ọnwụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Onye na-ege ntị na ndụmọdụ na-egosi ụzọ nke ndụ,",
            "poem": 1
          },
          {
            "text": "ma onye na-adịghị anabata ndụmọdụ na-eme ka ndị ọzọ kpafuo.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Onye ji egbugbere ọnụ ụgha ekpuchi ịkpọ asị",
            "poem": 1
          },
          {
            "text": "na onye na-agbasa okwu nkwutọ bụ onye nzuzu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Nʼọtụtụ okwu mmehie adịghị akwụsị",
            "poem": 1
          },
          {
            "text": "ma ndị nwere uche na-ejide ire ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Ire nke onye ezi omume bụ ọlaọcha kachasị mma,",
            "poem": 1
          },
          {
            "text": "ma obi onye ajọ omume bụ ihe ọnụahịa nta.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Egbugbere ọnụ onye ezi omume na-azụju ọtụtụ mmadụ afọ,",
            "poem": 1
          },
          {
            "text": "ma ndị nzuzu na-anwụ nʼihi amamihe nke kọrọ ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "Ngọzị Onyenwe anyị na-eme mmadụ ọgaranya,",
            "poem": 1
          },
          {
            "text": "ọ naghị atụkwasị ya nsogbu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 23,
        "content": [
          {
            "text": "Ihe na-atọ onye nzuzu ụtọ bụ izube nzube ọjọọ;",
            "poem": 1
          },
          {
            "text": "ma ihe na-atọ onye maara ihe ụtọ bụ inwe nghọta.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 24,
        "content": [
          {
            "text": "Ihe ahụ onye na-emebi iwu nʼatụ ụjọ ya ga-abịakwasị ya,",
            "poem": 1
          },
          {
            "text": "ihe onye ezi omume na-achọ ka a ga-enye ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 25,
        "content": [
          {
            "text": "Mgbe oke ifufe na-emebi ihe, ndị na-emebi iwu ka ọ na-eburu;",
            "poem": 1
          },
          {
            "text": "ma ndị ezi omume na-eguzosi ike ruo ebighị ebi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 26,
        "content": [
          {
            "text": "Onye umengwụ na-abụ ihe mgbu nye onyenwe ya;",
            "poem": 1
          },
          {
            "text": "ọ dịka anwụrụ ọkụ nʼanya mmadụ maọbụ dịka mmanya gbara ụka nke na-ejiji eze.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 27,
        "content": [
          {
            "text": "Ịtụ egwu Onyenwe anyị na-enye ogologo ndụ,",
            "poem": 1
          },
          {
            "text": "ma ọnụọgụgụ afọ nke onye ajọ omume na-adị mkpụmkpụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 28,
        "content": [
          {
            "text": "Olileanya ndị ezi omume ga-abụ ọṅụ;",
            "poem": 1
          },
          {
            "text": "olileanya ndị na-emebi iwu bụ naanị ihe efu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 29,
        "content": [
          {
            "text": "Ụzọ Onyenwe anyị bụ ebe mgbaba nye ndị na-enweghị ịta ụta,",
            "poem": 1
          },
          {
            "text": "maọbụ ịla nʼiyi nye ndị na-eme ajọ ihe.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 30,
        "content": [
          {
            "text": "A pụghị iwezuga",
            "poem": 1
          },
          {
            "noteId": 1541
          },
          {
            "text": "onye ezi omume,",
            "poem": 1
          },
          {
            "text": "ma onye na-emebi iwu agaghị anọgide.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 31,
        "content": [
          {
            "text": "Nʼọnụ onye ezi omume ka mkpụrụ amamihe si abịa,",
            "poem": 1
          },
          {
            "text": "ma ire na-ekwu okwu nduhie ka a ga-eme ka o dere duu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 32,
        "content": [
          {
            "text": "Egbugbere ọnụ onye ezi omume mara ihe na-eweta ihuọma,",
            "poem": 1
          },
          {
            "text": "ma ọnụ ndị ajọ omume bụ naanị okwu nduhie.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      }
    ],
