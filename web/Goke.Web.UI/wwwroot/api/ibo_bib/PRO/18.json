  "chapter": {
    "number": 18,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Onye ihe mmadụ na-adịghị amasị na-agbaso naanị ọchịchọ nke ya onwe ya,",
            "poem": 1
          },
          {
            "text": "na-alụsokwa ezi nzube niile ọzọ ọgụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Nghọta adịghị atọ onye nzuzu ụtọ.",
            "poem": 1
          },
          {
            "text": "Naanị ihe ọ maara bụ ikwu uche ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Mgbe onye ajọ omume na-abata, ka nleda anya na-abata,",
            "poem": 1
          },
          {
            "text": "otu a kwa nkọcha na-eweta ihere.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Okwu ọnụ niile bụ mmiri dị omimi,",
            "poem": 1
          },
          {
            "text": "ma amamihe dị ka mmiri iyi na-asọ asọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ọ dịghị mma ime ka ikpe gara onye mere ihe ọjọọ,",
            "poem": 1
          },
          {
            "text": "si otu a gbochie onye aka ya dị ọcha inweta ikpe ziri ezi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Egbugbere ọnụ ndị nzuzu na-ebutere ha esemokwu,",
            "poem": 1
          },
          {
            "text": "ọnụ ha na-ebutere ha ihe otiti.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ọnụ onye nzuzu na-etinye ya na nsogbu,",
            "poem": 1
          },
          {
            "text": "egbugbere ọnụ ya bụ ọnya nye mkpụrụobi ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Okwu niile nke onye na-agba asịrị na-ekwu dịka iberibe nri nʼatọ ezi ụtọ,",
            "poem": 1
          },
          {
            "text": "ha na-agbadakwa nʼime ime ala afọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Onye dị umengwụ nʼọrụ ya",
            "poem": 1
          },
          {
            "text": "bụ nwanne nke onye mbibi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Aha Onyenwe anyị bụ ụlọ elu e wusiri ike,",
            "poem": 1
          },
          {
            "text": "ndị ezi omume na-agbaba nʼime ya izere ndụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Akụ ndị ọgaranya bụrụ ha obodo e wusiri ike,",
            "poem": 1
          },
          {
            "text": "ọ bụkwa mgbidi dị elu nke a na-apụghị ịmafe nʼechiche ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Tupu mmadụ adaa obi ya na-ejupụta na nganga,",
            "poem": 1
          },
          {
            "text": "ma inwe obi umeala na-eweta nsọpụrụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Inye ọsịsa okwu tupu mmadụ ege ntị",
            "poem": 1
          },
          {
            "text": "bụ enweghị uche na ihe ihere.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Ọ bụ mmụọ mmadụ na-akwado ya nʼoge nrịa nrịa,",
            "poem": 1
          },
          {
            "text": "ma mmụọ e tipịara etipịa, onye pụrụ ibuli ya?",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Obi onye nwere nghọta na-enweta ihe ọmụma,",
            "poem": 1
          },
          {
            "text": "nʼihi na ntị onye maara ihe na-achọpụta ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Onyinye mmadụ na-akwara ya ụzọ sara mbara,",
            "poem": 1
          },
          {
            "text": "ọ na-edubakwa ya nʼihu ndị dị elu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Nʼikpe onye buru ụzọ kwuo ihe na-ese na-adị ka onye ikpe gaara,",
            "poem": 1
          },
          {
            "text": "tutu ruo mgbe mmadụ bịara bido ịjụ ya ajụjụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Ife nza na-akwụsị esemokwu na ndọrọndọrọ",
            "poem": 1
          },
          {
            "text": "dị nʼetiti mmadụ abụọ ndị olu ha na-ala elu.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Ọ dị mfe ịlụgbụ obodo karịa ime ka obi nwanne e mehiere tụgharịa.",
            "poem": 1
          },
          {
            "text": "Iwe ya na-adị ike dịka ọnụ ụzọ e jiri igodo tụchie.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Site na mkpụrụ nke okwu ọnụ mmadụ mịpụtara ka afọ ga-eji ju ya.",
            "poem": 1
          },
          {
            "text": "Sitekwa nʼihe owuwe ubi nke egbugbere ọnụ ha ka ha ga-eji rijuo afọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Ike ọnwụ na ndụ dị ire nʼaka.",
            "poem": 1
          },
          {
            "text": "Ndị hụrụ ya nʼanya ga-eri mkpụrụ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "Nwoke ahụ chọtara nwunye chọtara ezi ihe.",
            "poem": 1
          },
          {
            "text": "Ọ na-anatakwa ihuọma site nʼaka Onyenwe anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 23,
        "content": [
          {
            "text": "Onye ogbenye na-arịọ arịrịọ maka ebere;",
            "poem": 1
          },
          {
            "text": "ma ọgaranya na-asa okwu ike ike.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 24,
        "content": [
          {
            "text": "Onye nwere ọtụtụ ndị enyi ndị na-ekwesighị ga-abịa nʼịla nʼiyi mgbe na-adịghị anya,",
            "poem": 1
          },
          {
            "text": "ma ọ dị enyi nke na-arapara nso karịa nwanne.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      }
    ],
