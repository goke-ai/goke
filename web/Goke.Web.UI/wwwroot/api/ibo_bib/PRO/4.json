  "chapter": {
    "number": 4,
    "content": [
      {
        "type": "heading",
        "content": [
          "Amamihe Kachasị ihe niile"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Ụmụ m ndị ikom, geenụ ntị na ndụmọdụ nke nna unu,",
            "poem": 1
          },
          {
            "text": "lezienụ anya ka unu nwee nghọta.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Ṅaanụ ntị, nwee uche, nʼihi na ọ bụ ezi ozizi ka m na-ezi unu;",
            "poem": 1
          },
          {
            "text": "ya mere, unu ahapụkwala iwu m.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Mgbe m bụ nwantakịrị nʼụlọ nna m,",
            "poem": 1
          },
          {
            "text": "mgbe m ka dị ntakịrị, bụrụkwa naanị otu nwa nne m ji,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "ọ ziri m ihe, sịkwa m,",
            "poem": 1
          },
          {
            "text": "“Ka obi gị jidesie okwu m niile ike,",
            "poem": 2
          },
          {
            "text": "debe nʼiwu niile, ị ga-adịkwa ndụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Nweta amamihe, nwetakwa nghọta,",
            "poem": 1
          },
          {
            "text": "echezọkwala okwu m, maọbụ site na ya wezuga onwe gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ahapụla amamihe, ọ ga-echebe gị,",
            "poem": 1
          },
          {
            "text": "hụ ya nʼanya, nʼihi na ọ ga-eduzi gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Mmalite amamihe bụ nke a, nweta amamihe,",
            "poem": 1
          },
          {
            "text": "ihe ọbụla o ga-efu gị, nweta nghọta.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Ọ bụrụ na i bulie amamihe elu, o ga-ebulikwa gị elu.",
            "poem": 1
          },
          {
            "text": "Jidesie ya ike, ọ ga-ewetara gị oke nsọpụrụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Ọ ga-eji okpueze okoko osisi chọọ isi gị mma",
            "poem": 1
          },
          {
            "text": "nyekwa gị okpueze mara mma nʼisi gị.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Nwa m, ṅaa m ntị, meekwa ihe m gwara gị;",
            "poem": 1
          },
          {
            "text": "ị ga-enwekwa ndụ ogologo.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "M ga-edu gị nʼụzọ nke amamihe,",
            "poem": 1
          },
          {
            "text": "duokwa gị nʼụzọ ziri ezi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ọ bụrụ na i bie ụdị ndụ dị otu a, ihe agaghị akpọ gị nʼụkwụ nʼije gị,",
            "poem": 1
          },
          {
            "text": "ị gaghị asụkwa ngọngọ mgbe ị na-agba ọsọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Mee ihe niile m gwara gị, echefukwala ha,",
            "poem": 1
          },
          {
            "text": "nʼihi na ọ ga-eduba gị na ndụ zuruoke.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Emela dịka ndị na-emebi iwu.",
            "poem": 1
          },
          {
            "text": "Esokwala ụzọ ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Zeere ha, ejekwala nʼụzọ ha;",
            "poem": 1
          },
          {
            "text": "chepu ihu gị nʼụzọ ahụ,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "nʼihi na ndị na-emebi iwu adịghị arahụ ụra tutu ha emee arụ nke ụbọchị ahụ.",
            "poem": 1
          },
          {
            "text": "Ha anaghị ezu ike tutu ha emee ka otu onye maa nʼọnya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Ha na-eri ihe oriri nke mmebi iwu,",
            "poem": 1
          },
          {
            "text": "na-aṅụkwa ihe ọṅụṅụ nke ọgbaaghara.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Ma ụzọ ndị ezi omume dị ka anyanwụ na-awa nʼisi ụtụtụ,",
            "poem": 1
          },
          {
            "text": "nke na-enwuwakwa dịka ìhè nʼetiti ehihie ụbọchị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Ma ụzọ onye na-emebi iwu dị ka itiri nke ọchịchịrị,",
            "poem": 1
          },
          {
            "text": "ha anaghị ama ihe na-eme ka ha sụọ ngọngọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "Gee ntị nwa m, nʼihe m na-ekwu.",
            "poem": 1
          },
          {
            "text": "Gee ntị nke ọma.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "Doo ihe ndị a nʼuche gị mgbe niile;",
            "poem": 1
          },
          {
            "text": "ka ha bamiekwa nʼime obi gị,",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "nʼihi na ha bụ ndụ nye ndị chọtara ha,",
            "poem": 1
          },
          {
            "text": "bụrụkwa ahụ ike nye mmadụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 23,
        "content": [
          {
            "text": "Nke ka nke, lezie echiche uche gị niile anya,",
            "poem": 1
          },
          {
            "text": "nʼihi na ha na-emetụta ndụ gị niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 24,
        "content": [
          {
            "text": "Ka ụgha ọbụla ghara isi gị nʼọnụ pụta;",
            "poem": 1
          },
          {
            "text": "kpọọ ịgha ụgha niile na okwu rụrụ arụ niile asị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 25,
        "content": [
          {
            "text": "Legide anya nʼihu ime ihe bụ eziokwu;",
            "poem": 1
          },
          {
            "text": "atụgharịkwala isi gị ile ihe ọbụla.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 26,
        "content": [
          {
            "text": "Leruo ije ụkwụ gị anya,",
            "poem": 1
          },
          {
            "text": "doziekwa ụzọ gị niile ka ọ guzozie.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 27,
        "content": [
          {
            "text": "Ewezugala onwe gị site nʼaka nri maọbụ nʼaka ekpe;",
            "poem": 1
          },
          {
            "text": "si nʼihe ọjọọ wezuga ụkwụ gị.",
            "poem": 2
          }
        ]
      }
    ],
