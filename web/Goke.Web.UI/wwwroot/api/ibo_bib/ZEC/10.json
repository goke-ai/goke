  "chapter": {
    "number": 10,
    "content": [
      {
        "type": "heading",
        "content": [
          "Onyenwe anyị ga-elekọta Juda"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Rịọọnụ Onyenwe anyị mmiri ozuzo nʼoge mmiri ozuzo nke ikpeazụ,",
            "poem": 1
          },
          {
            "text": "nʼihi na ọ bụ Onyenwe anyị na-ezite oke amụma mmiri.",
            "poem": 2
          },
          {
            "text": "Ọ bụ ya na-enye ụmụ mmadụ mmiri ozuzo,",
            "poem": 1
          },
          {
            "text": "na-enyekwa mmadụ niile ihe ọkụkụ nke ubi.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Arụsị niile na-ekwu okwu aghụghọ,",
            "poem": 1
          },
          {
            "text": "ndị na-agba aja nʼahụ ọhụ bụ ụgha,",
            "poem": 2
          },
          {
            "text": "ndị ha na-arọ nrọ, na-akọwakwa nrọ ụgha,",
            "poem": 1
          },
          {
            "text": "okwu nkasiobi nke ihe na-adịghị nʼime ya ka ha na-enye.",
            "poem": 2
          },
          {
            "text": "Ya mere, ndị mmadụ na-akpagharị dịka atụrụ",
            "poem": 1
          },
          {
            "text": "a na-akpagbu ha nʼihi enweghị onye ọzụzụ atụrụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "“Iwe m na-ere ọkụ megide ndị ọzụzụ atụrụ,",
            "poem": 1
          },
          {
            "text": "aga m atakwa ndị ndu",
            "poem": 2
          },
          {
            "noteId": 1873
          },
          {
            "text": "niile ahụhụ;",
            "poem": 2
          },
          {
            "text": "nʼihi na Onyenwe anyị, Onye pụrụ ime ihe niile",
            "poem": 1
          },
          {
            "text": "ga-elekọta igwe anụ ụlọ ya, ya bụ ndị Juda,",
            "poem": 2
          },
          {
            "text": "ọ ga-eme ka ha dịrị ka ịnyịnya dị ebube nʼagha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "A ga-esi na Juda nweta nkume bụ ide ji ụlọ,",
            "poem": 1
          },
          {
            "noteId": 1874
          },
          {
            "text": "a ga-esi na ha nweta ǹtu ụlọ ikwu,",
            "poem": 2
          },
          {
            "text": "a ga-esi na ha nweta ụta a kwadoro maka agha,",
            "poem": 1
          },
          {
            "text": "sitekwa na ha nweta ndị ndu dị iche iche.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Ha niile ga-ejikọta onwe ha ọnụ, dịka ndị bụ dike nʼagha,",
            "poem": 1
          },
          {
            "text": "ha ga-azọda ndị iro ha nʼapịtị nke okporoụzọ niile.",
            "poem": 2
          },
          {
            "text": "Nʼihi na Onyenwe anyị nọnyeere ha,",
            "poem": 1
          },
          {
            "text": "ha ga-alụgbukwa ndị iro niile na-agba ịnyịnya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "“Aga m eme ka ụlọ Juda dị ike,",
            "poem": 1
          },
          {
            "text": "zọpụtakwa ụlọ Josef.",
            "poem": 2
          },
          {
            "text": "Aga m eme ka ha lọghachita,",
            "poem": 1
          },
          {
            "text": "nʼihi na aga m egosi ha obi ebere.",
            "poem": 2
          },
          {
            "text": "Ha ga-adị",
            "poem": 1
          },
          {
            "text": "ka ndị m na-ajụghị mgbe ọbụla,",
            "poem": 2
          },
          {
            "text": "nʼihi na mụ onwe m bụ Onyenwe anyị Chineke ha,",
            "poem": 1
          },
          {
            "text": "aga m azakwa arịrịọ ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Ndị Ifrem ga-aghọ ndị dị ike nʼagha,",
            "poem": 1
          },
          {
            "text": "ha ga-enwe ọṅụ dịka ndị na-aṅụ mmanya.",
            "poem": 2
          },
          {
            "text": "Ụmụ ha ga-ahụ ya ṅụrịakwa ọṅụ,",
            "poem": 1
          },
          {
            "text": "ha ga-ejikwa obi ha niile ṅụrịa ọṅụ nʼime Onyenwe anyị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Mgbe ahụ, aga m akpọ ha oku,",
            "poem": 1
          },
          {
            "text": "chịkọtaa ha.",
            "poem": 2
          },
          {
            "text": "Nʼezie, aga m agbapụta ha,",
            "poem": 1
          },
          {
            "text": "ha ga-adịkwa ukwuu dịka ha dị na mbụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "A sịkwarị na m chụsara ha nʼetiti ndị dị iche iche,",
            "poem": 1
          },
          {
            "text": "ma ha ga-echeta m site nʼala niile ahụ dị anya.",
            "poem": 2
          },
          {
            "text": "Ha na ụmụ ha ga-adịkwa ndụ;",
            "poem": 1
          },
          {
            "text": "ha ga-alọtakwa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Aga m esi nʼala Ijipt kpọlata ha,",
            "poem": 1
          },
          {
            "text": "sitekwa nʼAsịrịa chịkọtaa ha.",
            "poem": 2
          },
          {
            "text": "Aga m akpọta ha na Gilead na Lebanọn.",
            "poem": 1
          },
          {
            "text": "Ha agaghị enwekwa ebe ga-ezuru ha niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ha ga-esi nʼosimiri nsogbu gafee.",
            "poem": 1
          },
          {
            "text": "A ga-emekwa ka mmali elu nke osimiri dajụọ.",
            "poem": 2
          },
          {
            "text": "Ebe obibi niile nke osimiri Naịl ga-atakọrọkwa.",
            "poem": 2
          },
          {
            "text": "A ga-egbuda mpako niile nke Asịrịa,",
            "poem": 1
          },
          {
            "text": "wezugakwa mkpara ọchịchị nke ndị Ijipt.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Aga m eme ka ha dị ike nʼime Onyenwe anyị,",
            "poem": 1
          },
          {
            "text": "ọ bụkwa nʼaha ya ka ha ga na-ejegharị nʼudo.”",
            "poem": 2
          },
          {
            "text": "Otu a ka Onyenwe anyị kwubiri.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      }
    ],
