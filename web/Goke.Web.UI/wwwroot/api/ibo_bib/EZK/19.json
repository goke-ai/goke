  "chapter": {
    "number": 19,
    "content": [
      {
        "type": "heading",
        "content": [
          "Abụ akwa nʼihi ọnọdụ ụmụ eze Izrel"
        ]
      },
      {
        "type": "verse",
        "number": 1,
        "content": [
          "“Ma gị onwe gị, bụọrọnụ ndịisi Izrel abụ akwa a,"
        ]
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          "ma kwuo,",
          {
            "text": "“ ‘Ụdị nne Ọdụm gịnị ka nne gị bụ",
            "poem": 1
          },
          {
            "text": "nʼetiti ụmụ ọdụm.",
            "poem": 2
          },
          {
            "text": "Ọ dinara nʼetiti ha,",
            "poem": 1
          },
          {
            "text": "na-azụ ụmụ ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "Ọ zụlitere otu nʼime ụmụ ya",
            "poem": 1
          },
          {
            "text": "nke ghọrọ nwa ọdụm siri ike.",
            "poem": 2
          },
          {
            "text": "Ọ mụtakwara ka e si adọrisi anụ ọhịa,",
            "poem": 1
          },
          {
            "text": "ghọọ ọdụm na-eri mmadụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "Mba niile nụrụ maka ihe banyere ya,",
            "poem": 1
          },
          {
            "text": "mee ka ọ danye nʼolulu ha gwuru.",
            "poem": 2
          },
          {
            "text": "Ha ji nko koro ya,",
            "poem": 1
          },
          {
            "text": "duru ya gaa nʼala Ijipt.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "“ ‘Mgbe ọ hụrụ na ọ dịkwaghị ihe ọzọ o nwere ike ime banyere ọnọdụ ahụ,",
            "poem": 1
          },
          {
            "text": "matakwa na olileanya ya abụrụla ihe efu,",
            "poem": 2
          },
          {
            "text": "ọ họpụtara nwa ya ọzọ",
            "poem": 1
          },
          {
            "text": "kuziere ya otu ọ ga-esi bụrụ ọdụm dị ike.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "Ọ ghọrọ onyeisi ụmụ ọdụm niile,",
            "poem": 1
          },
          {
            "text": "nʼihi na ọ bụ ọdụm siri ike,",
            "poem": 2
          },
          {
            "text": "ọ mụtakwara ka e si ejide anụ ọhịa,",
            "poem": 1
          },
          {
            "text": "ya onwe ya ghọkwara ọdụm na-eri mmadụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "O tikpọrọ ebe ha niile e wusiri ike",
            "poem": 1
          },
          {
            "text": "meekwa obodo ha niile mkpọmkpọ ebe.",
            "poem": 2
          },
          {
            "text": "Ala ahụ na ndị niile nọ nʼime ya,",
            "poem": 1
          },
          {
            "text": "mara jijiji site nʼoke egwu nʼihi olu ịgbọ ụja ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Mgbe ahụ, ndị agha nke mba niile dị iche iche",
            "poem": 1
          },
          {
            "text": "pụtara gbaa ya gburugburu.",
            "poem": 2
          },
          {
            "text": "Ha sitere nʼakụkụ niile nwude ya",
            "poem": 1
          },
          {
            "text": "nʼọnya olulu ha siiri ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Ha tinyere ya nko nʼimi, were dọkpuo ya nʼụlọ igwe a kpara akpa,",
            "poem": 1
          },
          {
            "text": "bute ya nʼihu eze Babilọn.",
            "poem": 2
          },
          {
            "text": "E mere ka o biri na mba ọzọ dịka ohu,",
            "poem": 1
          },
          {
            "text": "ka a ghara ịnụ ụda olu ya ọzọ",
            "poem": 2
          },
          {
            "text": "nʼugwu niile nke Izrel.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "“ ‘Nne gị dịka osisi vaịnị nʼogige vaịnị",
            "poem": 1
          },
          {
            "text": "a kụrụ nʼakụkụ mmiri,",
            "poem": 2
          },
          {
            "text": "nke nwere ọtụtụ alaka, nke na-amịkwa mkpụrụ",
            "poem": 1
          },
          {
            "text": "nʼihi mmiri ọ na-enweta.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Alaka ya niile siri ike,",
            "poem": 1
          },
          {
            "text": "ha topụtara ghọọ mkpara eze.",
            "poem": 2
          },
          {
            "text": "O topụrụ etopụ,",
            "poem": 1
          },
          {
            "text": "tokarịa alaka ndị ọzọ elu.",
            "poem": 2
          },
          {
            "text": "Nke a mere e ji na-ahụ ya anya",
            "poem": 1
          },
          {
            "text": "site nʼebe dị anya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ma e ji oke iwe hopu ya",
            "poem": 1
          },
          {
            "text": "tụpụ ya nʼala.",
            "poem": 2
          },
          {
            "text": "Oke ifufe si nʼọwụwa anyanwụ mere ka akwụkwọ ya kpọnwụsịa,",
            "poem": 1
          },
          {
            "text": "meekwa ka mkpụrụ ya daa.",
            "poem": 2
          },
          {
            "text": "Alaka ya ndị ahụ dị ike kpọnwụsịrị,",
            "poem": 1
          },
          {
            "text": "ọkụ rechapụkwara ha niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Ugbu a, akụọla ya nʼime ọzara",
            "poem": 1
          },
          {
            "text": "ebe kpọrọ nkụ nke mmiri na-adịghị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Ọkụ esitela nʼogwe ya rechapụ alaka ya",
            "poem": 1
          },
          {
            "text": "na mkpụrụ ya niile.",
            "poem": 2
          },
          {
            "text": "Ọ dịkwaghị alaka siri ike fọdụrụ",
            "poem": 1
          },
          {
            "text": "nke dịkwa mma ịbụ mkpara eze.’",
            "poem": 2
          },
          "Abụ a bụ abụ akwa, nke e kwesiri ịbụ mgbe ịkwa akwa.”"
        ]
      }
    ],
