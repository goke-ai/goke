  "chapter": {
    "number": 2,
    "content": [
      {
        "type": "verse",
        "number": 1,
        "content": [
          {
            "text": "Lee ka Onyenwe anyị si jiri igwe ojii nke iwe ya",
            "poem": 1
          },
          {
            "text": "kpuchie ada Zayọn!",
            "poem": 2
          },
          {
            "text": "O sitela nʼeluigwe",
            "poem": 1
          },
          {
            "text": "tụda ịma mma Izrel nʼala,",
            "poem": 2
          },
          {
            "text": "o chetakwaghị ebe mgbakwasị ụkwụ ya,",
            "poem": 1
          },
          {
            "text": "nʼụbọchị iwe ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 2,
        "content": [
          {
            "text": "Onyenwe anyị elodala ebe obibi niile nke Jekọb;",
            "poem": 1
          },
          {
            "text": "na-enweghịkwa ọmịiko nʼebe ọ nọ.",
            "poem": 2
          },
          {
            "text": "O sitela nʼoke iwe ya",
            "poem": 1
          },
          {
            "text": "dọdasịa ebe niile e wusiri ike nke ada Juda.",
            "poem": 2
          },
          {
            "text": "Ọ wedala alaeze ya na ndị ụmụ eze ya nʼala,",
            "poem": 1
          },
          {
            "text": "mee ka ha gharakwa ị nwee nsọpụrụ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 3,
        "content": [
          {
            "text": "O sitela nʼoke iwe ya",
            "poem": 1
          },
          {
            "text": "bipụ ike niile Izrel nwere.",
            "poem": 2
          },
          {
            "text": "O seghachiri aka nri ya,",
            "poem": 1
          },
          {
            "text": "mgbe ndị iro ha na-abịa nso.",
            "poem": 2
          },
          {
            "text": "O nwukwara dịka ọkụ na-enwusi ike nʼetiti Jekọb;",
            "poem": 1
          },
          {
            "text": "si otu a rechapụ ihe niile dị gburugburu ya.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 4,
        "content": [
          {
            "text": "O kweela ụta ya, jide ya nʼaka nri ya,",
            "poem": 1
          },
          {
            "text": "dịka onye iro.",
            "poem": 2
          },
          {
            "text": "Dịka onye iro kwa",
            "poem": 1
          },
          {
            "text": "o gbuola ndị ahụ niile mara mma ile anya;",
            "poem": 2
          },
          {
            "text": "ọ wụpụla iwe ya, dịka ọ bụ ọkụ,",
            "poem": 1
          },
          {
            "text": "nʼelu ụlọ ada Zayọn.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 5,
        "content": [
          {
            "text": "Onyenwe anyị dị ka onye iro,",
            "poem": 1
          },
          {
            "text": "o lomichaala Izrel.",
            "poem": 2
          },
          {
            "text": "O lomichaala ụlọeze ya niile,",
            "poem": 1
          },
          {
            "text": "bibiekwa ebe ya niile e wusiri ike.",
            "poem": 2
          },
          {
            "text": "O meela ka iru ụjụ na ịkwa akwa",
            "poem": 1
          },
          {
            "text": "mụbaara ada Juda.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 6,
        "content": [
          {
            "text": "O meela ka ebe obibi ya tọgbọrọ nʼefu, dịka ubi a gbara ogige;",
            "poem": 1
          },
          {
            "text": "o bibiela ebe nzukọ ya niile.",
            "poem": 2
          },
          {
            "text": "Onyenwe anyị emeela ka Zayọn chezọọ",
            "poem": 1
          },
          {
            "text": "ime mmemme ya a kara aka, na idebe ụbọchị izuike ọbụla nsọ.",
            "poem": 2
          },
          {
            "text": "Site nʼoke iwe ya,",
            "poem": 1
          },
          {
            "text": "ọ jụla eze na onye nchụaja.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 7,
        "content": [
          {
            "text": "Onyenwe anyị ajụla ebe ịchụ aja ya",
            "poem": 1
          },
          {
            "text": "gbakụta ebe nsọ ya azụ.",
            "poem": 2
          },
          {
            "text": "O werela mgbidi niile nke ụlọeze ya",
            "poem": 1
          },
          {
            "text": "nyefee nʼaka onye iro;",
            "poem": 2
          },
          {
            "text": "ha emeela ka olu ha daa ụda nʼime ụlọ Onyenwe anyị",
            "poem": 1
          },
          {
            "text": "dịka nʼụbọchị mmemme a kara aka.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 8,
        "content": [
          {
            "text": "Onyenwe anyị ekpebiela ibibi",
            "poem": 1
          },
          {
            "text": "mgbidi gbara ada Zayọn gburugburu.",
            "poem": 2
          },
          {
            "text": "O setịpụrụ ihe ọtụtụ;",
            "poem": 1
          },
          {
            "text": "o sepụkwaghị aka ya na ibibi ya.",
            "poem": 2
          },
          {
            "text": "O mere ka mkputamkpu aja na mgbidi ruo ụjụ,",
            "poem": 1
          },
          {
            "text": "meekwa ka ha richapụsịa.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 9,
        "content": [
          {
            "text": "Ọnụ ụzọ ama ya niile emikpuola nʼala;",
            "poem": 1
          },
          {
            "text": "ọ gbajiela mkpọrọ niile e ji akpọchi ha, laakwa ha nʼiyi.",
            "poem": 2
          },
          {
            "text": "A dọtala eze ya na ụmụ eze ya nʼagha, buru ha jee mba ọzọ.",
            "poem": 1
          },
          {
            "text": "Iwu adịghịkwa,",
            "poem": 2
          },
          {
            "text": "otu a, ndị amụma ya enwekwaghị ike ịhụ ọhụ",
            "poem": 1
          },
          {
            "text": "site nʼebe Onyenwe anyị nọ.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 10,
        "content": [
          {
            "text": "Ndị okenye ada Zayọn",
            "poem": 1
          },
          {
            "text": "nọ ọdụ nʼala, dere duu.",
            "poem": 2
          },
          {
            "text": "Ha ekpokwasịla aja nʼisi ha;",
            "poem": 1
          },
          {
            "text": "ha eyikwasịkwala onwe ha uwe mkpe.",
            "poem": 2
          },
          {
            "text": "Ụmụ agbọghọ Jerusalem na-amaghị nwoke",
            "poem": 1
          },
          {
            "text": "ehulatala isi ha nʼala.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 11,
        "content": [
          {
            "text": "Ike agwụla anya m nʼihi ịkwa akwa,",
            "poem": 1
          },
          {
            "text": "ime ime ahụ m na-anụ ọkụ.",
            "poem": 2
          },
          {
            "text": "Obi m dị ka ihe a wụpụrụ nʼala,",
            "poem": 1
          },
          {
            "text": "nʼihi na e bibiela ndị m,",
            "poem": 2
          },
          {
            "text": "nʼihi na ụmụntakịrị na ụmụ a mụrụ ọhụrụ na-ada mba",
            "poem": 1
          },
          {
            "text": "nʼokporoụzọ nke obodo.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 12,
        "content": [
          {
            "text": "Ha na-ebere nne ha akwa na-asị,",
            "poem": 1
          },
          {
            "text": "“Olee ebe ọka na mmanya dị?”",
            "poem": 2
          },
          {
            "noteId": 1189
          },
          {
            "text": "Mgbe ha na-ada mba dịka ndị e merụrụ ahụ",
            "poem": 1
          },
          {
            "text": "nʼokporoụzọ obodo,",
            "poem": 2
          },
          {
            "text": "mgbe ọnwụ ji nwayọọ na-eripịasị ndụ ha",
            "poem": 1
          },
          {
            "text": "nʼaka nne ha ku ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 13,
        "content": [
          {
            "text": "Gịnị ka m nwere ike ikwu banyere gị?",
            "poem": 1
          },
          {
            "text": "Gịnị ka m ga-eji tụnyere gị,",
            "poem": 2
          },
          {
            "text": "gị ada Jerusalem?",
            "poem": 2
          },
          {
            "text": "Gịnị ka m ga-asị na i yiri,",
            "poem": 1
          },
          {
            "text": "nke ga-enyere m aka ịmata otu m ga-esi kasịe gị obi,",
            "poem": 2
          },
          {
            "text": "gị ada Zayọn na-amaghị nwoke?",
            "poem": 2
          },
          {
            "text": "Ọnya dị gị nʼahụ miri emi, dịka ịdị omimi nke osimiri.",
            "poem": 1
          },
          {
            "text": "Onye pụrụ ịgwọ gị?",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 14,
        "content": [
          {
            "text": "Ọhụ niile ndị amụma gị hụrụ",
            "poem": 1
          },
          {
            "text": "bụ ọhụ ụgha na ọhụ na-enweghị isi.",
            "poem": 2
          },
          {
            "text": "Ha ekpugheghị mmehie gị,",
            "poem": 1
          },
          {
            "text": "igbochi ndọta nʼagha nke a dọtara gị.",
            "poem": 2
          },
          {
            "text": "Kama ihe ha hụtaara gị",
            "poem": 1
          },
          {
            "text": "bụ ihe ụgha na nduhie.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 15,
        "content": [
          {
            "text": "Ndị niile si nʼebe ị nọ",
            "poem": 1
          },
          {
            "text": "na-agafe na-akụrụ gị aka ha;",
            "poem": 2
          },
          {
            "text": "ha na-efufe isi ha,",
            "poem": 1
          },
          {
            "text": "jirikwa ada Jerusalem na-eme ihe ọchị.",
            "poem": 2
          },
          {
            "text": "Ha na-ajụ ajụjụ na-asị, “Nke a ọ bụ obodo ahụ",
            "poem": 1
          },
          {
            "text": "a na-akpọ ebe mabigara mma oke,",
            "poem": 2
          },
          {
            "text": "ọṅụ nke ụwa niile?”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 16,
        "content": [
          {
            "text": "Ndị iro gị niile na-emeghepụ ọnụ ha",
            "poem": 1
          },
          {
            "text": "megide gị;",
            "poem": 2
          },
          {
            "text": "ha na-achị gị ọchị, na-atara gị ikekere eze.",
            "poem": 1
          },
          {
            "text": "Ha na-ekwu na-asị, “Anyị eloola ya.",
            "poem": 2
          },
          {
            "text": "Nke a bụ ụbọchị ahụ anyị na-ele anya ya.",
            "poem": 1
          },
          {
            "text": "Anyị adịrịla ndụ hụ ya.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 17,
        "content": [
          {
            "text": "Onyenwe anyị emeela ihe o zubere ime.",
            "poem": 1
          },
          {
            "text": "O mezuola ihe o kwuru na ọ ghaghị ime",
            "poem": 2
          },
          {
            "text": "site nʼoge gara aga.",
            "poem": 2
          },
          {
            "text": "Ọ kwatuola gị, na-enweghị ọmịiko ọbụla.",
            "poem": 1
          },
          {
            "text": "O meela ka ndị iro gị ṅụrịa ọṅụ nʼahụ gị;",
            "poem": 2
          },
          {
            "text": "o meekwala ka e bulie ike nke ndị iro gị.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 18,
        "content": [
          {
            "text": "Obi ndị mmadụ",
            "poem": 1
          },
          {
            "text": "na-etiku Onyenwe anyị.",
            "poem": 2
          },
          {
            "text": "Gị, mgbidi niile nke ada Zayọn,",
            "poem": 1
          },
          {
            "text": "ka anya mmiri gị na-asọpụta dịka osimiri",
            "poem": 2
          },
          {
            "text": "ehihie na abalị niile,",
            "poem": 2
          },
          {
            "text": "enyekwala onwe gị izuike,",
            "poem": 1
          },
          {
            "text": "enyekwala anya gị izuike.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 19,
        "content": [
          {
            "text": "Bilie, kwaa akwa nʼabalị,",
            "poem": 1
          },
          {
            "text": "nʼoge mmalite nche abalị.",
            "poem": 2
          },
          {
            "text": "Wụpụta ihe dị gị nʼobi nʼihu Onyenwe anyị",
            "poem": 1
          },
          {
            "text": "dịka ọ bụ mmiri.",
            "poem": 2
          },
          {
            "text": "Welie aka unu elu nye Onyenwe anyị",
            "poem": 1
          },
          {
            "text": "nʼihi ụmụntakịrị ndị a,",
            "poem": 2
          },
          {
            "text": "ndị na-ada nʼihi agụụ",
            "poem": 1
          },
          {
            "text": "nʼisi okporoụzọ niile.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 20,
        "content": [
          {
            "text": "“Lee anya, Onyenwe anyị, ma tụgharịa uche gị.",
            "poem": 1
          },
          {
            "text": "Onye ka i sirila otu a mesoo mmeso?",
            "poem": 2
          },
          {
            "text": "O kwesiri ka ndị bụ nne rie ụmụ ha,",
            "poem": 1
          },
          {
            "text": "bụ ụmụ nke ha mụrụ?",
            "poem": 2
          },
          {
            "text": "O kwesiri ka e gbuo onye nchụaja na onye amụma",
            "poem": 1
          },
          {
            "text": "nʼime ebe dị nsọ nke Onyenwe anyị?",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 21,
        "content": [
          {
            "text": "“Ụmụ okorobịa na ndị okenye dinakọrọ nʼotu ebe,",
            "poem": 1
          },
          {
            "text": "nʼuzuzu nke okporoụzọ.",
            "poem": 2
          },
          {
            "text": "E jirila mma agha gbuo",
            "poem": 1
          },
          {
            "text": "ụmụ okorobịa m na ụmụ agbọghọ m.",
            "poem": 2
          },
          {
            "text": "I gbuola ha nʼụbọchị iwe gị;",
            "poem": 1
          },
          {
            "text": "i gburu ha jụ inwe ọmịiko nʼahụ ha.",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "verse",
        "number": 22,
        "content": [
          {
            "text": "“Dịka i si akpọ oku ka a bịa nʼụbọchị oriri,",
            "poem": 1
          },
          {
            "text": "otu a ka i si kpọkọta ihe egwu niile ka ha bịakwasị m nʼakụkụ niile.",
            "poem": 2
          },
          {
            "text": "Nʼụbọchị nke iwe Onyenwe anyị",
            "poem": 1
          },
          {
            "text": "ọ dịghị onye gbapụrụ ọsọ, ọ dịkwaghị onye fọdụrụ ndụ;",
            "poem": 2
          },
          {
            "text": "ndị m lekọtara, zụlitekwa",
            "poem": 1
          },
          {
            "text": "ka ndị iro m lara nʼiyi.”",
            "poem": 2
          }
        ]
      },
      {
        "type": "line_break"
      },
      {
        "type": "line_break"
      }
